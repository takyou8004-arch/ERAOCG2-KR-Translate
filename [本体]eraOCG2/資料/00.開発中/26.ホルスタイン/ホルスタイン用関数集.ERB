			CASE 12
				鳴き声 = モぉんゥ…❤ %鳴き声%
			CASE 13
				鳴き声 = モォ…ゥ…❤ %鳴き声%
			CASE 14
				鳴き声 = もぉん…ぅ❤ %鳴き声%
			CASE 15
				鳴き声 = モぉぉ…ン❤ %鳴き声%
			CASE 16
				鳴き声 = モぉん…ぉっ❤ %鳴き声%
			CASE 17
				鳴き声 = モォぅ…もっ❤ %鳴き声%
			CASE 18
				鳴き声 = もォン…ぅ…❤ %鳴き声%
			CASE 19
				鳴き声 = モォ…ゥん…❤ %鳴き声%
		ENDSELECT
	ELSEIF 鳴きパターン == 3
		;繰り返し、うるさい鳴き(EX乳想定、激しい時等)
		SELECTCASE RAND:パターン範囲
			CASE 0
				鳴き声 = モォっもォっ❤ %鳴き声%
			CASE 1
				鳴き声 = モッっ❤モッ❤ %鳴き声%
			CASE 2
				鳴き声 = モッっモォっ❤ %鳴き声%
			CASE 3
				鳴き声 = モぉっモォゥっ❤ %鳴き声%
			CASE 4
				鳴き声 = モぉっもォぉっ❤ %鳴き声%
			CASE 5
				鳴き声 = モォっモゥ…っ❤ %鳴き声%
			CASE 6
				鳴き声 = モぉんっモぉんっ❤ %鳴き声%
			CASE 7
				鳴き声 = モォっモォゥ…っ❤ %鳴き声%
			CASE 8
				鳴き声 = モォゥっもォンっ❤ %鳴き声%
			CASE 9
				鳴き声 = モォっモォン…っ❤ %鳴き声%
			CASE 10
				鳴き声 = もぉっモォゥ…っ❤ %鳴き声%
			CASE 11
				鳴き声 = もぉぅっもぉぅっ❤ %鳴き声%
			CASE 12
				鳴き声 = モォン…っモぉぅっ❤ %鳴き声%
			CASE 13
				鳴き声 = モォオっ❤モォォっ❤ %鳴き声%
			CASE 14
				鳴き声 = もォンっ❤もォんっ❤ %鳴き声%
			CASE 15
				鳴き声 = モォゥっ❤モォォっ❤ %鳴き声%
			CASE 16
				鳴き声 = モォォ…っモォゥっ❤ %鳴き声%
			CASE 17
				鳴き声 = もォゥ…っもォゥ…っ❤ %鳴き声%
			CASE 18
				鳴き声 = モぉんっ❤モォゥ…っ❤ %鳴き声%
			CASE 19
				鳴き声 = モォゥ…っモォン…っ❤ %鳴き声%
		ENDSELECT
	ELSEIF 鳴きパターン == 4
		;濁点鳴き(美乳アレンジ)
		SELECTCASE RAND:パターン範囲
			CASE 0
				鳴き声 = モﾞッ❤ %鳴き声%
			CASE 1
				鳴き声 = モﾞーっ❤ %鳴き声%
			CASE 2
				鳴き声 = モﾞウッ❤ %鳴き声%
			CASE 3
				鳴き声 = ンﾞモー❤ %鳴き声%
			CASE 4
				鳴き声 = モﾞォッ❤ %鳴き声%
			CASE 5
				鳴き声 = モﾞウッ❤ %鳴き声%
			CASE 6
				鳴き声 = モﾞォーッ❤ %鳴き声%
			CASE 7
				鳴き声 = モﾞー…ン❤ %鳴き声%
			CASE 8
				鳴き声 = モﾞォンッ❤ %鳴き声%
			CASE 9
				鳴き声 = モﾞオーッ❤ %鳴き声%
			CASE 10
				鳴き声 = ンモﾞォ…❤ %鳴き声%
			CASE 11
				鳴き声 = モﾞォォォー❤ %鳴き声%
			CASE 12
				鳴き声 = モﾞオォォ…❤ %鳴き声%
			CASE 13
				鳴き声 = モﾞォー…ンッ❤ %鳴き声%
			CASE 14
				鳴き声 = ンﾞモォォンッ…❤ %鳴き声%
			CASE 15
				鳴き声 = モﾞォォォンッ…❤ %鳴き声%
			CASE 16
				鳴き声 = モﾞォォォォッー❤ %鳴き声%
			CASE 17
				鳴き声 = モﾞォォンモォッ…❤ %鳴き声%
			CASE 18
				鳴き声 = モﾞォォオォーッ❤ %鳴き声%
			CASE 19
				鳴き声 = モﾞォォォォォォッー❤ %鳴き声%
		ENDSELECT
	ENDIF
REND

;GETCOLORで現在値を취득して、설정後に戻す
文字色保持 = GETCOLOR()

SETCOLOR C_PINK
;通常(カッコつき)
PRINTFORML 「%鳴き声%」

;文字色を元に戻す
SETCOLOR 文字色保持

;――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
;HORUSTEIN_GET_DATA
;仕様：
;소환された몬스터의 IDとカード位置から、いろいろとデータを취득する。
;ホルスタインは自身の카드의 효과로 몬스터의 IDを보존するためこのような処理に。
;――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
@HORUSTEIN_GET_DATA2(듀얼리스트 = "자신",場所 = -1,選択カードID)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カードID
#DIMS DYNAMIC カテゴリ名称
#DIMS DYNAMIC 乳名
#DIMS DYNAMIC 암소名称
#DIMS DYNAMIC 암소略称
#DIM DYNAMIC 암소대상
#DIM DYNAMIC 암소所有
#DIM DYNAMIC 所有者
#DIM DYNAMIC 所有者_比較
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 参照場所;마법함정に置かれたホルスタインの参照先
#DIM DYNAMIC 参照カードID;마법함정に置かれたホルスタインの参照先
#DIM DYNAMIC 参照カードID
#DIM DYNAMIC T1ミルクml
#DIM DYNAMIC ミルクml

;필드에 存在する마법함정をチェックする(ホルスタイン用)

CALL FULLRESET
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 듀얼리스트 == "자신"
	所有者 = 1
	암소所有 = 2
ELSEIF 듀얼리스트 == "상대"
	所有者 = 2
	암소所有 = 1
ENDIF

;見つからなければ、そのまま返す
参照カードID = 選択カードID
参照場所 = -1
カテゴリ名称 = 없음
SELECTCASE 選択カードID
	CASE 179556
		カテゴリ名称 = 登録情報：ホルスタイン-美乳
		乳名 = 美乳
	CASE 179557
		カテゴリ名称 = 登録情報：ホルスタイン-愛乳
		乳名 = 愛乳
	CASE 179558
		カテゴリ名称 = 登録情報：ホルスタイン-旨乳
		乳名 = 旨乳
	CASE 179559
		カテゴリ名称 = 登録情報：ホルスタイン-EX乳
		乳名 = EX乳
ENDSELECT

;所有が자신경우、자신의 마법함정존의 카드나 ら、カテゴリの存在ﾁｪｯｸ
;＞ここで牛に変えたカードの元IDを취득している
IF カテゴリ名称 != "なし"
	IF 場所 == -1
		所有者_比較 = 所有者
	ELSE
		所有者_比較 = GETVAR(@"%듀얼리스트%_MZ_所有:場所")
	ENDIF
	
	IF 所有者_比較 == 所有者
		FOR カウンタ,0,20
			SIF GETVARS(@"%듀얼리스트%_永紋章:カウンタ") != 選択カードID - 100000
				CONTINUE
			;データを취득する
			参照カードID = GETVAR(@"%듀얼리스트%_永紋_汎数１:カウンタ")
			参照場所 = カウンタ
			암소所有 = 所有者
			BREAK
		NEXT
	ELSE
		;所有が상대의 경우、상대의 마법함정を参照
		FOR カウンタ,0,20
			SIF GETVARS(@"%対面者%_永紋章:カウンタ") != 選択カードID - 100000
				CONTINUE
			;データを취득する
			参照カードID = GETVAR(@"%対面者%_永紋_汎数１:カウンタ")
			参照場所 = カウンタ
			BREAK
		NEXT
	ENDIF
ENDIF

CALL CARD_EROTICS(参照カードID)
암소대상 = RESULT
;得た参照カードIDをここで名前/略称に変換して、冠を付けている
;エッチな事できる대상なら、そっちから名称を취득する
IF 암소대상 != -1
	암소名称 = %CALLNAME:암소대상%
ELSE
	TRYCALLFORM CARDNAME_{参照カードID},"略称"
	암소名称 = %RESULTS%
ENDIF
SUBSTRING 암소名称, 0, 8
암소略称 = 【%乳名%】%RESULTS%

;搾乳量の취득
IF 参照場所 != -1
	ミルクml = GETVAR(@"%듀얼리스트%_永紋_汎数２:参照場所")
	T1ミルクml = GETVAR(@"%듀얼리스트%_永紋_汎数３:参照場所")
ENDIF

RESULTS:0 = %암소名称%
RESULTS:1 = %암소略称%

RESULT:0 = 参照場所			;参照カードID(마함)の「場所」
RESULT:1 = 암소대상			;えっち用のID：csv無しなら-1
RESULT:2 = 암소所有			;
RESULT:3 = T1ミルクml		;
RESULT:4 = ミルクml			;
RESULT:99 = 参照カードID	;마함に置かれた「元のカードID」

RETURN 参照場所



;――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
;HORUSTEIN_GET_BUST
;仕様：
;参照カードID（ホルスタインカード）に登録されたカードの가슴사이즈の설정취득
;설정が行われるわけではない。
;――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
@HORUSTEIN_GET_BUST(参照カードID,乳추가,乳上書き)
#DIM DYNAMIC 参照カードID
#DIM DYNAMIC 乳추가
#DIM DYNAMIC 乳上書き ;(1以上で乳추가の値で書き換え)

#DIM DYNAMIC 암소대상
#DIM DYNAMIC 乳攻撃力
#DIM DYNAMIC 乳守備力
#DIM DYNAMIC 乳속성
#DIM DYNAMIC 元乳サイズ
#DIM DYNAMIC 元乳mm
#DIM DYNAMIC 現乳サイズ
#DIM DYNAMIC 現乳mm

;가슴を計測する(参考：EVENTTURNEND.ERB　OPTION_BUSTUP)
;虚가슴  550.0mm -3
;絶壁  600.0mm -2
;貧乳  650.0mm -1
;並み  700.0mm 0
;巨乳  800.0mm +1
;爆乳  950.0mm +2
;牛乳 1200.0mm +3
;　　 1300.0mm(母乳체질)
;魔乳 1500.0mm +4
;ＥＸ 2000.0mm +5
;(추가)以降、250.0mmずつ加算

;캐릭터クターcsvの存在チェック
CALL CARD_EROTICS(参照カードID)
암소대상 = RESULT

TRYCALLFORM CARD_{参照カードID},"攻撃力"
乳攻撃力 = RESULT
TRYCALLFORM CARD_{参照カードID},"守備力"
乳守備力 = RESULT
TRYCALLFORM CARD_{参照カードID},"속성"
乳속성 = RESULT * 250

;元の乳サイズを취득
IF 암소대상 == -1
	;캐릭터情報が無ければ、元々の攻守の最小値・最大値で初期サイズ설정。
	;1000で貧、2000で並...
	
	IF MAX(乳攻撃力,乳守備力) <= 1000
		元乳mm = 650 + MIN(乳攻撃力,乳守備力) % 49
		元乳サイズ = -1
	ELSEIF MAX(乳攻撃力,乳守備力) <= 2000
		元乳mm = 700 + MIN(乳攻撃力,乳守備力) % 99
		元乳サイズ = 0
	ELSEIF MAX(乳攻撃力,乳守備力) <= 3000
		元乳mm = 800 + MIN(乳攻撃力,乳守備力) % 149
		元乳サイズ = 1
	ELSE
		元乳mm = 950 + MIN(乳攻撃力,乳守備力) % 249
		元乳サイズ = 2
	ENDIF
ELSE
	元乳サイズ = TALENT:암소대상:가슴사이즈
	
	;(参考：PRINT_STATE)
	IF オプション_가슴AWS == 1
		IF TALENT:암소대상:체형 == -3
			元乳mm = (CFLAG:암소대상:가슴初期値 + CFLAG:암소대상:가슴成長) / 100
		ELSEIF TALENT:암소대상:체형 == -2
			元乳mm = (CFLAG:암소대상:가슴初期値 + CFLAG:암소대상:가슴成長 - 500) / 10
		ELSE 
			元乳mm = (CFLAG:암소대상:가슴初期値 + CFLAG:암소대상:가슴成長) / 10
		ENDIF
	ELSE
		IF TALENT:암소대상:체형 == -3
			元乳mm = (CFLAG:암소대상:가슴初期値) / 100
		ELSEIF TALENT:암소대상:체형 == -2
			元乳mm = (CFLAG:암소대상:가슴初期値 - 500) / 10
		ELSE
			元乳mm = (CFLAG:암소대상:가슴初期値) / 10
		ENDIF
	ENDIF
ENDIF

IF 乳上書き == 0 
	現乳サイズ = 元乳サイズ + 乳추가
ELSE
	現乳サイズ = 乳추가
ENDIF
IF 元乳サイズ != 現乳サイズ
	
	;増量分の가슴から、サイズを算出
	SELECTCASE 現乳サイズ
		CASE IS < -3
			;(虚乳以下なんてならんと思うけど、ドリップする際の母乳調整用に必要なので用意)
			現乳mm = 550 + (乳攻撃力+乳守備力+乳속성) % 49 + ( 現乳サイズ + 2 ) * 50 
		CASE -3
			現乳mm = 550 + (乳攻撃力+乳守備力+乳속성) % 49
		CASE -2
			現乳mm = 600 + (乳攻撃力+乳守備力+乳속성) % 49
		CASE -1
			現乳mm = 650 + (乳攻撃力+乳守備力+乳속성) % 49
		CASE 0 ;並み
			現乳mm = 700 + (乳攻撃力+乳守備力+乳속성) % 99
		CASE 1 ;巨乳
			現乳mm = 800 + (乳攻撃力+乳守備力+乳속성) % 149
		CASE 2 ;爆乳
			現乳mm = 950 + (乳攻撃力+乳守備力+乳속성) % 249
		CASE 3 ;牛乳
			現乳mm = 1200 + (乳攻撃力+乳守備力+乳속성) % 299
		CASE 4 ;魔乳
			現乳mm = 1500 + (乳攻撃力+乳守備力+乳속성) % 499
		CASE 5 ;規格外
			現乳mm = 2000 + (乳攻撃力+乳守備力+乳속성) % 499
	CASEELSE
		;規格外サイズ+250くらい씩 늘린다攻守の余りから端数を出して、すこし数値をずらす。
		現乳mm = 2000 + (乳攻撃力+乳守備力+乳속성) % 499 + (現乳サイズ- 5 ) * 240 + (乳攻撃力+乳守備力+現乳サイズ) % 9
	ENDSELECT

	IF 現乳サイズ >= 5 && オプション_가슴AWS == 1 && 암소대상 != -1
		;自力で規格外まで成長してると元の数値より減っちゃうことがあるので、加算する(デカくなりすぎる事は考慮しない)
		現乳mm = 現乳mm + CFLAG:암소대상:가슴成長 / 10
	ENDIF

ELSE
	現乳mm = 元乳mm
ENDIF

RESULT:0 = 元乳サイズ
RESULT:1 = 元乳mm
RESULT:2 = 現乳サイズ
RESULT:3 = 現乳mm

RETURN RESULT


