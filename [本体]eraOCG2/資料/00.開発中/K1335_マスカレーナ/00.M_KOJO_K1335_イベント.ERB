;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系이벤트
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K1335_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;空行
	PRINTFORML
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:TARGET:恋慕
			PRINTFORMW 「あっ、%CALLNAME:MASTER%！」
		ELSE
			PRINTFORMW 「どうしたの？何かあった？」
		ENDIF
		RETURN 1
	ENDIF
	[SKIPSTART]
	;-------------------------------------------------
	;TARGETが入ってきた
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW 
		RETURN 1
	ENDIF
	[SKIPEND]
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;未確認
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 
		RETURN 1
	ENDIF
	[SKIPSTART]
	;-------------------------------------------------
	;外出先で遭遇した
	;eraOCG2_V0.40_BT_240522時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他캐릭터とデート中に遭遇した
	;eraOCG2_V0.40_BT_240522時点では流れないため注意
	;ARG:1にデート상대
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;未確認だが、おそらくeraOCG2_V0.40_BT_240522時点では流れないため注意
	;注、このあと通常の遭遇口上、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で一日最初の挨拶
	;未確認だが、おそらくeraOCG2_V0.40_BT_240522時点では流れないため注意
	;ARG:1にデート상대、ARG:1が0の場合は非デート
	;注、このあと通常の外出先で遭遇した口上、ARG==6 OR ARG==7に接続します
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;ARG:1とデート中
		IF ARG:1
		;一人で外出
		ELSE
		ENDIF
		PRINTFORMW  
		RETURN 1
	ENDIF
	[SKIPEND]
ENDIF
RETURN 1

;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K1335_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;空行
	PRINTFORML
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ상대が先に起きてる
	;eraOCG2_V0.40_BT_240522時点では流れる状況を作れない
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時添い寝中就寝時のみ？
		;eraOCG2_V0.40_BT_240522時点では、TARGETの私室には入れない
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;eraOCG2_V0.40_BT_240522時点では、MASTERの睡眠＝1日の終了のため、実質流れない
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ상대が起きている
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「起きちゃった？%CALLNAME:MASTER%もセキュリティが甘いね♡」
			CASE 1
				PRINTFORMW 「おはよう！%CALLNAME:MASTER%！今日もハッキング日和だね」
			CASE 2
				PRINTFORML 「ふわぁ……おはよ…、%CALLNAME:MASTER%……」
				PRINTFORMW %CALLNAME:TARGET%も起きたばかりなのか、眠たげに挨拶を交わした
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ상대が寝ている
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		SELECTCASE RAND:2
			CASE 1
				PRINTFORML 「すぅ……」
				PRINTFORMW %CALLNAME:TARGET%は素直な寝息をたてながら、だらしない寝顔を晒している
			CASE 2
				PRINTFORML 「ぴぃ……%CALLNAME:MASTER%……」
				PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の寝言を呟いているようだ
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPSTART]
	;-------------------------------------------------
	;MASTERが起きていて、상대が起きてきた
	;添い寝時TARGET起床はこちら（TARGET衰弱or添い寝中TARGET睡魔負け後そのままMASTERが起き続けた上で상대が起床した場合限定
	;eraOCG2_V0.40_BT_240522時点では流れることを確認できなかったため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;eraOCG2_V0.40_BT_240522時点では、非同棲時に寝るとTARGETの自室に帰ってしまう。注意
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 「ふわぁ……おはよー、%CALLNAME:MASTER%……」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 「ふわぁ……おはよー、%CALLNAME:MASTER%……」
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	[SKIPEND]
	;その他
	PRINTFORMW   
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ口上）
;==================================================
@M_KOJO_EVENT_K1335_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;自室に帰って寝る（～は【現在地】から【캐릭터の自室】に戻りました）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:TARGET:恋慕
			PRINTFORMW 「ふわぁ……ワタシ、先に帰ってるね……」
		ELSE
			PRINTFORMW 「そろそろ帰るよ。また今度ね！」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている（～は眠そうにしている）
	;eraOCG2_V0.40_BT_240522時点では流れることを確認できなかったため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「んん……ハッ！ね、眠ってなんか！」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;睡眠中（～は眠っている）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「すぅ……」
		PRINTFORMW %CALLNAME:TARGET%はすやすやと眠っている
		RETURN 1
	ENDIF

	;-------------------------------------------------
	;衰弱して寝る（体力500以下,うふふ時はうふふ強制終了）
	;（캐릭터の自室以外：～を【現在地】から【캐릭터の自室】に運びました）
	;（캐릭터の自室　　：～は疲れたと言って、横になりました）
	;この後ARG == 1 が流れる
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF CFLAG:うふふ
			;うふふ中にダウン
			PRINTFORML 「ぅ゛……❤あぁ゛……❤」
			PRINTFORMW %CALLNAME:TARGET%はビクビクと身体を震わせながら、快楽の中で意識を手放してしまったようだ
		ELSE
			PRINTFORMW 「うう……もうダメ、休ませてぇ～……」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（恋慕のみ,そのまま一日が終了）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF CFLAG:うふふ
			;うふふ中にダウン
			PRINTFORMW 「ん……❤もう疲れちゃった？それじゃあ、続きのハッキングはまた今度ね❤」
		ELSE
			PRINTFORMW 「わっ！どうしたの！？%CALLNAME:MASTER%！無理はしちゃダメ！」
		ENDIF
		RETURN 1
	ENDIF
	[SKIPSTART]
	;-------------------------------------------------
	;TARGETが寝るために自室まで戻ってきた
	;a.m.0:00頃に突然、睡眠中のTARGETが寝室に出現する
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;-------------------------------------------------
		;そのまま着替えて寝た（【TARGET】が部屋に戻ってきた→寝間着へと着替え、寝床に潜った）
		;未確認、eraOCG2_V0.40_BT_240522時点ではこの状況にならない？
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 0
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;部屋から追い出して寝た（眠そうな【TARGET】に追い出された…）
		;未確認、eraOCG2_V0.40_BT_240522時点ではこの状況にならない？
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			IF CFLAG:前ターン位置 == CFLAG:現在位置
				;前のターンまで一緒に部屋にいた
