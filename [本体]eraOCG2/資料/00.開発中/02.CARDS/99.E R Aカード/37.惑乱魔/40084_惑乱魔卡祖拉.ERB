@CARDNAME_40084,参照先
;ココで지정卡片の名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 惑乱魔卡祖拉
	CASE "略称"
		RESULTS = 卡祖拉(惑)
	CASE "カテゴリ"
		RESULTS:0 = 惑乱魔
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_40084(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 식물족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 400
	CASE "守備力"
		RETURN 2400
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_40084(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL このカード名の①②の효과は1ターンに1度、いずれか1つしか使用できない。
CALLFORM CARD_ABLE(1,ターン開始時)
PRINTL ①：ターン開始時,자신の마법・함정「蜜罐」1枚を대상として발동する。
PRINTL 作为对象的卡从游戏中"소멸",给予对方2000伤害,自己回复2000基本分,このカードを묘지へ送る
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②：상대がモンスターの소환・특수소환に成功した時に발동出来る。
PRINTL このカードを묘지から특수소환する,そのモンスターは「蜜罐」としてこのカードに装備されている。
SETCOLOR C_PINK
CALLFORM CARD_ABLE(3,攻撃時誘発)
PRINTL ③：攻撃표시のこのカードが攻撃대상に選択された場合に발동する。
PRINTL 攻撃モンスターの攻撃力は800ポイントダウンする,このカードを守備표시にする。
RESETCOLOR
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ④：このカードは함정내성을 持つ。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_40084(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 效果先

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,40084)
SIF RESULT == 1
	RETURN 0

IF 종류 == ターン開始時
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_1(듀얼리스트,40084)
		RETURN 0
	SIF 발동済_2(듀얼리스트,40084)
		RETURN 0

    CALL FULLRESET
    효과範囲마함 = 1
    효과範囲상대마함 = 1
    효과条件カテゴリ = 蜜罐
    CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
    SIF RESULT != 1
    	RETURN 0
    RETURN 1

ELSEIF 종류 == 효과後誘発
	SIF ゾーン != "묘지"
		RETURN 0
	SIF 발동済_1(듀얼리스트,40084)
		RETURN 0
	SIF 발동済_2(듀얼리스트,40084)
		RETURN 0

	效果先 = FINAL_SUMMON_TARGET(듀얼리스트,1)
	SIF GETVAR(@"%対面者%_MZ_性:效果先") != 牝性 && GETVAR(@"%対面者%_MZ_性:效果先") != ふたなり && GETVAR(@"%対面者%_MZ_性:效果先") != 雄性
		RETURN 0
    RETURN 1

ELSEIF 종류 == 攻撃時誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_表:場所") == 2
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_表:場所") == 2
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == リアクション효과
	SIF ゾーン != "필드"
		RETURN 0

    CALL FULLRESET
	CALL CARD_EQUIP_CHECK(듀얼리스트,場所,ゾーン)
    SIF RESULT != 1
    	RETURN 0
    RETURN 1
ENDIF


@CARDEFFECT_40084(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 选择卡片
#DIM DYNAMIC 自己卡片
#DIM DYNAMIC 自己守備力修正
#DIM DYNAMIC 对方卡片
#DIM DYNAMIC 对方攻撃力修正
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 效果先

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == ターン開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,40084,1)
	IF RESULT == 0
		CALL FULLRESET
    	효과条件カテゴリ = 蜜罐
        CALL CARD_CHOOSE_BASIC(듀얼리스트,"마함",ゾーン,場所)
        选择卡片 = RESULT
        IF 选择卡片 != 9999
			TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_마함:选择卡片")}("名前")
			조교대상 = GETVAR(@"%듀얼리스트%_마함:选择卡片")
            CALL FULLRESET
            CALL CARD_DISPOSE(듀얼리스트,"마함",选择卡片,소멸行き)
            CALL LIFE_CHANGE(듀얼리스트,"減少","효과",2000,1)
            CALL LIFE_CHANGE(듀얼리스트,"増加","효과",2000)

            CALL CARD_EROTICS(조교대상)
			IF RESULT != -1
				조교대상 = RESULT
				SETCOLOR C_PINK
				PRINTFORML
				PRINTFORMW %CALLNAME:조교대상%丧失了最后逃脱的机会,被卡祖拉封装在叶笼中拖往地下♥永久珍藏♥
				PRINTFORML
				RESETCOLOR
				CALL EROTIC_MOVE("묘상捕縛",조교대상, -1, 1000)
			ENDIF
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
        ENDIF
    ENDIF

ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,40084,2)
	IF RESULT == 0
    	CALL FULLRESET
		效果先 = FINAL_SUMMON_TARGET(듀얼리스트,1)
        IF 效果先 != 5
        	TRYCALLFORM CARD_{GETVAR(@"%対面者%_필드:效果先")},"성별"
			IF RESULT == 牝性
				조교대상 = GETVAR(@"%対面者%_필드:效果先")
                CALL FULLRESET
            	CALL CARD_SP_SUMMON(듀얼리스트, "묘지", 場所)
				효과추가カテ = 蜜罐
            	CALL CARD_EQUIP_MONSTER(듀얼리스트,カード소환先,"상대필드",效果先)

            	CALL CARD_EROTICS(조교대상)
				IF RESULT != -1
					조교대상 = RESULT
					SETCOLOR C_PINK
					PRINTFORMW %CALLNAME:조교대상%还没来得及站稳,就被地面窜出的藤蔓拉入了陷阱之中。
					RESETCOLOR
				ENDIF
        	ELSE
            	CALL FULLRESET
        		CALL CARD_SP_SUMMON(듀얼리스트, "묘지", 場所)
				효과추가カテ = 蜜罐
            	CALL CARD_EQUIP_MONSTER(듀얼리스트,カード소환先,"상대필드",效果先)
        	ENDIF
        ENDIF
    ENDIF

ELSEIF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,40084)
	IF RESULT == 0
		自己卡片 = 戦闘場所(듀얼리스트)
		自己守備力修正 = GETVAR(@"%듀얼리스트%_MZ_守:自己卡片")
		对方卡片 = 戦闘場所(듀얼리스트,1)
		对方攻撃力修正 = GETVAR(@"%対面者%_MZ_攻:对方卡片")
		TRYCALLFORM CARD_{戦闘カードID(듀얼리스트,1)}("성별")
		IF RESULT == 牝性
			조교대상 = GETVAR(@"%対面者%_필드:对方卡片")
			CALL CARD_EROTICS(조교대상)
			IF RESULT != -1
				조교대상 = RESULT
				SETCOLOR C_PINK
				PRINTFORMW %CALLNAME:조교대상%被藤蔓拖进陷阱深处固定,衣服被快速溶解,全裸着浸泡在不妙的液体中。
				RESETCOLOR
				CALL EROTIC_MOVE("媚毒汚染",조교대상,NO_TO_CHARANUM(1373))
			ENDIF
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",对方卡片,"永攻DOWN",800)

		ELSEIF RESULT == 雄性 || RESULT == ふたなり && (对方攻撃力修正 - 800 ) - 自己守備力修正 >= 0
			조교대상 = GETVAR(@"%対面者%_필드:对方卡片")
			CALL CARD_EROTICS(조교대상)
			IF RESULT != -1
				조교대상 = RESULT
				SETCOLOR C_PINK
				PRINTFORMW 卡祖拉沉溺于交欢的快感，被%CALLNAME:조교대상%的肉棒彻底정복了……！
				RESETCOLOR
				CALL EROTIC_MOVE("前穴凌辱",NO_TO_CHARANUM(1373), -1)
				IF 마함数(対面者) < 5
					SETCOLOR C_PINK
					PRINTFORMW 随后就那样被%CALLNAME:조교대상%当作肉铠带走了……
					RESETCOLOR
					CALL CARD_EQUIP_MONSTER(対面者,对方卡片,"상대필드",自己卡片,(1 + 2))
				ENDIF
			ELSE
				CALLFORM CARDNAME_{GETVAR(@"%対面者%_필드:对方卡片")}("名前")
				SETCOLOR C_PINK
				PRINTFORMW 卡祖拉沉溺于交欢的快感，被%RESULTS%的肉棒彻底정복了……！
				RESETCOLOR
				CALL EROTIC_MOVE("前穴凌辱",NO_TO_CHARANUM(1373), -1)
				IF 마함数(対面者) < 5
					SETCOLOR C_PINK
					PRINTFORMW 随后就那样被%RESULTS%当作肉铠带走了……
					RESETCOLOR
					CALL CARD_EQUIP_MONSTER(対面者,对方卡片,"상대필드",自己卡片,(1 + 2))
				ENDIF
			ENDIF

		ELSEIF RESULT == 雄性 || RESULT == ふたなり
			조교대상 = GETVAR(@"%対面者%_필드:对方卡片")
			CALL CARD_EROTICS(조교대상)
			IF RESULT != -1
				조교대상 = RESULT
				SETCOLOR C_PINK
				PRINTFORMW 卡祖拉榨取着%CALLNAME:조교대상%的肉棒，射出的精液与消化液混合在一起，成为了卡祖拉的养料。
				RESETCOLOR
				CALL EROTIC_MOVE("媚毒汚染",조교대상,NO_TO_CHARANUM(1373),100)
			ENDIF
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",对方卡片,"永攻DOWN",800)
			
		ELSE
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",对方卡片,"永攻DOWN",800)
		ENDIF
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"表",2,1)
	ENDIF

ELSEIF 종류 == 攻撃時強制誘発
	CALL FULLRESET
	IF RESULT == 0
		IF 戦闘場所(듀얼리스트,1) != 5
			CALL CARD_EROTICS(戦闘カードID(듀얼리스트,1))
			IF RESULT != 0
				조교대상 = RESULT
			ENDIF
		ENDIF
		IF 조교대상 > 0
			PRINTFORML
			SETCOLOR C_PINK 
			PRINTFORML %CALLNAME:조교대상%一脚踩空落入了充满粘腻液体的陷阱中。
			RESETCOLOR
		ENDIF		
	ENDIF		

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"함정耐")

ELSEIF 종류 == リアクション효과
	FOR カウンタ,0,5
		SIF GETVAR(@"%듀얼리스트%_마함:カウンタ") == -1
			CONTINUE
		SIF GETVAR(@"%듀얼리스트%_魔_装:カウンタ") != 場所
			CONTINUE
		SIF GETVAR(@"%듀얼리스트%_魔_リ数:カウンタ") >= 10
			CONTINUE
			
		CALL CARD_EROTICS(GETVAR(@"%듀얼리스트%_마함:カウンタ"))
		IF RESULT != -1
			조교대상 = RESULT
			SETCOLOR C_PINK
			PRINTFORML
			PRINTFORML %CALLNAME:조교대상%想要逃出卡祖拉的陷阱,却只能无力的撞在猪笼草的盖子上。
            PRINTFORML 被卡祖拉操控的藤蔓好似玩乐一般放松束缚,又在%CALLNAME:조교대상%得以喘息之际将其迅速拉回。
			PRINTFORML
			RESETCOLOR
			CALL EROTIC_MOVE("媚毒汚染",조교대상,NO_TO_CHARANUM(1373),50)
			CALL SET_FIELD_INFO(듀얼리스트,"마함",カウンタ,"リ数",1,0,1)
		ENDIF
	NEXT
ENDIF
