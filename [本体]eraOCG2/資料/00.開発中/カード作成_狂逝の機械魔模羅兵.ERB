@CARDNAME_39910,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 狂逝の機械模魔羅兵（ィークギアディルドポーン）
	CASE "略称"
		RESULTS = 機械模魔羅兵
	CASE "カテゴリ"
		RESULTS:0 = 狂逝の機械
ENDSELECT


@CARD_39910(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
		RETURN 効果モン
	CASE "属性"
		RETURN 地属性
	CASE "種族"
		RETURN 機械族
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 500
	CASE "性別"
		RETURN 牝性
ENDSELECT
RETURN 0

@CARD_EXPLANATION_39910(種類)
#DIM DYNAMIC 種類

CALL TEXT_DECORATION("ROGUE")
PRINTL このカード名の①②の効果はそれぞれ１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,召喚時,特殊召喚時)
PRINTL ①：このカードが召喚・特殊召喚した場合に発動できる。
PRINTL デッキから「狂逝の機械」モンスター카드 １枚を手札に加える。
CALLFORM CARD_ABLE(10,攻撃時誘発)
PRINTL ②：このカードが牝性モンスターを攻撃する場合に発動できる。
PRINTL その몬스터에 このモンスターの攻撃力分の快感ダメージを与え発情させる。
PRINTL その後、フィールドの「狂逝の機械」카드 １枚を選び、そのカードに薬物カウンターを１つ置く。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_39910(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 調教対象

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,39910)
SIF RESULT == 1
	RETURN 0

IF 種類 == 召喚時 || 種類 == 特殊召喚時
	SIF 発動済_1(決闘者,39910)
		RETURN 0
	CALL FULLRESET
	効果範囲デッキ = 1
	効果条件カテゴリ = 狂逝の機械
	効果条件種類 = モン全般
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 種類 == 攻撃時誘発
	SIF ゾーン != "戦場"
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	;戦闘場所(決闘者)　：これから戦闘を行う自分のモンスターの位置
	;戦闘場所(決闘者,1)：これから戦闘を行う相手のモンスターの位置、
	;この関数（プログラム）は「,1」を付けることで相手と自分を分ける。
	SIF 戦闘場所(決闘者) != 場所
		RETURN 0
	IF 戦闘場所(決闘者,1) != 5
		TRYCALLFORM CARD_{戦闘カードID(決闘者,1)}("性別")
		SIF RESULT != 牝性
			RETURN 0
	ENDIF
	RETURN 1
ENDIF

@CARDEFFECT_39910(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 調教対象
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 戦闘カードID

CALL 対面者判定(決闘者)
対面者 = %RESULTS%


IF 種類 == 召喚時 || 種類 == 特殊召喚時
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,39910,1)
	IF RESULT == 0
		CALL FULLRESET
		効果範囲デッキ = 1
		効果条件カテゴリ = 狂逝の機械
		効果条件種類 = モン全般
		CALL CARD_ADD_ADV(決闘者,ゾーン,場所)
	ENDIF
ELSEIF 種類 == 攻撃時誘発
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,39910)
	IF RESULT == 0
		;ここで何をしているか：
		;戦闘場所(決闘者,1)＝これから戦闘行為を行う相手のカードの位置を指し、
		;相手몬스터에 攻撃した場合は0～4が、ダイレクトなら5が返される。
		IF 戦闘場所(決闘者,1) != 5
			CALL CARD_EROTICS_FIELD(決闘者,"戦場",戦闘場所(決闘者,1),1)
			IF RESULT != -1
				調教対象 = RESULT
			ENDIF
		ELSE
			CALL CARD_EROTICS(相手プレイヤー,決闘者)
			IF RESULT != -1
				調教対象 = RESULT
			ENDIF
		ENDIF

		IF MARK:調教対象:快楽刻印 + MARK:調教対象:苦痛刻印 + MARK:調教対象:屈服刻印 + MARK:調教対象:反発刻印 <= 3
			PRINTFORML 返り討ちにあった機械模魔羅兵として組み込まれた雌は、
			PRINTFORML 動かなくなり壊れた義肢と共に倒れまるで潰されたカエルのように無様に地面に転げ倒れる。
			PRINTFORML それと同時に生やされたふたなりちんぽの根元にある輪が締め付け始める。
			PRINTFORML 敗北した罰を与えるかのようにぎりぎりと締め付けられ組み込まれている雌は必死に
			PRINTFORML ゆるひれ”♡ゆるひでくだひゃ”♡ちんぽ♡ちんぽしめづけないでください”♡と言い懇願する。
			PRINTFORML その異様な光景を眺める%CALLNAME:調教対象%は無事に１장倒せたと一息つく……が、
			PRINTFORML その体が足先、腹、胸、首、頭の順に電流が遡ったかのように急に跳ね上がりながら、
			PRINTFORML その両穴の膣肉同士をすり合わせるようにうごめかせ、乳首とクリトリスは勃起したかのように固まる。
			PRINTFORML 急な事に%CALLNAME:調教対象%は膝を揺らしながら立つことに必死になりながら息を荒げる。
			PRINTFORML すると息をすればするほどその症状はより深く激しくなることに気づいた%CALLNAME:調教対象%。
			PRINTFORML それが機械魔羅兵の機械の四肢に組み込まれたタンクが割れ漏れ出した薬物が気化して起きたことだと
			PRINTFORML 気づけないまま、少しでも落ち着こうと深呼吸を繰り返す。
			PRINTFORML ぺたりと座り込んだ地面に、愛液と潮が混じった水溜まりをつくりながら。
			CALL EROTIC_MOVE("媚毒汚染",調教対象,50)
		;ELSEIFとは？IF条件というのは、複数設定できるが、IFと付けるのは最初の一つだけで、
		;そのほかは"ELSEIF"/ 또는 "ELSE"を使う。それぞれELSEIF「その他の条件」かELSE「その他」となる。
;		ELSEIF MARK:調教対象:快楽刻印 + MARK:調教対象:苦痛刻印 + MARK:調教対象:屈服刻印 + MARK:調教対象:反発刻印 >= 3
		;↑の条件設定でも問題無いが、今回の場合は……↓の「ELSE」…つまり最初に設定したIFの条件以外、でOK
		ELSE
			PRINTFORML 返り討ちにあった機械模魔羅兵として組み込まれた雌は
			PRINTFORML 動かなくなり壊れた義肢と共に倒れまるで潰されたカエルのように無様に地面に転げ倒れる。
			PRINTFORML それと同時に生やされたふたなりちんぽの根元にある輪が締め付け始める。
			PRINTFORML 敗北した罰を与えるかのようにぎりぎりと締め付けられ組み込まれている雌は必死に
			PRINTFORML ゆるひれ”♡ゆるひでくだひゃ”♡ちんぽ♡ちんぽしめづけないでください”♡と言い懇願する。
			PRINTFORML その異様な光景を眺める%CALLNAME:調教対象%は、もし捕らえられてしまったら、私もこうなるのだろうかという考えがよぎる。
			PRINTFORML その無様に、必死に喘ぎご主人様の名を呼びながら一片の慈悲を求めて媚びる姿に%CALLNAME:調教対象%は自分を重ねてしまう。
			PRINTFORML 私はあなた様の牝穴です、四肢を失い兵の一つとして扱われてもあなた様への服従と忠誠は失っておりません♡
			PRINTFORML おちんぽ様がなければ生きていけないメスに、生涯自身に生やしたおちんぽ様をも管理されそれに尽くす権利を捧げていただきありがとうございます♡
			PRINTFORML どうか私の全てを使い尽くしてください♡おちんぽ様が満足するまでこの機械模魔羅兵をお使いください♡
			PRINTFORML 苦痛に折れ、快楽に溶け、屈服に堕ちた%CALLNAME:調教対象%は眼前にあるふたなりちんぽを見ながら、
			PRINTFORML いやしい妄想を頭に流しながらその牝穴をぐしゃぐしゃに使い快楽を貪り始める。
			PRINTFORML 他の兵に見つかったら、誰かが来てしまったら、捕らえられてしまったら
			PRINTFORML そのような暴力と苦痛に満ちた妄想を餌に、%CALLNAME:調教対象%は喘ぎ散らし、身にまとう物を愛液で穢し、イキ狂い続けた
			CALL EROTIC_MOVE("自慰経験",調教対象,300)
			CALL EROTIC_MOVE("媚毒汚染",調教対象,300)
		ENDIF
		CALL STATCHANGE_EFFECT(対面者,"戦場",戦闘場所(決闘者,1),"発情",1)

		;その後、フィールドの「狂逝の機械」카드 １枚を選び、そのカードに薬物カウンターを１つ置く。
		CALL FULLRESET
		効果範囲戦場 = 1
		効果条件カテゴリ = 狂逝の機械
		CALL CARD_GRANT_ADV(決闘者,ゾーン,場所,"薬Ｃ",1)

;		CALL SET_FIELD_INFO(対面者,"戦場",選択カード,"薬Ｃ",1)
	ENDIF
ENDIF




