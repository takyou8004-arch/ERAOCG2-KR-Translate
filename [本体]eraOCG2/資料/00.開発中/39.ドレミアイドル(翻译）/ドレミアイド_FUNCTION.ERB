#DIM DYNAMIC 实行대상
#DIM DYNAMIC 发情标识
#DIM DYNAMIC 调教强度
#DIM DYNAMIC 호감도补正
#DIM DYNAMIC 侍奉选项

IF TALENT:调教对象:연모 >= 2
	호감도补正 = 4
	调教强度 = 500
ELSEIF TALENT:调教对象:연모 >= 1
	호감도补正 = 3
	调教强度 = 300
ELSEIF CFLAG:调教对象:호감도 > 1000
	호감도补正 = 2
	调教强度 = 200
ELSE
	호감도补正 = 1
	调教强度 = 100
ENDIF
SIF 发情标识 != 0
	调教强度 *= 2

调教强度 = 调教强度 * (80+RAND:40) / 100

