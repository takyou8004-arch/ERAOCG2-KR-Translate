@CARDNAME_91635,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

;エクトプラズマー
SELECTCASE 参照先
	CASE "名前"
		RESULTS = 요람의 스타베이더
	CASE "略称"
		RESULTS = 요람의 스타베이더
	CASE "カテゴリ"
		RESULTS:0 = 그렌디오스
		RESULTS:1 = 스타베이더
		RESULTS:2 = 링크 조커
		RESULTS:4 = 뱅가드
ENDSELECT


@CARD_91635(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_91635(종류)
#DIM DYNAMIC 종류
#DIMS DYNAMIC 듀얼리스트

PRINTL 이 카드명의 ①③의 효과는 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(1,마함起動,フリー誘発)
PRINTL ①：이 카드의 발동시의 효과 처리로서、패에 「스타베이더 “Я” 크레이들」토큰을 １장 생성한다。
CALL CARD_ABLE(0,지속효과)
PRINTL ②：자신 필드 위에 "Я"몬스터가 존재하는 한、
PRINTL 자신 필드 위의 "그렌디오스"몬스터 전부는 대상내성을 得る。
CALL CARD_ABLE(2,마함起動,フリー誘発)
PRINTL ③：자신 / 상대 턴에、자신필드에 "그렌디오스"몬스터가 존재하는 경우에 발동할 수 있다。
PRINTL 패의 "Я" 몬스터 １장을 특수 소환한다。

@CARDCAN_91635(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

#DIM DYNAMIC 選択カードID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,91635)
SIF RESULT == 1
	RETURN 0

IF (종류 == 마함起動 || 종류 == フリー誘発) && ゾーン == "마함" && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_2(듀얼리스트,91635)
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 그렌디오스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = Я
	효과条件종류 = モン全般
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	; SIF 손패数(듀얼리스트) < 2
	; 	RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,91635)
		RETURN 0
	IF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = Я
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 그렌디오스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_91635(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード数

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF (종류 == 마함起動 || 종류 == フリー誘発) && ゾーン == "마함" && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91635,2)
	IF RESULT == 0
		; CALL FULLRESET
		; 효과範囲손패 = 1
		; 효과条件カテゴリ = Я
		; 효과条件CNT = 1
		; CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		; 選択カード数 = RESULT
		; CALL FULLRESET
		; 효과範囲손패 = 1
		; SIF 選択カード数 == 1
		; 	효과条件固カテゴリ = Я
		; CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		; IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件カテゴリ = Я
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		; ENDIF
	ENDIF
ELSEIF (종류 == 마함起動 || 종류 == フリー誘発)
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91635,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
		CALL CARD_GENERATE_HAND(듀얼리스트,191635)
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과条件カテゴリ = 그렌디오스
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐")
ENDIF


