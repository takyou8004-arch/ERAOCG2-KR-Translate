		好感度修正 += 2
	SIF TALENT:TARGET:연모 >= 1
		好感度修正 += 2
	;ファーストキス
	IF EXP:TARGET:キス경험 == 1 && EXP:TARGET:펠라경험 == 0
		PRINTFORMW 
	ELSE
		SELECTCASE RAND:好感度修正 + 1
			CASE 1
				PRINTFORMW 
			CASE 2
				PRINTFORMW 
			CASE 3
				PRINTFORMW 
			CASE 4
				PRINTFORMW 
			CASE 5
				PRINTFORMW 
			;연모
			CASE 6
				PRINTFORMW 
			CASE 7
				PRINTFORMW 
			;親愛
			CASE 8
				PRINTFORMW 
			CASE 9
				PRINTFORMW 
			;애처
			CASE 10
				PRINTFORMW 
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
