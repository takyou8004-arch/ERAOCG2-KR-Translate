	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて一緒に入る
	;eraOCG2_V0.40_BT_240518時点では流れないため주의
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて出て行った
	;eraOCG2_V0.40_BT_240518時点では流れないため주의
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW 
		RETURN 1
	ENDIF
	[SKIPEND]
	;-------------------------------------------------
	;TARGETが목욕に入ってきて追い出された
	;未確認
	;記入チェック（=0, 非표시、1, 표시）
