;==================================================
;상대から데이트のお誘い
;==================================================
@M_KOJO_EVENT_K2172_27(ARG,ARG:1)
;구상主はLOCAL
;ARG:1は目的땅
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;誘い文句
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
PRINTFORMW 「마스터 같이 나가자」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;受けた
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
PRINTFORMW 「신난다 야옹♪」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;断った
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
PRINTFORMW 「에이~ 너무 짜!」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;部屋に誘われた
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;部屋に誘われたのを断った
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

