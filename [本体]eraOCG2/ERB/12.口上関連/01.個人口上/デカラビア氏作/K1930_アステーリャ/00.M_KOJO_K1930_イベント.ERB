;"EVENT"系이벤트
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K1930_1(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;空行
	PRINTFORML
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW アステーリャは本を読んでいるようだ
		RETURN 1
	ENDIF
	[SKIPSTART]
	;-------------------------------------------------
	;TARGETが入ってきた
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「あっお兄さん…いたんですか」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて一緒に入る
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「ごっごめんなさい…　えっ一緒にですか？わっわかりました」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて出て行った
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW 「ごっごめんなさい！」
		RETURN 1
	ENDIF
	[SKIPEND]
	;-------------------------------------------------
	;TARGETが목욕に入ってきて追い出された
	;未確認
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 
		RETURN 1
	ENDIF
	[SKIPSTART]
	;-------------------------------------------------
	;외출先で遭遇した
	;eraOCG2_V0.40_BT_240522時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他캐릭터と데이트중に遭遇した
	;eraOCG2_V0.40_BT_240522時点では流れないため注意
	;ARG:1に데이트상대
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;未確認だが、おそらくeraOCG2_V0.40_BT_240522時点では流れないため注意
	;注、このあと通常の遭遇구상、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출先で一日最初の挨拶
	;未確認だが、おそらくeraOCG2_V0.40_BT_240522時点では流れないため注意
	;ARG:1に데이트상대、ARG:1が0の場合は非데이트
	;注、このあと通常の외출先で遭遇した구상、ARG==6 OR ARG==7に接続します
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;ARG:1と데이트중
		IF ARG:1
		;一人で외출
		ELSE
		ENDIF
		PRINTFORMW  
		RETURN 1
	ENDIF
	[SKIPEND]
ENDIF
RETURN 1

;==================================================
;朝起きたとき（おはよう구상）
;==================================================
@M_KOJO_EVENT_K1930_2(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 0
;-------------
IF LOCAL
	;空行
	PRINTFORML
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;非同棲かつ상대가 先に起きてる
	;eraOCG2_V0.40_BT_240522時点では流れる상황を作れない
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（연모時添い寝中취침時のみ？
		;eraOCG2_V0.40_BT_240522時点では、TARGETの私室には入れない
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(연모無しでも入ってくる可能性有り
		;eraOCG2_V0.40_BT_240522時点では、MASTER의수면＝1日の終了のため、実質流れない
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ상대가 起きている
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ상대가 寝ている
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	[SKIPSTART]
	;-------------------------------------------------
	;MASTERが起きていて、상대가 起きてきた
	;添い寝時TARGET起床はこちら（TARGET쇠약or添い寝中TARGET睡魔負け後그대로 MASTERが起き続けた上で상대가 起床した場合限定
	;eraOCG2_V0.40_BT_240522時点では流れることを確認できなかったため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;eraOCG2_V0.40_BT_240522時点では、非同棲時に寝るとTARGETの自室に帰ってしまう。注意
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	[SKIPEND]
	;その他
	PRINTFORMW   
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ구상）
;==================================================
@M_KOJO_EVENT_K1930_3(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 0
;-------------

IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;自室に帰って寝る（～は【現在땅】から【캐릭터の自室】に戻りました）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF CFLAG:우후후
			;우후후ダウン後に何も言わせない場合はここに何も書かない
			PRINTFORML
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている（～は眠そうにしている）
	;eraOCG2_V0.40_BT_240522時点では流れることを確認できなかったため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;수면中（～は眠っている）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF

	;-------------------------------------------------
	;쇠약して寝る（체력500以下,우후후時は우후후強制終了）
	;（캐릭터の自室以外：～を【現在땅】から【캐릭터の自室】に運びました）
	;（캐릭터の自室　　：～は疲れたと言って、横になりました）
	;この後ARG == 1 が流れる
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF CFLAG:우후후
			;우후후中にダウン
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（연모のみ,그대로 一日が終了）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF CFLAG:우후후
			;우후후中にダウン
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	[SKIPSTART]
	;-------------------------------------------------
	;TARGETが寝るために自室まで戻ってきた
	;a.m.0:00頃に突然、수면中のTARGETが寝室に出現する
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;-------------------------------------------------
		;그대로 着替えて寝た（【TARGET】が部屋に戻ってきた→寝間着へと着替え、寝床に潜った）
		;未確認、eraOCG2_V0.40_BT_240522時点ではこの상황にならない？
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 0
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;部屋から追い出して寝た（眠そうな【TARGET】に追い出された…）
		;未確認、eraOCG2_V0.40_BT_240522時点ではこの상황にならない？
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
