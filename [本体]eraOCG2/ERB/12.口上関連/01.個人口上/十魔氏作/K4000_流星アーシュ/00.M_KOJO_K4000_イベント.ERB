;			PRINTFORMW 「ブラックマジシャン걸を소환！」
;		ENDIF

;==================================================
;0,デュエルスタート！
;==================================================
@M_KOJO_DUEL_K4000_0(カードID,ARG:1)
#DIM DYNAMIC 호감도修正
#DIM DYNAMIC カードID
#DIMS DYNAMIC カード名
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	SELECTCASE RAND:3 + 1
		CASE 1
			PRINTFORMW 
		CASE 2
			PRINTFORMW 
		CASE 3
			PRINTFORMW 
	ENDSELECT
ENDIF

