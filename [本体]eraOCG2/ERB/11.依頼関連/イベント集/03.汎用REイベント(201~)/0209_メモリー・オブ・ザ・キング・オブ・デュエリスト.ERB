@IRAI_EVENT_209(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 메모이 오브 더 킹 오브 듀얼리스트(몇번이든 수주 가능)
	CASE "이벤트種"
		RETURN 이벤트_施設
	CASE "依頼先"
		RETURN 場所_カドショ
	CASE "詳細"
		PRINTL 해보자, 전설의 듀얼 킹의 덱과 대결!
		PRINTL 
	CASE "条件"
		SIF 依頼_全完了횟수() < 3
			RETURN 0
		RETURN 1
	CASE "利用制限"
		SIF 依頼플래그:209 != 0
			RETURN 3
		RETURN 0
ENDSELECT

RETURN 0

@IRAI_ACT_209(参照)
#DIM DYNAMIC 参照
#DIM DYNAMIC 대상상대
#DIM DYNAMIC パック_종류
#DIM DYNAMIC カウンタ


IF 依頼_進行보존:209 == 0
	PRINTFORML 
	PRINTFORMW "해보자, 전설의 듀얼 킹의 덱과 대결!"
	PRINTFORMW 카드샵 벽에 어떤 캠페인 포스터가 붙어 있어서, 점원에게 물어봤다.
	PRINTFORMW "손님! 지금 그 전설의 듀얼 킹의 덱을 재현한 덱과 대결하는 캠페인을 개최 중입니다!"
	PRINTFORMW "승리하시면, 전설의 듀얼 킹을 테마로 한 카드가 들어있는 팩을 받을 수 있습니다!"
	PRINTFORMW "도전, 어떠십니까?"
	PRINTFORML 
	PRINTFORML 
	PRINTFORML [0] 할게요
	PRINTFORML [1] 안 할게요
	BINPUT
	IF RESULT == 0
		PRINTFORML "알겠습니다, 상대는 저쪽의 듀얼 머신입니다!"
	ELSE
		PRINTFORML "알겠습니다, 그럼 다시 말씀해주세요!"
		RETURN 0
	ENDIF

ELSE
	PRINTFORML 
	PRINTFORMW "손님! 오늘도 캠페인에 도전하시겠습니까?"
	PRINTFORML 
	PRINTFORML 
	PRINTFORML [0] 할게요
	PRINTFORML [1] 안 할게요
	BINPUT
	IF RESULT == 0
		PRINTFORML "알겠습니다, 상대는 저쪽의 듀얼 머신입니다!"
	ELSE
		PRINTFORML "알겠습니다, 그럼 다시 말씀해주세요!"
		RETURN 0
	ENDIF
	PRINTFORML 
	PRINTFORMW 오늘은 어떤 덱과 듀얼할까?
	PRINTFORML
	PRINTFORML [0] 기사 가이아 덱
	SIF 依頼_進行보존:209 >= 1
		PRINTFORML [1] 카오스 솔저 덱
	SIF 依頼_進行보존:209 >= 2
		PRINTFORML [2] 유익환수 키마이라 덱
	SIF 依頼_進行보존:209 >= 3
		PRINTFORML [3] 마그넷 워리어 덱
	SIF 依頼_進行보존:209 >= 4
		PRINTFORML [4] 버스터 블레이더 덱
	SIF 依頼_進行보존:209 >= 5
		PRINTFORML [5] 블랙 매지션 덱
	SIF 依頼_進行보존:209 == 6
		PRINTFORML [6] ??? 덱
	SIF 依頼_進行보존:209 >= 7
		PRINTFORML [6] 삼환신 덱
	PRINTFORML
	PRINTFORML [100] 돌아간다
	BINPUT
	依頼_完了횟수:209 = RESULT
ENDIF
IF 依頼_完了횟수:209 == 100
	依頼_完了횟수:209 = 0
	PRINTFORML 하지만 아직 준비가 안 되어 있어서, 잠시 기다려 달라고 했습니다.
	RETURN 0
ENDIF

TARGET = -1
SELECTCASE 依頼_完了횟수:209
	CASE 0, 1, 2
		CALL DUEL_SYSTEM(1,209,0,2)
	CASE 3, 4, 5, 6
		CALL DUEL_SYSTEM(1,209,0,1)
	CASEELSE
		RETURN 0
ENDSELECT
IF RESULT == 1
	PRINTFORML
	PRINTFORMW 이겼다!
	PRINTFORMW "축하드립니다! 그럼 여기, 특별 팩입니다!"
	PRINTFORML
	CALL CARD_PACK_SHORI(5,49)
	PRINTFORML
	IF 依頼_進行보존:209 == 0
		PRINTFORMW "다음번부터는 다른 덱과의 대결도 해금되오니, 꼭 다시 찾아주세요!"
		PRINTFORMW 어? 다른 덱?
		PRINTFORMW "네. 그 전설의 듀얼 킹의 덱을 하나의 덱으로 재현하기는 어려워서, 나눠서 재현되었다고 합니다."
		PRINTFORMW 그랬구나.
		PRINTFORMW "계속 이겨나가시면, 언젠가는 그 신의 카드가 들어있는 덱과도 대결할 수 있을지도 모르니, 열심히 해주세요!"
	ENDIF
	SIF 依頼_進行보존:209 <= 6 && 依頼_進行보존:209 == 依頼_完了횟수:209
		依頼_進行보존:209 += 1
	IF 依頼_進行보존:209 >= 6 && パック_解放:49 == 0
		PRINTFORMW "그리고 여기까지 이겨내신 보너스로, 특별 팩을 일반 판매합니다!"
		PRINTFORML 
		CALL CARD_PACK_RELEASE(49)
	ENDIF
	PRINTFORMW 그렇게 해서 다음 순서인 사람에게 듀얼 머신의 상대를 양보하고 그 자리를 떠났다.
	PRINTFORML 
	PRINTFORML 
ENDIF
依頼_完了횟수:209 = 0
TARGET = LOCAL
RETURN 2


