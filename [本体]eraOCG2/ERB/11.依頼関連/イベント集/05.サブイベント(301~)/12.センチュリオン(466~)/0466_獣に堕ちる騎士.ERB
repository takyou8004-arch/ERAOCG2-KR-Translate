@IRAI_EVENT_466(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS

;こちらで462番、8462番などの番号を使用してますので、
;もし462番の依頼を作成する場合には、デュエル이벤트를挟まないでください

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CP-CTR)짐승으로 떨어진 기사 도마뱀 편
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 어느 날, 응모한 기억도 없는 추천 당첨품으로 카드가 도착해서…
		PRINTL 
		PRINTL 
		PRINTL 보수: ?
	CASE "条件"
		;천년도어の解放が必要
		SIF ITEM:천년도어 == 0
			RETURN 0
		;裏ショップ解放が条件
		SIF パック_解放:99 == 0
			RETURN 0
		;악랄치66(悪人)以上も条件
		SIF FLAG:악랄치 < 66
			RETURN 0
		RETURN 1
ENDSELECT

RETURN 0

