@CARDCAN_299770(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,299770)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과範囲EX덱 = 1
	효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_299770(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 実行役대상
#DIM DYNAMIC レベル
#DIMS DYNAMIC 無効대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%



IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,299770,1)
	IF RESULT == 0
		CALL CARD_EROTICS(상대プレイヤー,듀얼리스트)
		조교대상 = RESULT

		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲필드 = 1
		효과範囲EX덱 = 1
		효과条件성별 = 牝性
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		実行役대상 = RESULT:1
		TRYCALLFORM CARD_{実行役대상},"レベル"
		レベル = RESULT
		CALL CARD_EROTICS(実行役대상)
		実行役대상 = RESULT

		IF 조교대상 > -1 && 実行役대상 > -1
			PRINTFORML
			PRINTFORML
			SETCOLOR C_PINK
			PRINTFORML %NAME:조교대상%는 %NAME:実行役대상%の穴に精を吸い取られた！
			RESETCOLOR
			PRINTFORML
			CALL EROTIC_MOVE("受펠라",조교대상,実行役대상,レベル*100)
			; SELECTCASE RAND:2
			; 	CASE 0
			; 		CALL EROTIC_MOVE("前穴凌辱",実行役대상,조교대상,レベル*100)
			; 	CASE 1
			; 		CALL EROTIC_MOVE("受펠라",조교대상,実行役대상,レベル*100)
			; ENDSELECT		
		ENDIF
		CALL LIFE_CHANGE(듀얼리스트,"減少","효과",レベル*500,1)

		CALL FULLRESET
		효과範囲상대필드 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"묘지")

	ENDIF
ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,299770,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件성별 = 牝性 
		효과추가종류 = 効追無効
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			SETVAR @"%듀얼리스트%_MZ_接可:カード소환先", 2
			조교대상 = RESULT:1
			TRYCALLFORM CARDNAME_{조교대상},"名前"
			無効대상 = %RESULTS%
			PRINTFORML
			SETCOLOR C_PINK
PRINTFORMW %무효 대상%이 육혈로 재배치되었다.
			RESETCOLOR
			PRINTFORML
			PRINTFORML

		ENDIF
	ENDIF
		
ELSEIF 종류 == 마함起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,299770)
	IF RESULT == 0
	ENDIF
ENDIF
