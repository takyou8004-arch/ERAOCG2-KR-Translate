@IRAI_EVENT_513(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (VV2)Stage2 Invasion B.E.S.
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 바이퍼에게서 긴급 통신이 왔다.
		PRINTL 무슨 일이라도 생긴 걸까?
		PRINTL
	CASE "条件"
		;前回이벤트クリア
		SIF 依頼_完了횟수:512 == 0
			RETURN 0
		RETURN 1
ENDSELECT

RETURN 0

