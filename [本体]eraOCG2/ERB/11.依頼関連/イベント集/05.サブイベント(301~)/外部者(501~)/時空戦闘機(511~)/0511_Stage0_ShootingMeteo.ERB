@IRAI_EVENT_511(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (VV0)Stage0 ShootingMeteor
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 場所_森
	CASE "詳細"
		PRINTL 왠지 모르게 몹시 별이 보고 싶어졌다.
		PRINTL 밤이 되면 숲으로 가자.
		PRINTL
	CASE "条件"
		;特に思いつかないので無し
		RETURN 1
ENDSELECT

RETURN 0

