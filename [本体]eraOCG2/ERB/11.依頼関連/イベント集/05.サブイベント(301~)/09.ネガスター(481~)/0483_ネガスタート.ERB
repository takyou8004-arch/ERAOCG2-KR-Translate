@IRAI_EVENT_483(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CP-NGA3)-반전시동- 아누스피어
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 눈동자에 변조된 표식을 새긴 피리카를 가스타의 마을에 데려다주고 나서
		PRINTL 일주일이 지나려 하고 있었다…
		PRINTL 슬슬 상황을 보러 가야 할까?
		PRINTL 
		PRINTL 보수: "-반전시동-아누스피어" 등
	CASE "条件"
		SIF 依頼_完了횟수:482 == 0
			RETURN 0
		SIF 依頼_完了済 != 0
			RETURN 0
		RETURN 1
ENDSELECT

RETURN 0

