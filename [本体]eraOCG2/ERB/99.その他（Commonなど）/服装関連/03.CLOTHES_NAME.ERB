@今日のぱんつ(ARG)
#FUNCTION
LOCAL = 1

IF TALENT:ARG:허접클리
	RETURNF 0
ENDIF

IF TALENT:ARG:소악마 || TALENT:ARG:음란
	LOCAL:2 = -1
ELSEIF TALENT:ARG:유치 || TALENT:ARG:유아／유아퇴행
	LOCAL:2 = 2
ELSEIF TALENT:ARG:체형 < 0
	LOCAL:2 = 1
ELSE
	LOCAL:2 = 0
ENDIF


WHILE LOCAL
	LOCAL:1 = RAND:100
	SELECTCASE LOCAL:1
		;ノーパン
		CASE 0
			SIF TALENT:ARG:음란
				LOCAL = 0
		;かぼちゃ팬티
		CASE 2
			SIF LOCAL:2 >= 2
				LOCAL = 0
		;くまさん팬티
		CASE 3
			SIF LOCAL:2 >= 2
				LOCAL = 0
		;물玉ぱんつ
		CASE 4
			SIF LOCAL:2 > 0
				LOCAL = 0
		;しましまぱんつ
		CASE 5
			LOCAL = 0
		;フリルぱんつ
		CASE 6
			SIF LOCAL:2 > 0
				LOCAL = 0
		;ローレグ팬티
		CASE 10
			LOCAL = 0
		;ローライズ팬티
		CASE 11
			SIF RAND:2 == 0
				CONTINUE
			SIF LOCAL:2 < 0
				LOCAL = 0
		;ハイレグ팬티
		CASE 12
			SIF RAND:2 == 0
				CONTINUE
			SIF LOCAL:2 < 0
				LOCAL = 0
		;レースのパンティ(白)
		CASE 13
			SIF LOCAL:2 <= 0
				LOCAL = 0
		;レースのパンティ(黒)
		CASE 14
			SIF LOCAL:2 <= 0
				LOCAL = 0
		;Tバック
		CASE 15
			SIF TALENT:ARG:음란
				LOCAL = 0
		;紐パン
		CASE 16
			SIF TALENT:ARG:음란
				LOCAL = 0
		;レザー팬티
		CASE 17
			SIF TALENT:ARG:음란 && ABL:ARG:새드끼 > 4
				LOCAL = 0
		;ペチショーツ
		CASE 20
			LOCAL = 0
		;フレアショーツ
		CASE 21
			SIF LOCAL:2 <= 0
				LOCAL = 0
		;スキャンティ
		CASE 22
			SIF RAND:2 == 0
				CONTINUE
			SIF LOCAL:2 < 0
				LOCAL = 0
		;クロッチオープン
		CASE 23
			SIF TALENT:ARG:음란
				LOCAL = 0
	ENDSELECT
WEND
RETURNF LOCAL:1
