SIF TEQUIP:15
	RETURN 0
;普通の관장사용중は불가
SIF TEQUIP:21
	RETURN 0
;バルーン사용중は불가
SIF TEQUIP:22
	RETURN 0
;電極사용중は불가
SIF TEQUIP:23
	RETURN 0
RETURN 1