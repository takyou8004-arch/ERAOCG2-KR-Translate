;-------------------------------------------------
;거품춤
;봉사계커맨드、レベル3
;-------------------------------------------------
@COM84
;-------------------------------------------------
;COM別FLAGの설정
;-------------------------------------------------
;PLAYER
TCVAR:MASTER:사정대상캐릭터_1 = TARGET
TCVAR:MASTER:사정부위플래그 = 8
TCVAR:MASTER:사정쾌락강도 = 2000

;TARGET
TCVAR:TARGET:사정대상캐릭터_1 = MASTER
TCVAR:TARGET:사정부위플래그 = 8
TCVAR:TARGET:사정쾌락강도 = 2000

;봉사쾌락
TFLAG:봉사쾌락플래그 = 1
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;消費체력、기력
DOWNBASE:체력 += 50
DOWNBASE:기력 += 180

CALL SOURCE_CALC_쾌Ｃ(TARGET, MASTER, 200)
CALL SOURCE_CALC_쾌Ｂ(TARGET, MASTER, 200)
CALL SOURCE_CALC_노출(TARGET, MASTER, 80)
CALL SOURCE_CALC_접촉(TARGET, MASTER, 500)
CALL SOURCE_CALC_봉사(TARGET, MASTER, 450)
CALL SOURCE_CALC_불결(TARGET, MASTER, 300)
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の汚れ
STAIN:0 = 0
STAIN:1 = 0
STAIN:2 = 2
STAIN:3 = 1
STAIN:4 = 8
STAIN:5 = 0
STAIN:6 = 0

;조교者の汚れ
	STAIN:MASTER:0 = 0
	STAIN:MASTER:1 = 0
	STAIN:MASTER:2 = 2
	STAIN:MASTER:3 = 1
	STAIN:MASTER:4 = 8
	STAIN:MASTER:5 = 0
	STAIN:MASTER:6 = 0

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;同性愛　레즈なら戻り値1게이なら戻り値2
IF HETEROSEX(TARGET,MASTER)
	EXP:(50 + HETEROSEX(TARGET,MASTER)) += 7
	EXP:MASTER:(50 + HETEROSEX(TARGET,MASTER)) += 7
ENDIF

;애정경험
SIF TALENT:연모
	EXP:애정경험 += 1
RETURN 1

@CAN_COM84(ARG)
#DIM DYNAMIC 素質確認
#DIM DYNAMIC 판정値
#DIM DYNAMIC 汚れ値
#DIM DYNAMIC 実行値
SKIPDISP ARG
PRINTL 거품 춤
;-------------------------------------------------
;実行판정処理
;-------------------------------------------------


;すべての命令に共通の要素を考慮
;(순종が高いと命令に従いやすいなど)
CALL COM_ORDER
素質確認 = RESULT:1
판정値 += RESULT
;ABL:욕망
IF ABL:욕망
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:욕망 * 1
	PRINT 욕망
	PRINTV 'LV,ABL:욕망,'(,ABL:욕망 * 1,')
	素質確認 = 1
ENDIF
;ABL:봉사정신
IF ABL:봉사정신
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:봉사정신 * 4
	PRINT 봉사정신
	PRINTV 'LV,ABL:봉사정신,'(,ABL:봉사정신 * 4,')
	素質確認 = 1
ENDIF
;ABL:정액중독
IF ABL:정액중독
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:정액중독 * 3
	PRINT 정액중독
	PRINTV 'LV,ABL:정액중독,'(,ABL:정액중독 * 3,')
	素質確認 = 1
ENDIF

IF MARK:쾌락각인
	SIF 素質確認
		PRINT  + 
	판정値 += MARK:쾌락각인 * 1
	PRINT 쾌락각인
	PRINTV 'LV,MARK:쾌락각인,'(,MARK:쾌락각인 * 1,')
	素質確認 = 1
ENDIF
;PALAM:욕정
IF GETPALAMLV(PALAM:욕정,5)
	SIF 素質確認
		PRINT  + 
	판정値 += GETPALAMLV(PALAM:욕정,5) * 1
	PRINT 욕정
	PRINTV 'LV,GETPALAMLV(PALAM:욕정,5),'(,GETPALAMLV(PALAM:욕정,5) * 1,')
	素質確認 = 1
ENDIF
;부끄럼쟁이
IF TALENT:수치심 > 0
	PRINT  - 
	판정値 -= 3
	PRINT 부끄러움
	PRINTV '(,3,')
	素質確認 = 1
ENDIF
;악취둔감
IF TALENT:악취내성 > 0
	SIF 素質確認
		PRINT  + 
	판정値 += 1
	PRINT 악취둔감
	PRINTV '(,1,')
	素質確認 = 1
ENDIF
;악취민감
IF TALENT:악취내성 < 0
	PRINT  - 
	판정値 -= 3
	PRINT 악취민감
	PRINTV '(,3,')
	素質確認 = 1
ENDIF
;헌신적
IF TALENT:헌신적
	SIF 素質確認
		PRINT  + 
	판정値 += 6
 	PRINT 헌신적
 	PRINTV '(,6,')
	素質確認 = 1
ENDIF
;쾌감의 부정（음란、쾌락광で無効）
IF TALENT:쾌감반응 < 0 && !TALENT:음란
	PRINT  - 
	판정値 -= 3
	PRINT 쾌감의 부정
	PRINTV '(,3,')
	素質確認 = 1
ENDIF
;男嫌い（연모、친애で無効）
IF TALENT:성별기호 & 1 && !(TALENT:MASTER:성별 & 1) && (!TALENT:연모 || MASTER != MASTER)
	PRINT  - 
	판정値 -= 12
	PRINT 남성혐오
	PRINTV '(,12,')
	素質確認 = 1
ENDIF
;연모
IF TALENT:연모
	SIF 素質確認
		PRINT  + 
	판정値 += 5
	PRINT 연모
	PRINTV '(,5,')
	素質確認 = 1
ENDIF
;친애
IF TALENT:연모 > 1
	SIF 素質確認
		PRINT  + 
	판정値 += 5
	PRINT 친애
	PRINTV '(,5,')
	素質確認 = 1
ENDIF

;조교者が후타나리
IF (TALENT:MASTER:성별 & 3) == 3
	SIF 素質確認
		PRINT  + 
	판정値 += 8
	PRINT 후타나리
	PRINTV '(,8,')
	素質確認 = 1
ENDIF


;愛液の汚れ
SIF STAIN:MASTER:2 & 1
	汚れ値 += 1
;정액の汚れ
SIF STAIN:MASTER:2 & 4
	汚れ値 += 3
;애널の汚れ
SIF STAIN:MASTER:2 & 8
	汚れ値 += 7
SIF STAIN:MASTER:2 & 16
	汚れ値 += 15

汚れ値 = 汚れ値 * (2 - TALENT:악취내성) / 2

;더러움있음
IF 汚れ値
 	PRINT  - 
	판정値 -= 汚れ値
	PRINT 더러움 있음
	;악취둔감
	IF TALENT:악취내성 > 0
		PRINT 、악취둔감
	ELSEIF TALENT:악취내성 < 0
		PRINT 、악취민감
	ENDIF
	PRINTV '(,汚れ値,')
	素質確認 = 1
ENDIF

;合計を표시(30以上で実行)
PRINT  = 
PRINTV 판정値

実行値 = 30
SIF 판정値 < 実行値
	PRINT  < 
SIF 판정値 == 実行値
	PRINT  = 
SIF 판정値 > 実行値
	PRINT  > 
PRINT 실행치
PRINTV 実行値

WAIT
SKIPDISP 0
;実行できない
SIF 판정値 < 実行値
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行판정
;-------------------------------------------------
