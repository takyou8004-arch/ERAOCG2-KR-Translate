@MAT_EVENT_CHECK(듀얼리스트,カード名,使用素材名前)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC カード名
#DIMS REF 使用素材名前
#DIM DYNAMIC カウンタ

; PRINTFORML 소환カード名:%カード名%
; PRINTFORML オプション_特定素材이벤트:{オプション_特定素材이벤트}

; FOR カウンタ,0,10
;     PRINTFORMW 사용소재이름:{カウンタ}=%使用素材名前:カウンタ%
; NEXT

SIF オプション_特定素材이벤트 != 1
    RETURN

;単体지정
IF MATCH(カード名, "엑소시스터・미카에리스")
    IF MATCH(使用素材名前, "엑소시스터・에리스")
        CALL MAT_EVENT_ACT(듀얼리스트,"엑소시스터・에리스 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터・카스피텔")
    IF MATCH(使用素材名前, "엑소시스터・스텔라")
        CALL MAT_EVENT_ACT(듀얼리스트,"엑소시스터・스텔라 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터・아소필")
    IF MATCH(使用素材名前, "엑소시스터・소피아")
        CALL MAT_EVENT_ACT(듀얼리스트,"엑소시스터・소피아 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터・지브린느")
    IF MATCH(使用素材名前, "엑소시스터・이렌느")
        CALL MAT_EVENT_ACT(듀얼리스트,"엑소시스터・이렌느 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터・루키파르")
    IF MATCH(使用素材名前, "엑소시스터・마르파")
        CALL MAT_EVENT_ACT(듀얼리스트,"엑소시스터・마르파 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터・미카에리스")
    IF !MATCH(使用素材名前, "엑소시스터・에리스")
        SIF MATCH(使用素材名前, "크라이시스터・에리스")
            CALL MAT_EVENT_ACT(듀얼리스트,"크라이시스터・에리스 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터・카스피텔")
    IF !MATCH(使用素材名前, "엑소시스터・스텔라")
        SIF MATCH(使用素材名前, "크라이시스터・스텔라")
            CALL MAT_EVENT_ACT(듀얼리스트,"크라이시스터・스텔라 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터・아소필")
    IF !MATCH(使用素材名前, "엑소시스터・소피아")
        SIF MATCH(使用素材名前, "크라이시스터・소피아")
            CALL MAT_EVENT_ACT(듀얼리스트,"크라이시스터・소피아 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터・지브린느")
    IF !MATCH(使用素材名前, "엑소시스터・이렌느")
        SIF MATCH(使用素材名前, "크라이시스터・이렌느")
            CALL MAT_EVENT_ACT(듀얼리스트,"크라이시스터・이렌느 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "새겨진 마의 크라이시스터")
    IF MATCH(使用素材名前, "마를 새기는 데먼스미스")
        CALL MAT_EVENT_ACT(듀얼리스트,"새겨진 마의 크라이시스터 소환")
    ENDIF
ENDIF

IF MATCH(カード名, "카타르시스터・말파스")
    IF MATCH(使用素材名前, "크라이시스터・마르파")
        IF MATCH(使用素材名前, "카타르시스터・에리스텔라")
            IF MATCH(使用素材名前, "카타르시스터・소피렌느")
                CALL MAT_EVENT_ACT(듀얼리스트,"말파스 소환")
            ENDIF
        ENDIF
    ENDIF
ENDIF


IF MATCH(カード名, "바르모니카の淫신－ラッシーヴィア")
    IF MATCH(使用素材名前:0, "サキューベ・바르모니카")
        IF MATCH(使用素材名前:1, "セレトリーチェ・바르모니카")
            CALL MAT_EVENT_ACT(듀얼리스트,"ラッシーヴィア소환")
        ENDIF
    ENDIF
ENDIF

;複数지정
IF MATCH(使用素材名前, "アルバスの落胤")
    IF MATCH(使用素材名前, "教導の聖女エクレシア") || MATCH(使用素材名前, "白の聖女エクレシア")
        CALL MAT_EVENT_ACT(듀얼리스트,"アルバス＋エクレシア")
    ENDIF
ENDIF

IF MATCH(使用素材名前, "デスピアの導化アルベル") || MATCH(使用素材名前, "더 비스테드アルベル")
    IF MATCH(使用素材名前, "教導の聖女エクレシア") || MATCH(使用素材名前, "白の聖女エクレシア")
        CALL MAT_EVENT_ACT(듀얼리스트,"アルベル＋エクレシア")
    ENDIF
ENDIF


IF MATCH(カード名, "카타르시스터・에리스텔라")
    IF MATCH(使用素材名前, "크라이시스터・에리스") || MATCH(使用素材名前, "엑소시스터・에리스")
        IF MATCH(使用素材名前, "크라이시스터・스텔라") || MATCH(使用素材名前, "엑소시스터・스텔라")
            CALL MAT_EVENT_ACT(듀얼리스트,"에리스텔라 소환")
        ENDIF
    ENDIF
ENDIF

IF MATCH(カード名, "카타르시스터・소피렌느")
    IF MATCH(使用素材名前, "크라이시스터・소피아") || MATCH(使用素材名前, "엑소시스터・소피아")
        IF MATCH(使用素材名前, "크라이시스터・이렌느") || MATCH(使用素材名前, "엑소시스터・이렌느")
            CALL MAT_EVENT_ACT(듀얼리스트,"소피렌느 소환")
        ENDIF
    ENDIF
ENDIF



RETURN

@MAT_EVENT_ACT(듀얼리스트,実行コマンド)
#DIMS DYNAMIC 実行コマンド
#DIMS DYNAMIC 듀얼리스트



SELECTCASE 実行コマンド

    CASE "엑소시스터・에리스 변신"
        PRINTFORML
        PRINTFORML 에리스「아멘트！」
        PRINTFORML 엑소시스터・에리스는 미카엘의 가호로 변신한다！
        PRINTFORMW

    CASE "엑소시스터・스텔라 변신"
        PRINTFORML
        PRINTFORML 스텔라「아멘트！」
        PRINTFORML 엑소시스터・스텔라는 카스피텔의 가호로 변신한다！
        PRINTFORMW

    CASE "엑소시스터・소피아 변신"
        PRINTFORML
        PRINTFORML 소피아「아멘트！」
        PRINTFORML 엑소시스터・소피아는 아나엘의 가호로 변신한다！
        PRINTFORMW

    CASE "엑소시스터・이렌느 변신"
        PRINTFORML
        PRINTFORML 이렌느「아멘트！」
        PRINTFORML 엑소시스터・이렌느는 지브릴의 가호로 변신한다！
        PRINTFORMW

    CASE "엑소시스터・마르파 변신"
        PRINTFORML
        PRINTFORML 마르파「아멘트」
        PRINTFORML 엑소시스터・마르파는 루시퍼의 가호로 변신한다！
        PRINTFORMW


    CASE "크라이시스터・에리스 변신"
        PRINTFORML
        PRINTFORML 에리스「아멘트！」
        PRINTFORML 크라이시스터・에리스는 미카엘의 가호로 변신한다！
        PRINTFORML 에리스「읏……！」
        PRINTFORML 하지만 미카엘의 빛은 칙칙하다…
        PRINTFORMW

    CASE "크라이시스터・스텔라 변신"
        PRINTFORML
        PRINTFORML 스텔라「아멘트！」
        PRINTFORML 크라이시스터・스텔라는 카스피엘의 가호로 변신한다！
        PRINTFORML 스텔라「읏……！」
        PRINTFORML 하지만 카스피엘의 빛은 칙칙하다…
        PRINTFORMW

    CASE "크라이시스터・소피아 변신"
        PRINTFORML
        PRINTFORML 소피아「아멘트！」
        PRINTFORML 크라이시스터・소피아는 아나엘의 가호로 변신한다！
        PRINTFORML 소피아「읏……！」
        PRINTFORML 하지만 아나엘의 빛은 칙칙하다…
        PRINTFORMW

    CASE "크라이시스터・이렌느 변신"
        PRINTFORML
        PRINTFORML 이렌느「아멘트！」
        PRINTFORML 크라이시스터・이렌느는 지브릴의 가호로 변신한다！
        PRINTFORML 이렌느「읏……！」
        PRINTFORML 하지만 리브릴의 빛은 칙칙하다…
        PRINTFORMW


    CASE "アルバス＋エクレシア"
        PRINTFORM
        PRINTFORML 알버스「에클레시아！」
        PRINTFORML 에클레시아「알버스！」
        PRINTFORML 알버스와 에클레시아가 손을 잡은 순간、두사람은 빛을 뿜어내고 합쳐졌다！
        PRINTFORMW

	CASE "アルベル＋エクレシア"
        PRINTFORM
        PRINTFORML 에클레시아「미안해、알버스。이것도 마스터의 명령이니까」
        PRINTFORML 에클레시아「저……알베르와 합체합니다」
        PRINTFORMW


	CASE "에리스텔라 소환"
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 『악령 아스모데우스』가 빛의 륜으로 변화하니、
        PRINTFORML 마치 도망칠수 없게하듯한 형태로 전개하여、
        PRINTFORML 싱크로소재가 된 에리스와 스텔라를 빛의 점으로 변화시킨다…
        PRINTFORMW 악령에 저항하지 못하고 마음이 무너진 에리스와 스텔라는 몸을 가까이 했다…！
        RESETCOLOR
        PRINTFORML
        CALL EROTIC_MOVE("全身凌辱",NO_TO_CHARANUM(2100),-1,500)
        CALL EROTIC_MOVE("全身凌辱",NO_TO_CHARANUM(2101),-1,500)
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 눈부신 빛속에서 에리스와 스텔라 두사람의 특징이 드문드문보인다
        PRINTFORML 높은키에 고혹적인 몸매의 타락한 구원소녀의 모습이 나타나니、
        PRINTFORMW 당신에게 공손하게 예를 차리고、대전상대쪽을 향했다…！
        RESETCOLOR
        PRINTFORML
		SETVAR @"%듀얼리스트%_MZ_言発:現필드(듀얼리스트)", 1

	CASE "소피렌느 소환"
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 『악령 마르코시어스』가 빛의 륜으로 변화하니、
        PRINTFORML 바치 도망칠수 없게하듯한 형태로 전개하여、
        PRINTFORML 싱크로소재가 된 소피아와 시렌느를 빛의 점으로 변화시킨다…
        PRINTFORMW 악령에 저항하지 못하고 마음이 무너진 소피아와 이렌느는 몸을 가까이 했다…！
        RESETCOLOR
        PRINTFORML
        CALL EROTIC_MOVE("全身凌辱",NO_TO_CHARANUM(2102),-1,500)
        CALL EROTIC_MOVE("全身凌辱",NO_TO_CHARANUM(2103),-1,500)
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 눈부신 빛속에서 소피아와 이렌느 두사람의 특징이 드문드문 보인다
        PRINTFORML 높은키에 고혹적인 몸매의 타락한 구원소녀의 모습이 나타나니、
        PRINTFORMW 당신의 오른손을 잡고 손등에 키스하고、대전상대쪽을 향했다…！
        RESETCOLOR
        PRINTFORML
		SETVAR @"%듀얼리스트%_MZ_言発:現필드(듀얼리스트)", 1


    CASE "말파스 소환"
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 크라이시스터・마르파는 말파스의 가호로 변신하면서 다른 시스터를 말려들게했다！
        RESETCOLOR
        PRINTFORML
        CALL EROTIC_MOVE("全身레즈애무",NO_TO_CHARANUM(2100),NO_TO_CHARANUM(2104),500)
        CALL EROTIC_MOVE("全身레즈애무",NO_TO_CHARANUM(2101),NO_TO_CHARANUM(2104),500)
        CALL EROTIC_MOVE("全身레즈애무",NO_TO_CHARANUM(2102),NO_TO_CHARANUM(2104),500)
        CALL EROTIC_MOVE("全身레즈애무",NO_TO_CHARANUM(2103),NO_TO_CHARANUM(2104),500)
		SETVAR @"%듀얼리스트%_MZ_言発:現필드(듀얼리스트)", 1


    CASE "ラッシーヴィア소환"
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 음마의 목소리에 꼬드겨진 셀레트리체 바르모니카는 개방적으로 되었다！
        RESETCOLOR
        PRINTFORML
		SETVAR @"%듀얼리스트%_MZ_言発:現필드(듀얼리스트)", 1

    CASE "새겨진 마의 크라이시스터 소환"
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 데먼스미스는 레퀴엠에 크라이시스터 몬스터를 집어넣고 나타났다！
        RESETCOLOR
        PRINTFORML

ENDSELECT


RETURN
