				条件달성 ++
			ENDIF
		ENDIF
		IF GETVARS(@"%実行者%_MZ_改名:カウンタ") != ""
			VARSET 条件달성
		ENDIF

	ENDIF
	IF 参照先 == "마함"
		;改名されてるとカテゴリをすべて失い、추가カテゴリのみ
		IF GETVARS(@"%実行者%_魔_改名:カウンタ") != ""
			VARSET 条件달성
		ENDIF
		VARSET カード追カテ
