IF 표시形式 == "守備표시"
	SETVAR @"%듀얼리스트%_MZ_表:カード소환先 ", 2
ELSEIF 표시形式 == "裏側표시"
	SETVAR @"%듀얼리스트%_MZ_表:カード소환先 ", 3
ELSE 
	SETVAR @"%듀얼리스트%_MZ_表:カード소환先 ", 1
ENDIF
SETVAR @"%듀얼리스트%_필드:カード소환先 ", カードID
CALL CARD_LOADUP

RESULT = LRESULT


@CARD_BANISH(듀얼리스트,ゾーン,場所)
