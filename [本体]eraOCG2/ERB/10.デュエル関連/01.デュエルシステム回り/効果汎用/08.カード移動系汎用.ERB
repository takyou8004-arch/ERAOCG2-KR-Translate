@CARD_EXDECK(듀얼리스트,ルーチン状態)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC スタッツ合計
#DIM DYNAMIC 最大スタッツ合計
#DIM DYNAMIC ルーチン状態
#DIM DYNAMIC 現攻撃力
#DIM DYNAMIC 現守備力
#DIM DYNAMIC カードID
#DIM DYNAMIC 소환可能



CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

$INPUT_LOOP_EXDECK
IF 듀얼리스트 == "자신"
	CUSTOMDRAWLINE =
	PRINTFORML 「EX덱 확인 화면」
	CUSTOMDRAWLINE =
	PRINTL
ENDIF
VARSET 選択カード, -1
VARSET 最大スタッツ合計, -1
FOR カウンタ,0,紙束最大値
	VARSET 소환可能
	SIF GETVAR(@"%듀얼리스트%_EX덱:カウンタ") == -1
		CONTINUE
	TRYCALLFORM CARD_{GETVAR(@"%듀얼리스트%_EX덱:カウンタ")}, "종류"
	IF GROUPMATCH(RESULT, 싱크로, 엑시즈, 링크, 초차원)
		TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:カウンタ")}(듀얼리스트,カウンタ,1)
		IF RESULT == 1 && 듀얼리스트 == "자신" && バトルフェイズ == 0
			소환可能 = 1
		ELSEIF RESULT == 0 && 듀얼리스트 == "자신" && バトルフェイズ == 0
			CALL CARD_SETCOLOR("자신","EX덱",カウンタ)
		ELSEIF RESULT == 1 && 듀얼리스트 == "상대"
			VARSET スタッツ合計 
			VARSET カードID		
			カードID = GETVAR(@"%듀얼리스트%_EX덱:カウンタ")

			TRYCALLFORM CARD_{カードID}, "攻撃力"
			現攻撃力 = RESULT
			TRYCALLFORM CARD_{カードID}, "守備力"
			現守備力 = RESULT
			IF ルーチン状態 == 0
				CALL ENEMY_RUTIN_PRIORITY(カードID)
				スタッツ合計 = 現攻撃力 + 現守備力 + RESULT
				IF スタッツ合計 > 最大スタッツ合計
					最大スタッツ合計 = スタッツ合計
					選択カード = カウンタ
				ENDIF
			ELSEIF ルーチン状態 == 1
				CALL ENEMY_RUTIN_PRIORITY(カードID)
				スタッツ合計 = 現攻撃力 + 現守備力 + RESULT
				IF スタッツ合計 > 最大スタッツ合計 && スタッツ合計 > 19999
					最大スタッツ合計 = スタッツ合計
					選択カード = カウンタ
				ENDIF
			ENDIF	
		ENDIF
	ENDIF
	IF 듀얼리스트 == "자신"
		VARSET RESULT
		TRYCALLFORM CARDCAN_{GETVAR(@"%듀얼리스트%_EX덱:カウンタ")}("자신",EX덱起動,"EX덱",カウンタ)
		IF RESULT == 1
			SETCOLOR C_G_YELLOW
			PRINT (발)
		ELSEIF 소환可能 == 1
			SETCOLOR C_G_YELLOW
			PRINT (소)
		ELSE
			SETCOLOR C_GRAY
			PRINT (　)
		ENDIF
		CALLFORM CARD_KANPRINT("EX덱",カウンタ,자신_EX덱:カウンタ)

		RESETCOLOR
	ENDIF
NEXT

IF 듀얼리스트 == "자신"
	CUSTOMDRAWLINE —
	PRINTL
	PRINTL [100] - 돌아간다
	BINPUT
	選択カード = RESULT
ELSEIF 듀얼리스트 == "상대"
	SIF 選択カード == -1
		RETURN 0
	CUSTOMDRAWLINE —
	PRINTL
	PRINTL 【엑스트라덱에서 특수소환 될 카드를 확인해주세요】
	PRINTL
	CUSTOMDRAWLINE —
	WAIT
ENDIF

;돌아간다
IF 選択カード== 100
	PRINTFORMW 돌아갑니다。
	RETURN 0
ELSEIF INRANGE(選択カード, 0,紙束最大値)
	選択カード_ID = GETVAR(@"%듀얼리스트%_EX덱:選択カード")	
	CALL CARD_UI_RESET
	確認듀얼리스트 = %듀얼리스트%
	確認場所 = 選択カード
	確認ゾーン = EX덱
	TRYCALLFORM CARD_UI(選択カード_ID,2);　カード生成からくるUI（受け渡し必要：듀얼리스트情報のみ）

	TRYCALLFORM CARD_{選択カード_ID}("종류")
	SELECTCASE RESULT
	CASE 싱크로
		IF 듀얼리스트 == "자신"
			TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:選択カード")}(듀얼리스트,選択カード,1)
			SIF RESULT == 1 && ターンプレイヤー == 듀얼리스트 && バトルフェイズ == 0
