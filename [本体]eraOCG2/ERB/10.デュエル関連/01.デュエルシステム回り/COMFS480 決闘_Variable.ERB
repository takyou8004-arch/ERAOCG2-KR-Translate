@CHOICE_DUEL_TYPE(이벤트起動,連戦플래그,先攻설정)
#DIM DYNAMIC 이벤트起動
#DIM DYNAMIC ターゲット보존,9999
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 連戦플래그
#DIM DYNAMIC 先攻설정
#DIM DYNAMIC デュエル結果
FOR カウンタ,0,TARGETNUM
	ターゲット보존:カウンタ = TARGET:カウンタ
NEXT
$INPUT_RETURN
CUSTOMDRAWLINE —
PRINT 준비화면
IF 依頼_見返し中 >= 1
	PRINT 　의뢰 되돌아보기 중에는 DP가 늘어나지 않습니다
ENDIF
PRINTL 
CUSTOMDRAWLINE —
PRINTL 　　[0] - 듀얼개시！
PRINTL 
IF 依頼_見返し中 >= 1
	PRINTL 　　[2] - 듀얼을 스킵한다
ELSE
	PRINTL 
ENDIF
CUSTOMDRAWLINE —
IF 連戦플래그 == 0
	PRINTL [90] - 덱을 편집한다
	CUSTOMDRAWLINE —
ENDIF
PRINT [99] - 듀얼타입 설정
SELECTCASE デュエル타입
CASE 0
	PRINTL [스탠다드 룰]
CASE 1
	PRINTL [스피드 룰:LP 4000]
CASE 2
	PRINTL [간이 듀얼：가위바위보]
ENDSELECT
PRINTL 
BINPUT
IF RESULT == 0
	IF TARGET >= 1
		BASE:TARGET:체력 = MAXBASE:TARGET:체력
		BASE:TARGET:기력 = MAXBASE:TARGET:기력
	ENDIF
	IF デュエル타입 == 2
		CALL DUEL_KANNI
	ELSE
		CALL DUEL_SYSTEM(1,이벤트起動,連戦플래그,先攻설정)
	ENDIF

	デュエル結果 = RESULT
ELSEIF RESULT == 2
	デュエル結果 = 1
ELSEIF RESULT == 90
	CALL EDIT_DECK_EX
	GOTO INPUT_RETURN

ELSEIF RESULT == 99
	PRINTL 듀얼타입의 설정
	PRINTL [0] - OCG에 가까운 룰（LP8000스타트）
	PRINTL [1] - 스피드 듀얼에 가까운 룰（LP4000스타트）
	PRINTL [2] - 간이 룰（가위바위보 듀얼）
	BINPUT
	デュエル타입 = RESULT
	GOTO INPUT_RETURN
ENDIF

VARSET OPPOSER名

FOR カウンタ,0,TARGETNUM
	TARGET:カウンタ = ターゲット보존:カウンタ
NEXT
RETURN デュエル結果


;――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――

@DUEL_STANDBY(選択モード)
#DIM DYNAMIC デュエル結果
#DIM DYNAMIC ターゲット보존,9999
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択モード
#DIM DYNAMIC LTARGET,99999
#DIM DYNAMIC LTARGETNUM
#DIM DYNAMIC 事後カード化
#DIM DYNAMIC 패배のロストバージン
#DIM DYNAMIC 先攻플래그
#DIM DYNAMIC ランダム値
SELECTCASE 選択モード
;TARGETと듀얼
CASE 0
	TFLAG:SELECTCOM분기 = 10
	CALL KOJO_MESSAGE_SEND("COMMAND")
;レート戦/練習戦
CASE 2,4
	TARGET = 0 
;その他
CASE 3
	;몬스터로 あり、所有をしていない場合。
	PRINTFORML 
	PRINTFORML 무엇을 거시겠습니까？
	PRINTFORML 
	PRINTFORML [1] - 팩을 받는다
	;동행者が居ない場合
	IF CFLAG:TARGET:동행 <= 0
		PRINTFORML [2] - 강제적으로 데이트하자 한다
		SIF CFLAG:TARGET:패배함 >= 1
			PRINTFORML [3] - 팬티를 받는다
		SIF CFLAG:TARGET:패배함 >= 2 && CFLAG:MASTER:현재위치 >= 20
			PRINTFORML [4] - 방에 데리고 간다
		IF TALENT:TARGET:듀얼리스트 == 0 && CFLAG:TARGET:소유함 == 0 && CFLAG:MASTER:현재위치 < 500
			PRINTFORML [5] - 이 세계의 주민이 아니지 않나？라고 몰아붙인다
		;Anonym추가
		ELSEIF ITEM:천년카메라 != 0 && CFLAG:TARGET:소유함 == 0
			PRINTFORML [10] - 어둠의 게임
		ENDIF
		IF CFLAG:MASTER:현재위치 >= 20
			;勝ったら賞金、負けたらホテルのハイリスクハイリターンなバトル。所持金一定以上が条件。お金をちらつかせて女の子の判断を鈍らせよう！
			;ゆくゆくは大会優勝とかで手に入るレア카드 라던가を賭けたり、자신의 悪辣度が高いと상대から「討伐」しようとしてきたりする……みたいな感じに변경予定？
			;子供 まだ幼いので10万円でも乗っちゃう
			IF TALENT:TARGET:연령 == -2 && MONEY >= 100000
				PRINTFORML [6] - 몸을 요구한다
			;学生 10万じゃリスクに釣り合わないけど50万なら渋々飲みそう
			ELSEIF TALENT:TARGET:연령 == -1 && MONEY >= 500000
				PRINTFORML [6] - 몸을 요구한다
			;それ以上 一括で100万。実家が太かったり金持ちだったりする캐릭터を素質で判別する手段がなさそうなので。
			;弱みを握った상대
			ELSEIF (TALENT:TARGET:연령 >= 0 && MONEY >= 1000000) || CFLAG:TARGET:약점장악 == 1
				PRINTFORML [6] - 몸을 요구한다
			ENDIF
		ENDIF
	ENDIF
	PRINTFORML 
	PRINTFORML [100] - 돌아간다
	BINPUT
	SIF RESULT == 100
		RETURN 0

	賭け内容 = RESULT
	SIF 賭け内容 >= 1
		CFLAG:TARGET:내기플래그 = 1
	SELECTCASE 賭け内容
	CASE 1 TO 4
		IF RAND:1000 > CFLAG:TARGET:호감도 && CFLAG:TARGET:약점장악 == 0
			$INPUT_LOOP
			PRINTFORML 거절당했습니다……
			PRINTFORML [0] - 끈질기게 말해본다
			PRINTFORML [1] - 포기한다
			BINPUT
			IF RESULT == 1
				TFLAG:SELECTCOM분기 = 0
				RETURN 0
			ENDIF
			ランダム値 = RAND:100
			SELECTCASE ランダム値
			CASE 0 TO 4
				CALL EVENT_FAVUP(TARGET,-500)
				PRINTFORMW %CALLNAME:TARGET%은 지나친 집요함에、디스크를 준비했다。
				PRINTFORMW 응해준 것 같지만…… 왠지 선공을 잡을 수 있을 것 같지 않아……。
				先攻플래그 = 1
			CASE 5 TO 24
				CALL EVENT_FAVUP(TARGET,-30)
				PRINTFORMW %CALLNAME:TARGET%는 어쩔수 없이 내기에 반응해주었습니다。
			CASE 25 TO 44
				CALL EVENT_FAVUP(TARGET,-100)
				PRINTFORMW %CALLNAME:TARGET%는 화나서 돌아가버렸습니다……。
				CFLAG:TARGET:현재위치 = 100
				TFLAG:SELECTCOM분기 = 0
				RETURN 0
			CASEELSE
				PRINTFORML 거절당했습니다……
				CALL EVENT_FAVUP(TARGET,-30)
				GOTO INPUT_LOOP
			ENDSELECT
		ELSE
			PRINTFORML 내기에 응해주었습니다。
		ENDIF
	CASE 5
		PRINTFORML   ！？！？
		PRINTFORMW 추궁당한 %CALLNAME:TARGET%는 디스크를 빼고、듀얼의 체제를 갖춘다。
		PRINTFORMW 그냥은 카드가 될 생각이 없는 모양이다……。이쪽도 준비를 시작하자！
	CASE 6
		IF TALENT:TARGET:패배버릇 == 1
			PRINTFORML %CALLNAME:MASTER%가 『언제나의 내기』를 제안하자、지금까지 몇 번이고 몸에 패배를 새겨온 %CALLNAME:TARGET%은
			PRINTFORMW 다시 패배와 쾌락의 예감에 볼을 빨갛게 물들이며、표면상으로는 강인한 척하며 디스크를 준비했다……！
		ELSEIF CFLAG:TARGET:약점장악 == 1
			IF EXP:TARGET:패배조교경험 >= 1
				IF RAND:2 == 0
					PRINTFORMW %CALLNAME:MASTER%은 천천히 핸드폰을 만지며、 %CALLNAME:TARGET%에게 핸드폰을 확인하라고 말한다。
					PRINTFORMW 의심스러운 표정으로 스마트폰을 확인하며、메시지 어플의 새로운 알림을 발견한 %CALLNAME:TARGET%은 어플을 열어──
					CALLFORM IMAGE_PRODUCTION(TARGET,9999)
					PRINTFORML ──그리고 그것이 %CALLNAME:MASTER%가 보낸 언젠가의 『기념사진』이었다는 것을 알게 된 %CALLNAME:TARGET%은
					PRINTFORMW 얼굴을 빨갛게 물들이며 디스크를 준비해、필사적인 모습으로 「사진 삭제」와 「다시 한번의 섹스」를 건 싸움에 임했다……！
				ELSE
					PRINTFORML %CALLNAME:MASTER%이 이전에 %CALLNAME:TARGET%과 찍은 『기념사진』을 슬쩍 흔들흔들 하며 승부를 걸자、
					PRINTFORMW %CALLNAME:TARGET%는 분한 듯 입술을 깨물며、얼굴을 붉히면서도 디스크를 준비했다……！
				ENDIF
			ELSE
				PRINTFORML %CALLNAME:MASTER%가 이전에 찍은 %CALLNAME:TARGET%의 『약점』을 슬쩍 흔들흔들 하며 승부를 걸자、
				PRINTFORMW %CALLNAME:TARGET%는 분한 듯 입술을 깨물며、얼굴을 붉히면서도 디스크를 준비했다……！
			ENDIF
		ELSE
			;最終的には「レア카드를 ちらつかせた」とかに변경予定
			PRINTFORML 고액의 높은 상금을 내건 %CALLNAME:MASTER%가、지면 몸으로 갚으라는 하이레이트 승부를 제시하자
			PRINTFORMW %CALLNAME:TARGET%는 잠시 %TEXTR("골똘히 생각했/고개를 숙이며 망설이는 모습을 보였")%지만、이윽고 %TEXTR("떨떠름하게/")% 끄덕이며、조건을 받아들여 디스크를 준비했다……！
		ENDIF
	;Anonym추가
	CASE 10
		CALL EVILTY_POINT(10)
		PRINTFORMW   ！？！？
		PRINTL
		CALL FLASH(3)
		FOR LOCAL, 0, 70
			PRINTL
		NEXT
		PRINTFORMW %CALLNAME:MASTER%가 어둠의 게임을 선언한 순간、주위 일대가 어둠에 휩싸였다。
		PRINTFORMW 서로 지는 것이 용납되지 않는 듀얼이 시작된다……！
	ENDSELECT
CASE 5
	TFLAG:SELECTCOM분기 = 10
	CALL KOJO_MESSAGE_SEND("COMMAND")
ENDSELECT

;ターゲットの보존
FOR カウンタ,0,CHARANUM
	IF TARGET >= 1
		LTARGET:カウンタ = TARGET:カウンタ
		LTARGETNUM ++
	ENDIF
NEXT
SELECTCASE デュエル타입
CASE 2
	CALL DUEL_KANNI
CASEELSE
	;オンライン練習
	IF 選択モード == 2
		CALL DUEL_SYSTEM(2,,,先攻플래그설정)
	;オンラインレートマッチ
	ELSEIF 選択モード == 4 || 選択モード == 5
		CALL DUEL_SYSTEM(3)
	;公式デュエル
	ELSE
		CALL DUEL_SYSTEM(,,,先攻플래그)
	ENDIF
ENDSELECT
デュエル結果 = RESULT

;ターゲットの開放
FOR カウンタ,0,LTARGETNUM
	TARGET:カウンタ = LTARGET:カウンタ
NEXT

CALL DUEL_KAKE(デュエル結果,選択モード)
RETURN デュエル結果

;――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
@DUEL_KAKE(デュエル結果,選択モード)
#DIM DYNAMIC デュエル結果
#DIM DYNAMIC ターゲット보존,9999
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択モード
#DIM DYNAMIC LTARGET,99999
#DIM DYNAMIC LTARGETNUM
#DIM DYNAMIC 先攻플래그
#DIM DYNAMIC 事後カード化
#DIM DYNAMIC 패배のロストバージン
#DIM DYNAMIC ラブホ以外
#DIM DYNAMIC ＢＳＳ

ラブホ以外 = 0

IF 選択モード == 0
	IF デュエル結果 == 結果_승리
		IF CFLAG:TARGET:전화번호 == 0 && TALENT:TARGET:듀얼리스트 && TARGET != 0
			CFLAG:TARGET:전화번호 = 1
			PRINTFORMW 듀얼에서 승리하여 %CALLNAME:TARGET%로부터 전화번호를 받았다！
		ENDIF
		CALL SOURCE_CALC_환락(TARGET, MASTER, 500)
		CALL SOURCE_CALC_정복(TARGET, MASTER, 100)
	ELSEIF デュエル結果 == 結果_패배
		CALL SOURCE_CALC_환락(TARGET, MASTER, 200)
		CALL SOURCE_CALC_정복(TARGET, MASTER, 50)
	ELSEIF デュエル結果 == 結果_引分
		CALL SOURCE_CALC_환락(TARGET, MASTER, 200)
	ENDIF
ELSEIF 選択モード == 4
	IF デュエル結果 == 結果_승리
		PRINTFORML 승리해서 레이트가 올랐다！
		PRINTFORMW 현재 온라인 레이트:{オンラインレート} + {レート増減}
		オンラインレート += レート増減
	ELSEIF デュエル結果 == 結果_패배
		PRINTFORML 패배해서 레이트가 내려갔다…
		PRINTFORMW 현재 온라인 레이트:{オンラインレート} - {レート増減}
		オンラインレート -= レート増減
	ENDIF
ELSEIF 選択モード == 5 && TALENT:TARGET:듀얼리스트 >= 1
	IF デュエル結果 == 結果_승리
		PRINTFORML 승리해서 레이트가 올랐다！
		PRINTFORML 현재 자신의 오프라인 레이트:{CFLAG:MASTER:오프라인레이트} + {レート増減}
		PRINTFORML 현재 %CALLNAME:TARGET%의 오프라인 레이트:{CFLAG:TARGET:오프라인레이트} - {レート増減 }
		CFLAG:MASTER:오프라인레이트 += レート増減
		CFLAG:TARGET:오프라인레이트 -= レート増減 * 3
		SIF CFLAG:TARGET:오프라인레이트 == 0
			CFLAG:TARGET:오프라인레이트 = -1
	ELSEIF デュエル結果 == 結果_패배
		PRINTFORML 패배해서 레이트가 내려갔다…
		PRINTFORML 현재의 자신의 오프라인 레이트:{CFLAG:MASTER:오프라인레이트} - {レート増減 * 3}
		PRINTFORML 현재의 %CALLNAME:TARGET%의 오프라인 레이트:{CFLAG:TARGET:오프라인레이트} + {レート増減 * 3}
		CFLAG:MASTER:오프라인레이트 -= レート増減
		SIF CFLAG:MASTER:오프라인레이트 == 0
			CFLAG:MASTER:오프라인레이트 = -1
		CFLAG:TARGET:오프라인레이트 += レート増減 * 3
	ENDIF
ENDIF
SIF 賭け内容 == 0
	GOTO SKIP_KAKE
IF デュエル結果 == 結果_승리
	SELECTCASE 賭け内容
	CASE 1
		CALL EVILTY_POINT(1)
		PRINTFORMW 건 만큼의 카드팩을 받았다！
		CALL CARD_PACK_SHORI(3)
	CASE 2
		CALL EVILTY_POINT(5)
		PRINTFORMW 듀얼에서 승리하여 %CALLNAME:TARGET%을 데이트에 초대하는 데 성공했다。
		PRINTFORMW 내기에 진 %CALLNAME:TARGET%은、얼굴을 붉히며 손을 내밀어 주었다。
		SIF CFLAG:TARGET:패배함 <= 1
			CFLAG:TARGET:패배함 = 1
		SIF CFLAG:TARGET:데이트중 != 1
			CFLAG:TARGET:데이트중 = 1
		CFLAG:TARGET:동행 = 1400
	CASE 3
		CALL EVILTY_POINT(10)
		PRINTFORML 듀얼에 패배한 %CALLNAME:TARGET%은、%TEXTR("분한 듯 얼굴을 찌푸리며/부끄러운듯이 얼굴이 빨개지며")% 그 자리에서 치마 속으로 손을 넣어、
		PRINTFORMW %CALLNAME:MASTER%가 보는 앞에서 스르륵 속옷을 내리고 있다……。
		PRINTL
		PRINTFORMW 수치를 견디며 팬티를 다 벗은 %CALLNAME:TARGET%은、아직 %TEXTR("온기/체온")%이 남은 막 벗은 속옷을 %CALLNAME:MASTER%에게 넘겨주었다！
		PRINTL
		PRINTFORML %TEXTR("히죽히죽 웃는/촉감을 만끽하는/온기를 만끽하는/그 자리에서 냄새를 맡기 시작하는")% %CALLNAME:MASTER%의 모습에 역시 돌려줘！라며 매달리지만、내기는 절대적이다。
		PRINTFORMW %CALLNAME:MASTER%는 %CALLNAME:TARGET%의 속옷을 컬렉션에 추가했다……。
		TSTR:3 = %CALLNAME:TARGET%{EQUIP:TARGET:10}/
		SAVESTR:10 += CSTR:MASTER:모은팬티
		CSTR:MASTER:모은팬티 =
		EQUIP:TARGET:하반신속옷２ = 0
		CFLAG:TARGET:(200 + 6) = 0
		SIF CFLAG:TARGET:패배함 <= 2
			CFLAG:TARGET:패배함 = 2
	CASE 4
		CALL EVILTY_POINT(10)
		PRINTFORMW 듀얼에서 승리하여 %CALLNAME:TARGET%을 정중하게 모셔가기로 했다……。
		PRINTFORMW 납득하지 못하는 것 같지만、듀얼리스트가 건 내기에 말을 바꾸진 않았다。
		CFLAG:MASTER:현재위치 = 1
		CFLAG:TARGET:현재위치 = 1
		TIME += 30
		SIF CFLAG:TARGET:패배함 <= 3
			CFLAG:TARGET:패배함 = 3
		SETBIT CFLAG:TARGET:기정사실, 0
		人祓いモード = 0
	CASE 5
		IF TALENT:TARGET:성별 == 1
			PRINTFORML 패자가 된 %CALLNAME:TARGET%에게 %CALLNAME:MASTER%가 다가가자、 %CALLNAME:TARGET%는 특히나 겁에 질려、
			;캐릭터によっては露骨に性をアピールして命乞いをするかもしれない。ブリーチでそんな感じの見たぞオイラ！
			;非처녀、포식자、あとは無様な命乞い姿が似合う메스가키と즉시타락でとりあえずGO!
