@ENEMY_RUTIN_EXSUM(소환カード,カードID,소환法) 
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환法
#DIM DYNAMIC 소환カード
#DIM DYNAMIC カードID
#DIM DYNAMIC カウンタ

FOR カウンタ,0,5
	SIF 상대_필드:カウンタ == -1
		CONTINUE
	IF カードID == 상대_필드:カウンタ
		RETURN 1
	ENDIF
NEXT
RETURN 0

;そもそも효과를 발동するかの설정
@ENEMY_RUTIN_EFFECT(종류,ゾーン,場所,カードID)
#DIM DYNAMIC カードID
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 대상효과種
#DIM DYNAMIC 대상효과ID
#DIM DYNAMIC 行数カウント
#DIM DYNAMIC 行数カウント（終）
#DIM DYNAMIC 使用行数

#DIM DYNAMIC 後カード_ID
#DIMS DYNAMIC 後듀얼리스트
#DIM DYNAMIC 後효과種
#DIMS DYNAMIC 後ゾーン
#DIM DYNAMIC 後場所

IF 効後_カウンタ != 0
	後カード_ID = 効後_효과ID:(効後_カウンタ - 1)
	後듀얼리스트 = %効後_実行者:(効後_カウンタ - 1)%
	後효과種 = 効後_효과種:(効後_カウンタ - 1)
	後ゾーン = %効後_ゾーン:(効後_カウンタ - 1)%
	後場所 = 効後_設場所:(効後_カウンタ - 1)
ENDIF

SIF カードID <= 0
	RETURN 0
;基本的には同じIDの 카드는 연속的に발동しない
FOR カウンタ,0,100
	SIF GROUPMATCH(상대_誘発選択済:カウンタ,カードID)
		RETURN 1
NEXT

CALLFORM CARDNAME_{カードID}("名前")
SELECTCASE RESULTS
CASE "사이코 엔드 퍼니셔"
	IF 종류 == 필드起動
		SIF モンスター数("자신") == 0
			RETURN 1
		CALL FULLRESET_EXT
		濾過範囲필드 = 1
		濾過条件固名前 = 사이코 엔드 퍼니셔
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 0
			RETURN 1
	ENDIF

;トラブルサン：無駄に使わないようにするため
CASE "Evil★Twin's 트러블 써니","The Evil★Twins！키스킬＆리일라"
	SIF モンスター数("자신") == 0
		RETURN 1
;シュトラールを戻させないため
CASE "드래곤메이드의 전환"
	IF 종류 == 묘지起動
		CALL FULLRESET_EXT
		濾過範囲필드 = 1
		濾過条件カテゴリ = 드래곤메이드
		濾過条件종류 = 효과モン
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 0
			RETURN 1
	ENDIF
;상검顕現、有効な場間がないため
CASE "용상검현"
	SIF 종류 == 제외時
		RETURN 1
;同上
CASE "충혹의 동산"
	SIF 종류 == 마함起動 && 상대_魔_表:場所 == 1
		RETURN 1

;비밀병기 폭탄
CASE "비밀병기 폭탄"
	SIF (モンスター数("자신") + 마함数("자신")) < 4
		RETURN 1
;デミス、裁き、無駄な殺傷はやめろ
CASE "종언의 왕 데미스", "저지먼트 드래곤"
	SIF (モンスター数("자신") + 마함数("자신")) == 0
		RETURN 1
	SIF (モンスター数("상대") + 마함数("상대")) >= 3
		RETURN 1
;無駄な殺傷はやめろ
CASE "뉴스타트", "격류장"
	SIF モンスター数("자신") <= 1
		RETURN 1
CASE "여섯 무사의 문"
	IF 종류 == 마함起動 && 상대_魔_表:場所 == 1
		SIF 상대_魔_個Ｃ:場所 < 4
			RETURN 1
	ENDIF
CASE "여섯 무사의 결속"
	IF 종류 == 마함起動 && 상대_魔_表:場所 == 1
		SIF 상대_魔_個Ｃ:場所 <= 1
			RETURN 1
	ENDIF

CASE "드래그마의 기사 플루르드리스"
	SIF (モンスター数("자신")) == 0
		RETURN 1
CASE "섬도희－레이"
	SIF ターンプレイヤー == "상대" && メインフェイズ == 1
		RETURN 1
CASE "다크 다이브 봄버"
	IF 종류 == 필드起動
		SIF 상대_MZ_戦:場所 != 0
			RETURN 1
	ENDIF
CASE "무한포영", "이펙트 뵐러", "상검대사－적소"
	SIF 종류 != フリー誘発
		RETURN 1
	SIF 効後_カウンタ < 1
		RETURN 1

	FOR カウンタ,0,100
		SIF GROUPMATCH(상대_誘発選択済:カウンタ,90100,1095,50610)
			RETURN 1
	NEXT
CASE "마를 새기는 데먼스미스"
	IF 종류 == 필드起動
		CALL FULLRESET_EXT
		濾過範囲상대필드 = 1
		濾過範囲상대마함 = 1
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 0
			RETURN 1
	ENDIF
CASE "플레시아의 충혹마"
	IF 종류 == 필드起動 || 종류 == フリー誘発
		CALL FULLRESET_EXT
		濾過範囲묘지 = 1
		효과条件名前 = 홀티아의 충혹마
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 0
			RETURN 1
	ENDIF
CASE "하루 우라라"
	SIF 効後_カウンタ == 0
		RETURN 1
	SIF 後듀얼리스트 == "상대"
		RETURN 1

	うららチェック = 1

;	PRINTFORML 다음카드_ID:{後カード_ID}/다음듀얼리스트:%後듀얼리스트%/다음효과종류:{後효과種}/다음존:%後ゾーン%/다음장소:{後場所}
	TRYCALLFORM CARDCAN_{後カード_ID}(後듀얼리스트,後효과種,後ゾーン,後場所)
	VARSET うららチェック
;	PRINTFORMW 우라라체크패스 : {うららチェックパス}
	IF うららチェックパス == 0
		VARSET うららチェックパス	
		RETURN 1
	ELSE
		VARSET うららチェックパス	
	ENDIF
CASE "저택 와라시"
	SIF 効後_カウンタ == 0
		RETURN 1
	SIF 後듀얼리스트 == "상대"
		RETURN 1
	わらしチェック = 1
	TRYCALLFORM CARDCAN_{後カード_ID}(後듀얼리스트,後효과種,後ゾーン,後場所)
	VARSET わらしチェック
	IF わらしチェックパス == 0
		VARSET わらしチェックパス	
		RETURN 1
	ELSE
		VARSET わらしチェックパス	
	ENDIF
CASE "화목의 사자", "위협하는 포효", "패자의 일괄"
	SIF 발동済_1("상대",91090)
		RETURN 1
	SIF 발동済_1("상대",91091)
		RETURN 1
	SIF 발동済_1("상대",91100)
		RETURN 1
	SIF ターンプレイヤー == "상대"
		RETURN 1
CASE "멸망룡 베이도스"
	IF 종류 == 戦破壊→묘지
		CALL FULLRESET_EXT
		濾過範囲상대필드 = 1
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 0
			RETURN 1
	ENDIF
ENDSELECT

;カテゴリチェック
CALLFORM CARDNAME_{カードID}("カテゴリ")
SELECTCASE RESULTS
CASE "함정 속으로"
	FOR カウンタ,0,100
		VARSET RESULTS
		TRYCALLFORM CARDNAME_{상대_誘発選択済:カウンタ}("カテゴリ")
		SIF RESULTS　== "함정 속으로"
			RETURN 1
	NEXT
ENDSELECT

RETURN 0

;カードの素材化を守るための설정
@ENEMY_RUTIN_SOZAI(ゾーン,지정カード,소환法,소환カード_ID)
#DIM DYNAMIC 소환法
#DIM DYNAMIC 지정カード
#DIM DYNAMIC 소환カード_ID
#DIMS DYNAMIC ゾーン

SIF ALT条件 >= 1
	RETURN 0
	
IF ゾーン == "필드"
	SELECTCASE 소환法

	CASE 엑시즈
	CASE 싱크로
		CALLFORM CARD_{소환カード_ID}("レベル")
		IF RESULT < 8
			SIF 상대_MZ_攻:지정カード > 1950
				RETURN 1
		ENDIF
	CASEELSE
		SIF 상대_MZ_攻:지정カード > 1950
			RETURN 1
	ENDSELECT
	;セラは素材化から守られる
	SIF 상대_필드:지정カード == 1370
		RETURN 1
ENDIF
RETURN 0


;카드를 패에 加える処理で、どの카드가 選ばれるかの설정。
@ENEMY_RUTIN_CARD_ADD(参照先,場所,カードID)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 場所
#DIM DYNAMIC カードID
#DIM DYNAMIC 条件달성


SIF カードID <= 0
	RETURN 0

CALLFORM CARDNAME_{カードID}("名前")
SELECTCASE RESULTS
	CASE "마기스토스 베르 산드리용", "마기스토스 플레임 조로아", "마기스토스 마스터 엔디미온", "마기스토스 그리모어 크로울리", "마기스토스 다이몬 조로아"
		효과条件固名前:0 = 마기스토스 우르스라그나 조로아
		효과条件固名前:1 = 마기스토스 다이몬 조로아

	CASE "씰 오브・마기스토스 스푼"
		효과条件固名前:0 = 씰 오브 마기스토스 스푼
		효과条件固名前:1 = 마기스토스 우르스라그나 조로아
		효과条件固名前:2 = 마기스토스 다이몬 조로아

	CASE "사이코 엔드 퍼니셔"
		효과条件固名前 = 사이코 엔드 퍼니셔
	CASE "용상검현", "상검대사－적소"
		CALL FULLRESET_EXT
		濾過範囲손패 = 1
		濾過範囲묘지 = 1
		濾過条件名前 = 상검사－막야
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		IF RESULT == 0
			효과条件名前 = 상검사－막야
		ELSEIF RESULT == 1
			CALL FULLRESET_EXT
			濾過範囲손패 = 1
			濾過範囲묘지 = 1
			濾過条件名前 = 상검군사－용연
			CALL CARD_CHECK_EXT("상대",0,"",場所)
			SIF RESULT == 0
				효과条件名前 = 상검군사－용연
		ENDIF
	CASE "디마이즈도우저"
		CALL FULLRESET_EXT
		濾過範囲덱 = 1
		濾過条件名前 = 루인섹트 나이트
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 1
			条件달성 ++

		CALL FULLRESET_EXT
		濾過範囲묘지 = 1
		濾過条件名前 = 루인섹트 나이트
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		IF RESULT == 0 && 条件달성
			효과条件名前 = 루인섹트 나이트

		ELSEIF RESULT == 1
			VARSET 条件달성
			CALL FULLRESET_EXT
			濾過範囲덱 = 1
			濾過条件名前 = 공진충
			CALL CARD_CHECK_EXT("상대",0,"",場所)
			SIF RESULT == 1
				条件달성 ++
			CALL FULLRESET_EXT
			濾過範囲묘지 = 1
			濾過条件名前 = 공진충
			CALL CARD_CHECK_EXT("상대",0,"",場所)
			SIF RESULT == 0 && 条件달성
				효과条件名前 = 공진충
		ENDIF
	CASE "디클레어러 디바이너"
		CALL FULLRESET_EXT
		濾過範囲EX덱 = 1
		濾過条件名前 = 아크 디클레어러
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 1
			효과条件名前 = 아크 디클레어러
	CASE "우두귀", "유니좀비" 
		;PRINTL 여기까지 기안했다～
		CALL FULLRESET_EXT
		濾過範囲덱 = 1
		濾過条件名前 = 좀비 캐리어
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 1
			条件달성 ++
		CALL FULLRESET_EXT
		濾過範囲묘지 = 1
		濾過条件名前 = 좀비 캐리어
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 0 && 条件달성
			효과条件名前 = 좀비 캐리어
	CASE "골드 가제트", "실버 가제트","양철 금붕어"
		CALL FULLRESET_EXT
		濾過範囲손패 = 1
		濾過条件名前:0 = 그린 가제트
		濾過条件名前:1 = 레드 가제트
		濾過条件名前:2 = 옐로 가제트
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		IF RESULT == 1
			효과条件名前:0 = 그린 가제트
			효과条件名前:1 = 레드 가제트
			효과条件名前:2 = 옐로 가제트
		ENDIF

	CASE "여섯 무사의 군대장" ;Anonym추가
		CALL FULLRESET_EXT
		濾過範囲손패 = 1
		濾過範囲마함 = 1
		濾過条件名前 = 여섯 무사의 문
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		IF RESULT != 1
			CALL FULLRESET_EXT
			濾過範囲덱 = 1
			濾過条件名前 = 여섯 무사의 문
			CALL CARD_CHECK_EXT("상대",0,"",場所)
			IF RESULT == 1
				효과条件名前 = 여섯 무사의 문
			ENDIF
		ENDIF
	CASE "시엔의 봉화"
		CALL FULLRESET_EXT
		濾過範囲덱 = 1
		濾過条件名前 = 진 여섯 무사－카게키
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		IF RESULT == 1
			효과条件名前 = 진 여섯 무사－카게키
		ENDIF
	CASE "여섯 무사의 문"
		CALL FULLRESET_EXT
		濾過範囲덱 = 1
		濾過範囲묘지 = 1
		濾過条件名前 = 진 여섯 무사－키잔
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		IF RESULT == 1
			효과条件名前 = 진 여섯 무사－키잔
		ENDIF

	CASE "라바르 헬하운드"
		CALL FULLRESET_EXT
		濾過範囲덱 = 1
		濾過条件名前 = 라바르 아처
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 1
			条件달성 ++

		CALL FULLRESET_EXT
		濾過範囲묘지 = 1
		濾過条件名前 = 라바르 아처
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		IF RESULT == 0 && 条件달성
			효과条件名前 = 라바르 아처
		ENDIF
	CASE "E─이멀전시 콜", "엘리멘틀 히어로 에어맨"
		CALL FULLRESET_EXT
		濾過範囲덱 = 1
		濾過条件名前 = 엘리멘틀 히어로 어나더 네오스
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 1
			효과条件名前 = 엘리멘틀 히어로 어나더 네오스

	CASE "천저의 사도"
		IF 参照先 == "EX덱"
			CALL FULLRESET_EXT
			濾過範囲EX덱 = 1
			濾過条件名前 = 공명의 날개 가루라
			CALL CARD_CHECK_EXT("상대",0,"",場所)
			IF RESULT == 1
				SIF 발동済_1("상대",53000) == 0
					효과条件名前 = 공명의 날개 가루라
			ENDIF
		ENDIF
			
	CASE "빛의 원군"
		CALL FULLRESET_EXT
		濾過範囲덱 = 1
		濾過条件名前 = 라이트로드 서머너 루미너스
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 1
			효과条件名前 = 라이트로드 서머너 루미너스
	CASE "섬도희－하야테"
		CALL FULLRESET_EXT
		濾過範囲묘지 = 1
		濾過条件名前 = 섬도기동－인게이지
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 0
			효과条件名前 = 섬도기동－인게이지
		CALL FULLRESET_EXT
		濾過範囲묘지 = 1
		濾過条件名前 = 섬도희－레이
		CALL CARD_CHECK_EXT("상대",0,"",場所)
		SIF RESULT == 0
			효과条件名前 = 섬도희－레이
	CASE "섬도희－カガリ"
		CALL FULLRESET_EXT
		濾過範囲묘지 = 1
		濾過条件名前:0 = 섬도기동－인게이지
		CALL CARD_CHECK_EXT("상대",0,"",0)
		SIF RESULT == 1
			효과条件名前 = 섬도기동－인게이지
	CASE "섬도희－カガリ"
		CALL FULLRESET_EXT
		濾過範囲묘지 = 1
		濾過条件名前:0 = 섬도기동－인게이지
		CALL CARD_CHECK_EXT("상대",0,"",0)
		SIF RESULT == 1
			효과条件名前 = 섬도기동－인게이지
	CASE "섬도희－シズク"
		CALL FULLRESET_EXT
		濾過範囲묘지 = 1
		濾過条件名前:0 = 섬도기동－인게이지
		CALL CARD_CHECK_EXT("상대",0,"",0)
		SIF RESULT == 0
			효과条件名前 = 섬도기동－인게이지
