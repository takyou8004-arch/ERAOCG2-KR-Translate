	CALL FULLRESET
	CALL CARD_EQUIP_CHECK("자신",攻撃先,"필드")
	SIF RESULT == 0
		RETURN 0
	RETURN 999999
CASE "구슬의 미캉코 후우리"
	CALL FULLRESET
	CALL CARD_EQUIP_CHECK("자신",攻撃先,"필드")
	SIF RESULT == 0
		RETURN 0
	RETURN 999999
CASE "잭나이츠 파라디온 아스트람"
	SIF GETVAR(@"상대_MZ_召種:攻撃先") != 召種通常
		RETURN 999999
