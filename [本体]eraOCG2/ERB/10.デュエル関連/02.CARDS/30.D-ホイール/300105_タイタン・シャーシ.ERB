@CARDNAME_300105,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 타이탄 섀시
	CASE "略称"
		RESULTS = 타이탄 섀시
	CASE "カテゴリ"
		RESULTS:0 = 섀시
ENDSELECT


@CARD_300105(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN システム
ENDSELECT

RETURN 0

@CARD_EXPLANATION_300105(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,소환強制誘発)
PRINTL ①：자신이 주는 전투 데미지는 절반이 된다。
PRINTL ②：자신이 얻는 스피드 카운터는 최대 ３이 된다。


@CARDCAN_300105(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,300105)
SIF RESULT == 1
	RETURN 0

IF 종류 == 지속효과
	RETURN 1
ENDIF

@CARDEFFECT_300105(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択肢

IF 종류 == 지속효과
	CALL SET_PLAYER_INFO(듀얼리스트,"ダメージ半化", 3)
	IF GETVAR(@"%듀얼리스트%_SpＣ") >= 3
		SETVAR @"%듀얼리스트%_SpＣ", 3
	ENDIF
ENDIF
