@CARDNAME_300007,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 이펙트 엔진
	CASE "略称"
		RESULTS = 이펙트 엔진
	CASE "カテゴリ"
		RESULTS:0 = 엔진
ENDSELECT


@CARD_300007(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN システム
ENDSELECT

RETURN 0

@CARD_EXPLANATION_300007(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(2,효과後強制誘発)
PRINTL ①：１턴에 ２번까지、자신이 몬스터 효과를 발동할 때마다 스피드 카운터를 １개 늘린다。



@CARDCAN_300007(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,300007)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과後強制誘発
	SIF 발동済_2(듀얼리스트,300007)
		RETURN 0
	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과발동")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_300007(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択肢

IF 종류 == 효과後強制誘発
	CALL SET_PLAYER_INFO(듀얼리스트,"SpＣ",1,0,1)
	IF 발동済_1(듀얼리스트,300007)
		CALL CARD_EFFECT_USE(듀얼리스트,300007,2)
	ELSE
		CALL CARD_EFFECT_USE(듀얼리스트,300007,1)
	ENDIF
ENDIF
