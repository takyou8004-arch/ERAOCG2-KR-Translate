@CARDNAME_55530,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 록 오브 뱅키셔
	CASE "略称"
RESULTS = 투신의 허상
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_55530(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 원소정령족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
ENDSELECT

RETURN 0

@CARDSUMMON_55530(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要링크数 = 1
素材条件カテゴリ = 뱅키시 소울

IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,55530) 
	RETURN RESULT
ENDIF


@CARD_EXPLANATION_55530(종류)
#DIM DYNAMIC 종류

PRINTL 「ＶＳ」 몬스터 1체
PRINTL 이 카드명의 ②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,バトル開始時強制)
PRINTL ①：자신 필드에 「ＶＳ」 몬스터가 존재할 경우, 상대 배틀 페이즈 개시시에 발동한다.
PRINTL 자신 필드의 공격력이 가장 높은 몬스터 1체를 고르고, 이 턴 그 몬스터는 공격 유도를 얻는다.
CALLFORM CARD_ABLE(1,필드起動,フリー誘発)
PRINTL ②：자신 또는 상대의 메인 페이즈에, 이하의 효과 중에서 1개를 선택하여 발동할 수 있다.
PRINTL ●손패에서 「ＶＳ」 몬스터 1체를 특수 소환한다.
PRINTL ●자신의 묘지에서 「ＶＳ」 몬스터 1체를 고르고 손패에 더한다.


@CARDCAN_55530(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 발동불가,2

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,55530)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_1(듀얼리스트,55530)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = 뱅키시 소울
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		발동불가:0 = 1

	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 뱅키시 소울
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		발동불가:1 = 1
	RETURN 1

ELSEIF 종류 == バトル開始時強制
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 뱅키시 소울
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_55530(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성,2
#DIM DYNAMIC 攻撃力판정
#DIM DYNAMIC 攻撃誘導者

IF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,55530,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SS_CHECK(듀얼리스트,55530,"손패")
		SIF RESULT != 1
			条件달성:0 = 1
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件カテゴリ = 뱅키시 소울
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件달성:0 = 1
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 뱅키시 소울
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件달성:1 = 1
		
		IF 条件달성:0 == 1 && 条件달성:1 == 1
			IF 듀얼리스트 == "자신"
				CALL YN_CHOICE(듀얼리스트,"●손패から특수소환する","●묘지のモンスターを손패に加える")
			ELSE
				IF ターンプレイヤー == 듀얼리스트
					RESULT = 1
				ELSE
					RESULT = 0
				ENDIF
			ENDIF
		ELSEIF 条件달성:0 == 1
			RESULT = 0
		ELSEIF 条件달성:1 == 1
			RESULT = 1
		ENDIF

		IF RESULT == 0
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件カテゴリ = 뱅키시 소울
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ELSEIF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 뱅키시 소울
			효과条件종류 = モン全般
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == バトル開始時強制
PRINTL 허상에서 뱅키시 소울이 솟아나온다!
	FOR カウンタ,0,5
		IF 攻撃力판정 < GETVAR(@"%듀얼리스트%_MZ_攻:{カウンタ}")
			攻撃力판정 = GETVAR(@"%듀얼리스트%_MZ_攻:{カウンタ}")
		ENDIF
	NEXT
	CALL FULLRESET
	효과条件以攻撃力 = 攻撃力판정
	CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
	攻撃誘導者 = RESULT
	CALL SET_FIELD_INFO(듀얼리스트,"필드",攻撃誘導者,"攻誘",1,1)
ENDIF