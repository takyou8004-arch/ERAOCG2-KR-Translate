@CARDNAME_73833,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = Start for VS!
	CASE "略称"
		RESULTS = Start for VS!
	CASE "カテゴリ"
		RESULTS:0 = 뱅키시 소울
ENDSELECT


@CARD_73833(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속마법
	CASE "추가日"
		RETURN 250527
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73833(종류)
#DIM DYNAMIC 종류

PRINTL 필드는 "VS"가 된다.
PRINTL 이 카드명의 ①②③ 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,마함起動)
PRINTL ①: 필드의 "VS" 몬스터 1장을 대상으로 발동할 수 있다.
PRINTL 그 몬스터와는 속성이 다른 "VS" 몬스터 1장을 덱에서 패에 더한다.
CALL CARD_ABLE(2,身代효과)
PRINTL ②: 필드의 "VS" 몬스터가 전투 / 효과로 파괴될 경우,
PRINTL 대신에 패의 "VS" 몬스터 1장을 보여줄 수 있다.
CALL CARD_ABLE(3,ターン終了時)
PRINTL ③: 엔드 페이즈에, 필드에 "VS" 몬스터가 2장 이상 존재할 경우 발동할 수 있다.
PRINTL 덱에서 "VS" 함정 카드 1장을 필드에 세트한다.



@CARDCAN_73833(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73833)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_1(듀얼리스트,73833)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 뱅키시 소울
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 뱅키시 소울
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1

ELSEIF 종류 == 身代효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_2(듀얼리스트,73833)
		RETURN 0
	TRYCALLFORM CARDNAME_{破壊대상ID}("カテゴリ")
	SIF !MATCH(RESULTS,"뱅키시 소울")
		RETURN 0
	SIF !GROUPMATCH(破壊종류,"효과","戦闘")
		RETURN 0
	RETURN 1

ELSEIF 종류 == ターン終了時
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_3(듀얼리스트,73833)
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 뱅키시 소울
	효과条件종류 = モン全般
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 뱅키시 소울
	효과条件종류 = 함정全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73833(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 選択カード속성
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73833,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = モン全般
		효과条件カテゴリ = 뱅키시 소울
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		IF RESULT != 9999
			選択カード_ID = RESULT:99
			TRYCALLFORM CARD_{選択カード_ID},"속성"
			選択カード속성 = RESULT
			CALL FULLRESET
			효과条件カテゴリ = 뱅키시 소울
			효과条件종류 = モン全般
			CALLFORM 효과条件속성_ADV(選択カード속성,"以外")
			CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73833)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF

ELSEIF 종류 == 身代효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73833,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
효과条件カテゴリ = 뱅키시 소울
		효과条件종류 = モン全般
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT != 9999
			RETURN 1
		ELSE
			RETURN 0
		ENDIF
	ENDIF
	RETURN 0

ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73833,3)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 뱅키시 소울
		효과条件종류 = 함정全般
		CALL CARD_SET_BASIC(듀얼리스트,"덱",ゾーン,場所,"裏側표시")
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트,"ＶＳ")
ENDIF
