@CARDNAME_81320,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = S－Force 체이스
	CASE "略称"
		RESULTS = SF 체이스
	CASE "カテゴリ"
		RESULTS:0 = S－Force
ENDSELECT


@CARD_81320(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_81320(종류)
