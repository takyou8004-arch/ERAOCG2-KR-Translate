@CARDNAME_58269,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = No.69 갓 메달리온 코트 오브 암즈－갓 레이지
	CASE "略称"
		RESULTS = #69 CoA G R
	CASE "カテゴリ"
		RESULTS:0 = 넘버즈
		RESULTS:1 = 문장수
ENDSELECT


@CARD_58269(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	;효과モン/通常モン/의식/융합/싱크로/엑시즈/링크/초차원
	CASE "종류"
		RETURN 엑시즈
	;어둠속성/빛속성/땅속성/물속성/화염속성/바람속성
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 3100
	CASE "守備力"
		RETURN 1900
	CASE "추가日"
		RETURN 250713
ENDSELECT
RETURN 0


