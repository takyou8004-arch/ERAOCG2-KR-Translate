@CARDNAME_58093,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = No.93 유토피아 카이저
	CASE "略称"
		RESULTS = #93 유토카이저
	CASE "カテゴリ"
		RESULTS:0 = 넘버즈
		RESULTS:1 = 유토피아
ENDSELECT


@CARD_58093(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 12
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 2000
	CASE "추가日"
		RETURN 250712
ENDSELECT

RETURN 0

