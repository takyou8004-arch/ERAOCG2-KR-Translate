@CARDCAN_58093(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,58093)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件カテゴリ = 넘버즈
	효과条件以攻撃力 = 3000
	CALLFORM 효과条件レベル_ADV(9,"以下")
	효과条件特不無視 = 1
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 넘버즈
	효과条件종류 = 엑시즈
	효과条件표시 = 表側표시
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_58093(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カードレベル,10
#DIM DYNAMIC リピート횟수

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,58093,10)
	IF RESULT == 0
		リピート횟수 = GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") + 1
		REPEAT リピート횟수
			CALL FULLRESET
			효과範囲EX덱 = 1
			효과条件カテゴリ = 넘버즈
			효과条件以攻撃力 = 3000
			효과条件レベル:0 = 0
			FOR カウンタ,1,10
				SIF 選択カードレベル:カウンタ != 1
					효과条件レベル:カウンタ = カウンタ
			NEXT
			효과추가종류 = 効追無効
			효과条件任意 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			SIF RESULT != 1
				BREAK
			TRYCALLFORM CARD_{RESULT:1},"レベル"
			選択カードレベル:RESULT = 1
		REND

		CALL SET_FIELD_INFO(듀얼리스트,ゾーン,場所,"Ｘ",-1)
		CALL SET_PLAYER_INFO(듀얼리스트,"ダメージ半化",1,1)
		CALL SET_PLAYER_INFO(듀얼리스트,"SS阻害状態",1)
	ENDIF

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"破耐")
ENDIF
