@CARDNAME_91060,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 리브로만서 인터페어
	CASE "略称"
RESULTS = 인터페어
	CASE "カテゴリ"
RESULTS:0 = 리브로만서
ENDSELECT


@CARD_91060(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_91060(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드명의 카드는 1턴에 1장밖에 발동할 수 없다.
CALL CARD_ABLE(1,발동誘発)
PRINTL ①: 상대가 마법/함정 몬스터의 효과를 발동했을 때 발동할 수 있다.
PRINTL 자신 필드의 「리브로만서」 몬스터 1장을 주인의 손패로 되돌리고, 그 발동한 효과를 무효로 한다.
PRINTL 그 후, 자신 손패/묘지에서 「리브로만서」 몬스터 1장을 선택하여 특수 소환할 수 있다.
PRINTL 

@CARDCAN_91060(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,91060)
SIF RESULT == 1
	RETURN 0
IF 종류 == 발동誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_1(듀얼리스트,91060)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
    효과条件カテゴリ = 리브로맨서
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_91060(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 묘지送カード,5

#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91060,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件カテゴリ = 리브로맨서
		CALL CARD_ADD_BASIC(듀얼리스트,"필드",ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効")
		
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲묘지 = 1
		효과条件종류 = モン全般
		효과条件カテゴリ = 리브로맨서
		효과条件任意 = 1
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,場所)
		RETURN 1
	ENDIF
	RETURN 0
ENDIF
