@CARDNAME_12191,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = K9-666호“Jacks”
	CASE "略称"
		RESULTS = K9“Jacks”
	CASE "カテゴリ"
		RESULTS:0 = K9
ENDSELECT


@CARD_12191(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 악마족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 2600
	CASE "守備力"
		RETURN 2500
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250726
	CASE "連動ID"
		RESULT:1 = 2192
		RETURN 2191
ENDSELECT
RETURN 0


