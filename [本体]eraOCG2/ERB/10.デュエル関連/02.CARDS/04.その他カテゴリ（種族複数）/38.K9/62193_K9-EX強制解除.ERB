@CARDNAME_62193,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = K9-EX 포스 레버레이션
	CASE "略称"
		RESULTS = EX 강제해제
	CASE "カテゴリ"
		RESULTS:0 = K9
ENDSELECT


@CARD_62193(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 速攻마법
	CASE "추가日"
		RETURN 250322
ENDSELECT

RETURN 0

@CARD_EXPLANATION_62193(종류)
