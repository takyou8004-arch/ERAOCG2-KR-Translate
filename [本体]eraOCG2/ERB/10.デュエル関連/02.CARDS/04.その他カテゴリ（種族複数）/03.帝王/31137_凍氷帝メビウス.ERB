@CARDNAME_31137,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 차가운 얼음 제왕 뫼비우스
	CASE "略称"
		RESULTS = 동빙제 뫼비우스
	CASE "カテゴリ"
		RESULTS:0 = 제
ENDSELECT


@CARD_31137(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 원소정령족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 2800
	CASE "守備力"
		RETURN 1000
	CASE "성별"
		RETURN 雄性
ENDSELECT
RETURN 0

@CARD_EXPLANATION_31137(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드는 일반 소환한 레벨 5 이상의 몬스터 1장을 릴리스하고 어드밴스 소환할 수 있다.
CALLFORM CARD_ABLE(0,소환時)
PRINTL ①: 이 카드가 소환했을 경우에 발동할 수 있다.
PRINTL 필드의 마법 / 함정 카드를 3장까지 파괴한다.
PRINTL 이 카드가 물속성 몬스터를 릴리스하고 어드밴스 소환에 성공했을 경우, 그 때의 효과에 이하의 효과를 추가한다.
PRINTL ● 이 효과의 발동은 무효화되지 않는다.

@CARDSUMMON_AA_31137


@CARDCAN_31137(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,31137)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과소환
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件召種 = 召種通常
	CALLFORM 효과条件レベル_ADV(5,"以上")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 소환時
	SIF 마함数(듀얼리스트) < 1 && 마함数(対面者) < 1
		RETURN 0
	SIF 듀얼리스트 != "자신" && 마함数(対面者) < 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_31137(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 選択カードレベル
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 효과소환
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件召種 = 召種通常
	CALLFORM 효과条件レベル_ADV(5,"以上")
	CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 소환時
	CALL FULLRESET
	효과条件속성 = 물속성
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"リリース")
	SIF RESULT == 1
		효과無効不能 = 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,31137)
	IF RESULT == 0
		CALL FULLRESET
		효과条件複数 = 97
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"마함単体",場所,"묘지")
	ENDIF
ENDIF
