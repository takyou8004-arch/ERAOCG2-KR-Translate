@CARDNAME_78004,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = O－오버 소울
	CASE "略称"
RESULTS = O－소울
	CASE "カテゴリ"
RESULTS:0 = HERO
RESULTS:1 = E•HERO
ENDSELECT


@CARD_78004(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

