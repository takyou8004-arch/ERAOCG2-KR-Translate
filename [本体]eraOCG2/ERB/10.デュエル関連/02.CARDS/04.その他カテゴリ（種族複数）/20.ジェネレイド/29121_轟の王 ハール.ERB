@CARDNAME_29121,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 포효의 제너레이드 하르
	CASE "略称"
		RESULTS = 굉왕 하르
	CASE "カテゴリ"
		RESULTS:0 = 제너레이드
ENDSELECT


@CARD_29121(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 9
	CASE "攻撃力"
		RETURN 3000
	CASE "守備力"
		RETURN 3000
	CASE "성별"
		RETURN 雄性
	CASE "単一存在"
		RETURN 1
ENDSELECT

RETURN 0

@CARD_EXPLANATION_29121(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②③의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
PRINTL ①: "포효의 제너레이드 하르"는 자신 필드에 1장밖에 앞면 표시로 존재할 수 없다.
CALLFORM CARD_ABLE(1,효과後誘発)
PRINTL ②: 상대가 통상 드로우 이외로 덱에서 카드를 패에 추가했을 경우에 발동할 수 있다.
PRINTL 상대는 자신의 패 / 필드의 몬스터 1장을 묘지로 보내야 한다.
CALLFORM CARD_ABLE(2,발동誘発)
PRINTL ③: 상대의 카드의 효과가 발동했을 때,
PRINTL 자신 필드의 "제너레이드" 몬스터 또는 마법사족 몬스터를 합계 2장 릴리스하고 발동할 수 있다.
PRINTL 그 발동을 무효로 하고 파괴한다.

@CARDCAN_29121(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성,5

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,29121)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과後誘発
	SIF 발동済_1(듀얼리스트,29121)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	SIF ドローフェイズ == 1
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"덱취득+D",1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF 발동済_2(듀얼리스트,29121)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件スーパーカテゴリ = 제너레이드
	효과条件종족 = 마법사족
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT <= 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_29121(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 選択カード名前
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29121,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲손패 = 1
		효과条件종류 = モン全般
		CALL CARD_ADD_GY_ADV(対面者,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29121,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件スーパーカテゴリ = 제너레이드
		효과条件종족 = 마법사족
		효과条件複数 = 2
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"필드",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
			RETURN 1
		ENDIF
	ENDIF
	RETURN 0
ENDIF


