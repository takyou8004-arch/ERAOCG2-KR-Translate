@CARDNAME_29120,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 왕의 그림자 로프토르
	CASE "略称"
RESULTS = 왕그림자로프토르
	CASE "カテゴリ"
RESULTS:0 = 제네레이드
ENDSELECT


@CARD_29120(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 1500
	CASE "성별"
		RETURN 雄性
	CASE "単一存在"
		RETURN 1
ENDSELECT

RETURN 0

@CARD_EXPLANATION_29120(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ③의 효과는 1턴에 1번밖에 사용할 수 없다.
PRINTL ①: "왕의 그림자 로프토르"는 자신 필드에 1체밖에 앞면 표시로 존재할 수 없다.
CALLFORM CARD_ABLE(0,攻守変動)
PRINTL ②: 자신 필드의 "제네레이드" 몬스터의 공격력 / 수비력은 상대 턴 동안 1000 상승한다.
CALLFORM CARD_ABLE(1,필드起動,フリー誘発)
PRINTL ③: 자신 / 상대의 메인 페이즈에, 자신 필드의 "제네레이드" 몬스터 1장을 릴리스하고 발동할 수 있다.
PRINTL 그 몬스터와는 카드명이 다른 레벨 9의 "제네레이드" 몬스터 1장을 덱에서 특수 소환한다.


@CARDCAN_29120(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,29120)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_1(듀얼리스트,29120)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 제네레이드
	효과条件レベル = 9
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_29120(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 選択カード名

IF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29120,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 제네레이드
		CALL CARD_CHOOSE_BASIC(듀얼리스트, "필드",ゾーン,場所)	
		選択カード = RESULT
		IF 選択カード != 9999
			TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_필드:選択カード")}, "名前"
			選択カード名 = %RESULTS%
			CALL CARD_DISPOSE(듀얼리스트,"필드",選択カード)
			
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = 제네레이드
			효과条件レベル = 9
			효과条件固名前 = %選択カード名%
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF	
ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과条件カテゴリ = 제네레이드
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,1000)
ENDIF



