@CARDNAME_2206,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 죽음의 왕 헬
	CASE "略称"
RESULTS = 사왕 헬
	CASE "カテゴリ"
RESULTS:0 = 제너레이드
ENDSELECT


@CARD_2206(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 언데드족
	CASE "レベル"
		RETURN 9
	CASE "攻撃力"
		RETURN 800
	CASE "守備力"
		RETURN 2800
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_2206(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 1턴에 1번밖에 사용할 수 없다.
PRINTL ①: '죽음의 왕 헬'은 자신 필드에 1장밖에 앞면 표시로 존재할 수 없다.
CALLFORM CARD_ABLE(1,필드起動,フリー誘発)
PRINTL ②: 자신·상대 턴에, 자신 필드의 몬스터 1장을 릴리스하고,
PRINTL 그 몬스터와는 카드명이 다른 자신의 묘지의 '제너레이드' 몬스터 또는 언데드족 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터를 수비 표시로 특수 소환한다.


@CARDCAN_2206(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,2206)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_1(듀얼리스트,2206)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件スーパーカテゴリ = 제너레이드
	효과条件종족 = 언데드족
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_2206(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 選択カード名

IF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,2206,1)
	IF RESULT == 0

		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"필드",ゾーン,場所)	
		IF RESULT == 1
			TRYCALLFORM CARDNAME_{RESULT:1}, "名前"
			選択カード名 = %RESULTS%
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件スーパーカテゴリ = 제너레이드
			효과条件종족 = 언데드족
			효과条件固名前 = %選択カード名%
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"守備표시")
		ENDIF
	ENDIF
ENDIF




