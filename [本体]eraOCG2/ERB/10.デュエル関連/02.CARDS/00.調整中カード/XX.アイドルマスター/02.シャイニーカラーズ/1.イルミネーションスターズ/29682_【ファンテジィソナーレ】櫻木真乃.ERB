@CARDCAN_329681(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,329681)
SIF RESULT == 1
	RETURN 0

IF 종류 == 攻守変動 || 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_3(듀얼리스트,329681)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン開始時
	SIF 발동済_2(듀얼리스트,329681)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,329681)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 맥시멈条件
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_필드:2") != 329680
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_329681(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,329681,3)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,EX덱行き)
		CALL FULLRESET
		CALL CARD_GENERATE_HAND(듀얼리스트,4661)
		CALL CARD_SS_RESTRICTION(듀얼리스트,"일루미네이션 스타즈")
	ENDIF
ELSEIF 종류 == ターン開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,329681,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,329681,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効")
		RETURN 1
	ENDIF
	RETURN 0
ELSEIF 종류 == 맥시멈条件
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦無")
ELSEIF 종류 == 攻守変動
	CALL FULLRESET
효과条件名前 = 【판타지 소나레】일루미네이션 스타즈
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,GETVAR(@"%듀얼리스트%_MZ_攻:場所"))
ENDIF


