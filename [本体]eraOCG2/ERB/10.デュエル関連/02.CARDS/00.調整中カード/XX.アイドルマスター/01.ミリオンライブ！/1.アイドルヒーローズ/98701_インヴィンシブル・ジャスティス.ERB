@CARD_EXPLANATION_98701(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①② 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,발동誘発)
PRINTL ①: 상대의 마법/함정 몬스터의 효과가 발동했을 경우에 발동할 수 있다.
PRINTL 제외 상태의 "I HERO" 몬스터 1장을 덱으로 되돌리고,
PRINTL 턴 종료 시까지, 자신 필드의 "I HERO" 몬스터 전부는 발동한 카드의 종류(마법/함정 몬스터)에 대한 내성을 얻는다.

@CARDCAN_98701(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ数

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,98701)
SIF RESULT == 1
	RETURN 0

IF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,98701)
		RETURN 0	
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	CALL FULLRESET
	효과範囲제외 = 1
	효과条件カテゴリ = I HERO
	효과条件종류 = モン全般
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = I HERO
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_98701(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID

#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ
#DIM DYNAMIC 발동カードID
#DIM DYNAMIC 選択효과
#DIMS DYNAMIC 獲得耐性

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,98701,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲제외 = 1
		효과条件カテゴリ = I HERO
		효과条件종류 = モン全般
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			발동カードID = 効後_設場所:(処理中番号 - 1)
			TRYCALLFORM CARD_{발동カードID},"종류"
			IF RESULT < 10
				獲得耐性 = モ耐
			ELSEIF INRANGE(RESULT,20,26)
				獲得耐性 = 魔耐
			ELSEIF INRANGE(RESULT,30,33)
				獲得耐性 = 함정耐
			ENDIF
			CALL FULLRESET
			효과条件カテゴリ = I HERO
			CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,獲得耐性,1)
		ENDIF
	ENDIF
ENDIF

