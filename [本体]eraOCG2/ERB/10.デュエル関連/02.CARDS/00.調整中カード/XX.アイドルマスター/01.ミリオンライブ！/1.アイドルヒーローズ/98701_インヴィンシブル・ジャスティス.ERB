@CARDNAME_98701,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 인빈시블 저스티스
	CASE "略称"
RESULTS = 무적의 정의
	CASE "カテゴリ"
RESULTS:0 = HERO
RESULTS:1 = I HERO
RESULTS:2 = 밀리언 라이브!
		; RESULTS:2 = THE IDOLM@STER
RESULTS:4 = MILLION LIVE!
ENDSELECT

@CARD_98701(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
ENDSELECT
RETURN 0

