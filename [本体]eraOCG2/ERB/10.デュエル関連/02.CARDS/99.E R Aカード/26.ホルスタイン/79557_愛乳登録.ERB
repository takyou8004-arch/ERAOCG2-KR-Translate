@CARDNAME_79557,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

;カード制作者：xonng
SELECTCASE 参照先
	CASE "名前"
		RESULTS = 【애유】호루스타인─프리티
	CASE "略称"
		RESULTS = 암소등록【애유】
	CASE "カテゴリ"
		RESULTS:0 = 호루스
		RESULTS:1 = 호루스타인
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_79557(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250325
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79557(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드는 １턴에 １번밖에 발동할 수 없다。
PRINTL 이 카드의 ①의 효과는 듀얼 중 １번밖에 적용할 수 없으며、①의 효과로 선택한 카드는 "암소"의 등록정보로 지속마법이 된다。
CALLFORM CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：패 / 덱에서 "호루스타인" 이외의 암컷 몬스터를 １장 선택하여、그 몬스터를 지속 마법카드 취급으로 자신의 마법＆함정 존에 앞면 표시로 둔다。
PRINTL 그 후、덱밖에서 "【애유】호루스타인" １장을 패에 넣는다、
PRINTL 자신의 덱 / 패 / 몬스터 존 / 묘지에 존재하는 모든 같은 이름의 카드를 "【미유】호루스타인" 으로 재정의한다。
PRINTL 이 효과의 처리 후、이하의 효과에서 선택한다。
PRINTL ●패에서 "【애유】호루스타인" １장을 특수 소환한다。
PRINTL ●덱에서 "호루스타인" 일반마법을 １장 골라 패에 넣는다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79557(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

;戦闘終了플래그を拾うため
IF 종류 == 永攻紋章起動 && (자신_듀얼패배플래그 > 0 || 상대_듀얼패배플래그 > 0)
	RETURN 1
ENDIF

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79557)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_D1(듀얼리스트,79557) 
		RETURN 0
	SIF 발동済_1(듀얼리스트,79557) 
		RETURN 0
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과条件성별 = 牝性
	효과条件固カテゴリ = 호루스타인
	
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 永紋章起動
	RETURN 1
ELSEIF 종류 == 永攻紋章起動
	RETURN 1
ENDIF

@CARDEFFECT_79557(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 参照カードID
#DIM DYNAMIC 書換先カードID
#DIM DYNAMIC 所有者
#DIM DYNAMIC 参照場所
#DIM DYNAMIC 암소대상
#DIM DYNAMIC 암소所有
#DIMS DYNAMIC 암소名称
#DIMS DYNAMIC 암소略称
#DIM DYNAMIC 암소속성
#DIM DYNAMIC 암소レベル
#DIM DYNAMIC 암소攻撃力
#DIM DYNAMIC 암소守備力
#DIM DYNAMIC 암소튜너
#DIM DYNAMIC 암소乳サイズ
#DIM DYNAMIC 암소ミルクml
#DIM DYNAMIC 암소ミルク売却額
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 貧乳可否
#DIMS DYNAMIC カテゴリ名称

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

書換先カードID = 179557
カテゴリ名称 = 登録情報：호루스타인─애유

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79557,1)
	IF RESULT == 0 && 마함数(듀얼리스트) < 5
		
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과条件성별 = 牝性
		효과条件固カテゴリ = 호루스타인
		효과추가カテ = %カテゴリ名称%
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시")
		
		選択カードID = RESULT:1
		
		IF RESULT != 0
			;마함に置く(デュエル中１번)
			CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79557,20)
			
			;호루스타인情報の취득(カードIDから各種情報を취득)
			CALL HORUSTEIN_GET_DATA(듀얼리스트,-1,書換先カードID)
			암소名称 = %RESULTS:0%
			암소略称 = %RESULTS:1%
			参照場所 = RESULT:0
			암소대상 = RESULT:1
			参照カードID = RESULT:99
			CALL HORUSTEIN_GET_BUST(参照カードID)
			암소乳サイズ = RESULT:2
			
			;耐性と발동無効の付与
			CALL SET_FIELD_INFO(듀얼리스트,"마함",参照場所,"破耐",2)
			CALL SET_FIELD_INFO(듀얼리스트,"마함",参照場所,"対耐",2)
			CALL SET_FIELD_INFO(듀얼리스트,"마함",参照場所,"発無",2)
			
			; 카드의 書き換え
			;필드의 카드
			FOR カウンタ,0,5
				SIF GETVAR(@"%듀얼리스트%_MZ_ID:カウンタ") != 選択カードID
					CONTINUE
				CALL CARD_RECONSTRUCT(듀얼리스트,"필드",カウンタ,書換先カードID)
			NEXT
			
			;패의 カード
			FOR カウンタ,0,現손패(듀얼리스트)
				SIF GETVAR(@"%듀얼리스트%_손패:カウンタ") != 選択カードID
					CONTINUE
				CALL CARD_RECONSTRUCT(듀얼리스트,"손패",カウンタ,書換先カードID)
			NEXT
			
			;덱のカード
			FOR カウンタ,0,現山札数(듀얼리스트)
				SIF GETVAR(@"%듀얼리스트%_덱:カウンタ") != 選択カードID
					CONTINUE
				CALL CARD_RECONSTRUCT(듀얼리스트,"덱",カウンタ,書換先カードID)
			NEXT
			
			;묘지의 카드
			FOR カウンタ,0,現묘지数(듀얼리스트)
				SIF GETVAR(@"%듀얼리스트%_묘지:カウンタ") != 選択カードID
					CONTINUE
				CALL CARD_RECONSTRUCT(듀얼리스트,"묘지",カウンタ,書換先カードID)
			NEXT
			
			PRINTFORMW 등록증에 이름이 새겨지니、어디선가 호루스의 수호자가 나타났다！
			PRINTFORML %암소名称%는 저항할 새도없이 세명의 수호자에게 밀려져、카우벨이 붙은 목걸이가 채워져 버렸다…
			PRINTFORML 종자중 한명이 카우벨을 손가락으로 튕기니、차라랑…하고 %암소名称%의 머리에 울려퍼져간다…
			PRINTFORML 
			PRINTFORML "…음머…음머-…？"
			PRINTFORMW 
			PRINTFORML %암소名称%는 넋이 나갓것같은 머리를 일으키는듯이、필사적으로 말을 꺼내려 하지만、그 목소리는 암소의 울음소리로 바뀌어버렸다。
			PRINTFORMW 곤란해하는 %암소名称%를 무시하고、수호자는 그 튼튼한 손으로 가슴을 정중하게 암소에게 상응하는 "상품"으로 "조교"해간다…
			
			IF 암소乳サイズ >= 0
				
				SETCOLOR C_GRAY
				PRINTFORML 수호자중 한명이 이쪽을 보고있다… 
				PRINTFORML
				RESETCOLOR
				CALL YN_CHOICE(듀얼리스트,"「빈유」로 만든다","가슴의 사이즈를 변경하지 않는다")
				
				貧乳可否 = RESULT
			ENDIF
			
			SETCOLOR C_PINK
			PRINTFORML 
			PRINTFORML "음머-…""으、음머-…❤"
			PRINTFORMW 
			RESETCOLOR
			PRINTFORML …조교가 끝난것 같다。
			PRINTFORML 아까까지와 상태가 바뀌어、목소리를 낼때마다 열을 띤 암소의 울음소리로 바뀌어간다…
			PRINTFORML 
			IF 암소乳サイズ <= -1 || 貧乳可否 == 1
				;元々貧乳未満の경우、変わらなかった…
				
				PRINTFORM %암소名称%의 가슴은
				IF 암소乳サイズ <= -1
					SETCOLOR C_L_BLUE
				ELSE
					SETCOLOR C_PINK
				ENDIF
				PRINTFORM  [%GET_TALENTNAME(121,암소乳サイズ)%] 
				RESETCOLOR
				PRINTFORM 에서……딱히 변동은 없던것같다……
				PRINTFORMW
				
				;着替え
				PRINTFORML %암소名称%는 밀어붙이던 수호자가 어느샌가 사라진것을 확인하니、
				PRINTFORMW 달궈진 몸이 뜨거운것인지、옷을 벗기 시작했다…
			ELSE
				;貧乳より大きい경우、貧乳にする
				PRINTFORM %암소名称%의 가슴은
				SETCOLOR C_PINK
				PRINTFORM  [%GET_TALENTNAME(121,암소乳サイズ)%] 
				RESETCOLOR
				PRINTFORM 에서
				SETCOLOR C_L_BLUE
				PRINTFORM  [%GET_TALENTNAME(121,-1)%] 
				RESETCOLOR
				PRINTFORML 가 되어버렸다…
				PRINTFORMW
				
				;着替え
				PRINTFORML %암소名称%는 밀어붙이던 수호자에게서 해방되었지만
				PRINTFORMW 살짝 몸을 움직인것만으로、사이즈에 맞지 않게된 옷이 몸에서 훌렁 떨어져버렸다…
				
				;元々が小柄체형じゃなければ표시(実際には변경しない)
				IF 암소대상 != -1 && TALENT:암소대상:100 != -1
					PRINTFORML
					PRINTFORMW %암소名称%는 [작은체형]이 되어버린것같다…
				ENDIF
			ENDIF
			
			PRINTFORML 
			PRINTFORML %암소名称%는 발밑에 준비되어있던 옷을 주워、그것이 당연하다는듯이 소모양 마이크로 비키니로 갈아입기 시작했다…
			
			;服装の변경(牛柄ビキニと角)
			IF 암소대상 != -1 
				;악세사리&모자：つけ角 小(2枠目を변경)
				EQUIP:암소대상:2 = 20
				CFLAG:암소대상:(200 + 2) = 20
				;신발、양말は触らない
				;바지～이너までは全剥き
				FOR カウンタ,6,13
					EQUIP:암소대상:カウンタ = 0
					CFLAG:암소대상:(200 + カウンタ) = 0
				NEXT
				;원피스：牛柄ビキニ(マイクロビキニは無かった)
				EQUIP:암소대상:15 = 33
				CFLAG:암소대상:(200 + 15) = 33
				
				;조교
				CALL EROTIC_MOVE("バスト조교",암소대상,-1,300)
			ENDIF
			
			;書き換えた 카드를 패에 넣는다。
			CALL CARD_GENERATE_HAND(듀얼리스트,書換先カードID)
			
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = 호루스타인
			효과条件종류 = 通常마법
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			
			IF RESULT == 1
				;덱에 ある場合
				CALL YN_CHOICE(듀얼리스트,"「호루스타인」通常마법 카드를 패에 넣는다","「" + 암소名称 + "」を패에서 특수 소환한다")
			ELSE
				;덱에 존재하고ない경우、選択肢は표시しない
				PRINTFORML
				PRINTFORMW "호루스타인" 통상 마법카드가 덱에 존재하지 않기 때문에、"%암소名称%"를 패에서 특수 소환한다…가 선택됩니다。
				RESULT = 1
			ENDIF
			
			IF RESULT == 0
				;덱에서 "호루스타인"通常마법を패에 넣는다
				CALL FULLRESET
				효과範囲덱 = 1
				효과条件カテゴリ = 호루스타인
				효과条件종류 = 通常마법
				CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
			ELSE
				;패에서 특수소환
				CALL FULLRESET
				;選択時に【〇乳】호루스타인…と표시されるとｷﾞｮｯっとするので、패에서 探して특수 소환한다
				FOR カウンタ,0,現손패(듀얼리스트)
					SIF GETVAR(@"%듀얼리스트%_손패:カウンタ") != 書換先カードID
						CONTINUE
					CALL CARD_SP_SUMMON(듀얼리스트,"손패",カウンタ)
					BREAK
				NEXT
				IF RESULT != 0
					PRINTFORMW %암소名称%의 귀여운 유두가 탱글하고 흔들렸다…
					CALL HORUSTEIN_RAND_COW_TXT(1,1)
				ENDIF
				
			ENDIF
			
			PRINTFORML 
			CALL FULLRESET
			효과範囲마함 = 1
			효과範囲손패 = 1
			효과条件ID = 79555
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT == 1
				;패 / 필드에 호루스타인ファ-ムが존재하는 場合
				SETCOLOR C_PINK
				PRINTFORML %암소名称%의 귀여운 유두를 탱글하고 흔들며、아무말도 하지 않고 가슴을 이쪽으로 내어왔다…
				PRINTFORML %암소名称%는 짜여질 준비가 되어있는것 같다…
				PRINTFORMW "……음멋❤ 머어-…❤"
				PRINTFORML 
				RESETCOLOR
			ENDIF
			
			PRINTFORML 
			;永紋章の표시をスキップ
			SKIPDISP 1
			CALL CARD_RESERV(듀얼리스트,79557,永紋章予約)
			CALL CARD_RESERV(듀얼리스트,79557,永攻紋章予約)
			SKIPDISP 0
			SETCOLOR C_YELLOW
			PRINTFORML "【애유】호루스타인"은 필드에 존재하는 경우 "%암소名称%"로 취급됩니다。
			RESETCOLOR
			PRINTFORMW
			
			IF 암소乳サイズ <= -1 || 貧乳可否 == 1
				;貧乳未満なら貧乳のまま
				암소乳サイズ = 0
			ELSE
				;貧乳にする
				암소乳サイズ = -(암소乳サイズ)-1
			ENDIF
			;애유は카운터を貧乳になるように調整してスタ-ト(乳サイズ：DOWN)
			CALL SET_FIELD_INFO(듀얼리스트,"마함",参照場所,"個Ｃ",암소乳サイズ)
			
		ELSE
			PRINTFORMW 마법＆함정 존에 카드를 두지 못했기 때문에、등록할수 없었습니다…
		ENDIF
		
	ENDIF
	
	SKIPDISP 1
	CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
	SKIPDISP 0
	
ELSEIF 종류 == 永紋章起動 || 종류 == 永攻紋章起動
	;紋章を使用し、패 / 필드의 암소の情報を書き換え
	CALL FULLRESET
	CALL HORUSTEIN_GET_DATA(듀얼리스트,,179557)
	암소名称 = %RESULTS:0%
	암소略称 = %RESULTS:1%
	参照場所 = RESULT:0
	암소대상 = RESULT:1
	암소所有 = RESULT:2
	암소ミルクml = RESULT:4		;착유量(듀얼中総量)
	参照カードID = RESULT:99
	암소乳サイズ = GETVAR(@"%듀얼리스트%_魔_個Ｃ:参照場所")
	
	;参照先카드가 吹っ飛んでたら処理しない
	IF 参照場所 != -1
		
		IF 듀얼리스트 == "자신"
			所有者 = 1
		ELSEIF 듀얼리스트 == "상대"
			所有者 = 2
		ENDIF
		
		;속성
		TRYCALLFORM CARD_{参照カードID},"속성"
		암소속성 = RESULT
		
		;레벨의 취득
		TRYCALLFORM CARD_{参照カードID},"レベル"
		암소レベル = RESULT
		
		;攻守
		TRYCALLFORM CARD_{参照カードID},"攻撃力"
		암소攻撃力 = RESULT
		TRYCALLFORM CARD_{参照カードID},"守備力"
		암소守備力 = RESULT

		;애유の攻守は0に近いほど高くなる
		IF 암소攻撃力 >= 3000
			암소攻撃力 = 0
		ELSE 
			암소攻撃力 = 3000 - 암소攻撃力
		ENDIF

		IF 암소守備力 >= 3000
			암소守備力 = 0
		ELSE 
			암소守備力 = 3000 - 암소守備力
		ENDIF

		;튜너
		TRYCALLFORM CARD_{参照カードID},"튜너"
		암소튜너 = RESULT
		
		;패의  카드의 レベル변경(손패は설정分Lvが下がる)
		IF 종류 == 永攻紋章起動
			CALL FULLRESET
			효과条件ID = 書換先カードID
			CALL STATCHANGE_HAND(듀얼리스트,"全体",場所,"永星",12 - 암소レベル)
		ENDIF
		
		;자신 필드의 카드で、所有者が자신
		FOR カウンタ,0,5
			SIF GETVAR(@"%듀얼리스트%_MZ_ID:カウンタ") != 書換先カードID
				CONTINUE
			SIF GETVAR(@"%듀얼리스트%_MZ_所有:カウンタ") != 所有者
				CONTINUE

			IF 종류 == 永紋章起動 
				CALL CARD_SET_RENAME(듀얼리스트,"필드",カウンタ,암소略称)
				CALL STATCHANGE_EFFECT(듀얼리스트,"자신",カウンタ,"変化元ID",参照カードID)
			ELSEIF 종류 == 永攻紋章起動 
				
				CALL STATCHANGE_OTHER(듀얼리스트,"자신",カウンタ,"属",암소속성)
				CALL STATCHANGE_EFFECT(듀얼리스트,"자신",カウンタ,"永星",암소レベル)
				CALL STATCHANGE_BASIC(듀얼리스트,"자신",カウンタ,암소攻撃力,암소守備力)
				CALL CARD_SET_CATEGORY(듀얼리스트,"필드",カウンタ,"E.R.A")
				;カテゴリ2は未설정なら변경する
				SIF GETVARS(@"%듀얼리스트%_MZ_追カテ2:カウンタ") == ""
					CALL CARD_SET_CATEGORY(듀얼리스트,"필드",カウンタ,"호루스타인",1)
				SIF 암소튜너 == 1
					CALL STATCHANGE_EFFECT(듀얼리스트,"자신",カウンタ,"一Ｔ",2)
				
				;지속마법扱いの자신に乗ってる카운터의 수 만큼 乳を盛る
				CALL STATCHANGE_OTHER(듀얼리스트,"자신",カウンタ,"가슴追",암소乳サイズ)
			ENDIF
		NEXT
		
		;상대 필드의 카드で、所有者が자신
		FOR カウンタ,0,5
			SIF GETVAR(@"%対面者%_MZ_ID:カウンタ") != 書換先カードID
				CONTINUE
			SIF GETVAR(@"%対面者%_MZ_所有:カウンタ") != 所有者
				CONTINUE

			IF 종류 == 永紋章起動 
				CALL CARD_SET_RENAME(対面者,"필드",カウンタ,암소略称)
				CALL STATCHANGE_EFFECT(対面者,"자신",カウンタ,"変化元ID",参照カードID)
			ELSEIF 종류 == 永攻紋章起動 
				
				CALL STATCHANGE_OTHER(対面者,"자신",カウンタ,"属",암소속성)
				CALL STATCHANGE_EFFECT(対面者,"자신",カウンタ,"永星",암소レベル)
				CALL STATCHANGE_BASIC(対面者,"자신",カウンタ,암소攻撃力,암소守備力)
				CALL CARD_SET_CATEGORY(対面者,"필드",カウンタ,"E.R.A")
				;カテゴリ2は未설정なら변경する
				SIF GETVARS(@"%対面者%_MZ_追カテ2:カウンタ") == ""
					CALL CARD_SET_CATEGORY(対面者,"필드",カウンタ,"호루스타인",1)
				
				SIF 암소튜너 == 1
					CALL STATCHANGE_EFFECT(対面者,"자신",カウンタ,"一Ｔ",2)
				
				CALL STATCHANGE_OTHER(対面者,"자신",カウンタ,"가슴追",암소乳サイズ)
				
			ENDIF
		NEXT
	ENDIF
	IF (자신_듀얼패배플래그 > 0 || 상대_듀얼패배플래그 > 0) && 암소ミルクml > 0
	
		;基本は마함見るけど、자신 필드에 존재하는 경우、更に別의 효과で乳がデカくなってるかチェック
		FOR カウンタ,0,5
			SIF GETVAR(@"%듀얼리스트%_MZ_ID:カウンタ") != 書換先カードID
				CONTINUE
			SIF GETVAR(@"%듀얼리스트%_MZ_가슴追:カウンタ") > 암소乳サイズ
				암소乳サイズ = GETVAR(@"%듀얼리스트%_MZ_가슴追:カウンタ")
		NEXT
		
		CALL HORUSTEIN_GET_BUST(参照カードID,암소乳サイズ)
		암소乳サイズ = RESULT:2
		암소ミルク売却額 = 암소ミルクml *10
		
		CUSTOMDRAWLINE —
		PRINTFORML
		IF 암소乳サイズ <= -1
			;貧乳の乳は高く売れる
			TIMES 암소ミルク売却額 , 2.5
			
			SETCOLOR C_L_BLUE
			PRINTFORM [%GET_TALENTNAME(121,암소乳サイズ)%] 
			RESETCOLOR
			PRINTFORML 의 젖은 높게 판매되는듯 하다…
		ELSEIF 암소乳サイズ >= 5
			;規格外な
			TIMES 암소ミルク売却額 , 0.15
		
			SETCOLOR C_H_PINK
			PRINTFORM [%GET_TALENTNAME(121,암소乳サイズ)%] 
			RESETCOLOR
			PRINTFORML 의 젖은 판매가 어려운듯 하다…
		ELSE
			;それ以外なら普通の価格に
			TIMES 암소ミルク売却額 , 0.8
		
			SETCOLOR C_PINK
			PRINTFORML [%GET_TALENTNAME(121,암소乳サイズ)%] 
			RESETCOLOR
		ENDIF
		
		PRINTFORML %암소名称%에게서 뽑은{암소ミルクml}ml의 우유를{암소ミルク売却額}엔에 판매했다…
		PRINTFORML
		CUSTOMDRAWLINE —
		
		CALL MONEY_GET(암소ミルク売却額)


		;リセットも兼ねて、ミルクの情報を削除
		CALL SET_FIELD_INFO(듀얼리스트,"마함",参照場所,"T1汎数",0,上書きモード)
		CALL SET_FIELD_INFO(듀얼리스트,"마함",参照場所,"汎数",0,上書きモード)
	ENDIF
ENDIF


