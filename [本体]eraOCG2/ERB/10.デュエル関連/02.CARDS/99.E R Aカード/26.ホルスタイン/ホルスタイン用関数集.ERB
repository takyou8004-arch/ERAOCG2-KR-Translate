CUSTOMDRAWLINE —
	RESETCOLOR

IF 암소대상 == -1 || 画像표시 == 2
	;CSV無
	TRYCALLFORM CARDNAME_{参照カードID},"名前"
	PRINTFORM 【%RESULTS%】
	SETCOLOR C_PINK
	PRINTFORML （%"♀",2%）
	RESETCOLOR

	PRINTL ————————————————————————————————————————————————————————————————————————————————————————————————

