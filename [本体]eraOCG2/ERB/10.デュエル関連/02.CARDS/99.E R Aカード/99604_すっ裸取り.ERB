@CARDCAN_99604(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99604)
SIF RESULT == 1
	RETURN 0

IF 종류 == 발동誘発
	IF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0		
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0	
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件종류 = モン全般
		CALL CARD_CHECK_START_DATA(듀얼리스트,1)
		SIF RESULT != 1
			RETURN 0
	
	ELSEIF ゾーン == "손패"  
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件종류 = モン全般
		효과条件성별 = 牝性
		CALL CARD_CHECK_START_DATA(듀얼리스트,1)
		SIF RESULT != 1
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	RETURN 1
ENDIF

@CARDEFFECT_99604(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 설정場所

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99604)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		TRYCALLFORM CARD_{効後_효과ID:(処理中番号 - 1)},"성별"
		IF RESULT == 牝性
			選択カード = 効後_設場所:(処理中番号 - 1) 
			CALL CARD_EROTICS_FIELD(듀얼리스트,"손패",選択カード,1)
			IF RESULT != -1
				조교대상 = RESULT
				PRINTFORMW 진공 칼날에 의해 %CALLNAME:조교대상%의 옷이 찢겨졌다.
				PRINTFORMW %CALLNAME:조교대상%는 큰 눈을 뜨고 놀란 표정을 지은 채,
				PRINTFORMW 발을 헛디뎌 이쪽 필드로 넘어져버렸다。
				CALL EROTIC_MOVE("晒しもの",조교대상)
			ENDIF
			CALL FULLRESET
			효과추가종류 = 691
			CALL CARD_SP_SUMMON(対面者,"손패",選択カード,"守備표시",1)
		ENDIF
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効")
	ENDIF
ENDIF