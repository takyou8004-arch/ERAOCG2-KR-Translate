@CARDNAME_59911,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 퍼실리즘 이스크
	CASE "略称"
		RESULTS = Pha이스크
	CASE "カテゴリ"
		RESULTS:0 = 퍼실리즘
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_59911(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250908
ENDSELECT
RETURN 0

@CARD_EXPLANATION_59911(종류)
