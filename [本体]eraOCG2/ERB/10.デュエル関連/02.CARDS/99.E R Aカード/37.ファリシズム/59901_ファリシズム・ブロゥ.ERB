	CASE "略称"
		RESULTS = Pha블로우
	CASE "カテゴリ"
		RESULTS:0 = 퍼실리즘
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_59901(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
