@CARDCAN_39553(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 装備者
#DIM DYNAMIC 装備先
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 雌犬대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39553)
SIF RESULT == 1
	RETURN 0

CALL 装備판정(듀얼리스트,場所,ゾーン)
装備者 = %RESULTS%
装備先 = RESULT

IF 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF RESULT == 1
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	SIF モンスター数(対面者) >= 5
		RETURN 0
	
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과範囲상대마함 = 1
	효과条件名前 = 아누스피어─암캐의 꼬리
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 0
		RETURN 0
	
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件성별 = 牝性
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	
	IF RESULT == 0
		효과範囲상대필드 = 0
		효과範囲상대묘지 = 1
		효과条件소환可 = 2
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	ENDIF
	
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動
	SIF 발동済_1(듀얼리스트,39553)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,39553,ゾーン)
	SIF RESULT == 1
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF 装備先 == 5
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,39553)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件성별 = 牝性
	FOR カウンタ,0,5
		;카드가 存在しない場所は見ない
		SIF GETVAR(@"%듀얼리스트%_필드:カウンタ") == -1
			CONTINUE
		;자신은 제외
		SIF カウンタ == 場所
			CONTINUE
		;裏側표시は参照しない
		SIF GETVAR(@"%듀얼리스트%_MZ_表:カウンタ") == 3
			CONTINUE
		IF GETVAR(@"%듀얼리스트%_MZ_所有:カウンタ") == 2
			효과条件ID:雌犬대상 = GETVAR(@"%듀얼리스트%_MZ_ID:カウンタ")
			雌犬대상 ++
		ENDIF
	NEXT
	
	;元々상대가 所有する카드가 ない場合
	SIF 雌犬대상 == 0
		RETURN 0
		
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	
	SIF RESULT != 1
		RETURN 0
	
	RETURN 1
ENDIF
@CARDEFFECT_39553(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID
#DIMS DYNAMIC 装備先カード名
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 조교대상
#DIMS DYNAMIC 装備者
#DIM DYNAMIC 装備先
#DIM DYNAMIC 雌犬대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL 装備판정(듀얼리스트,場所,ゾーン)
装備者 = %RESULTS%
装備先 = RESULT

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39553)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과条件성별 = 牝性
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 0
			CALL FULLRESET
			효과範囲상대묘지 = 1
			효과条件성별 = 牝性
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,,,1)
		ENDIF

		CALL FULLRESET
		효과範囲상대필드 = 1
		효과条件성별 = 牝性
		CALLFORM CARD_EQUIP_ADV(듀얼리스트,場所,ゾーン)
		選択カード = RESULT:2
		選択カードID = RESULT:1
		IF 選択カード != 9999
			TRYCALLFORM CARDNAME_{選択カードID},"略称"
			PRINTFORML %RESULTS%의 엉덩이구멍에 개의 꼬리가 잠입했다！
			PRINTFORMW 엉덩이 구멍으로의 감각과 같이"개"로서의 의식이 박아넣어져간다…
			PRINTFORML 
			;PRINTFORML あなたの手には"犬"の躾けを行うための스위치が握られている...
			;CALL CARD_GENERATE_HAND(듀얼리스트,139553)
			
			CALL CARD_EROTICS(選択カードID)
			IF RESULT != -1
				조교대상 = RESULT
				CALL EROTIC_MOVE("애널解し",조교대상,-1,300)
			ENDIF
			
		ENDIF
	ENDIF
ELSEIF 종류 == 마함起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39553,1)
	IF RESULT == 0
		CALL FULLRESET
		
		IF 装備者 == 対面者
			TRYCALLFORM CARDNAME_{GETVAR(@"%対面者%_필드:装備先")},"略称"
		ELSE
			TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_필드:装備先")},"略称"
		ENDIF
		装備先カード名 = %RESULTS%
		PRINTFORML 당신이 스위치를 누르니、큰 진동음이 울려퍼졌다！
		PRINTFORML 진동음이 멈추니、%装備先カード名%는 떨어져버린 꼬리를 사랑스러운듯이 물어、다시"개"가 되기 위해 찾아왔다…
		PRINTFORMW %装備先カード名%의 엉덩이 구멍은、무언가를 기대하는듯이 떨리고있다……
		
		
		IF 装備者 == 対面者
			CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",装備先,1)
		ELSE
			CALL CARD_EROTICS_FIELD(듀얼리스트,"필드",装備先)
		ENDIF
		IF RESULT != -1
			조교대상 = RESULT
			CALL EROTIC_MOVE("애널解し",조교대상,-1,300)
		ENDIF
		
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,"마함",場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",装備先,"強奪")
		
		
	ENDIF
ELSEIF 종류 == 지속효과
	;CALL STATCHANGE_BASIC(듀얼리스트,"装備",場所,-100000,-100000)
	CALL STATCHANGE_EFFECT(듀얼리스트,"装備",場所,"破耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"装備",場所,"戦耐")
	
	;CALL STATCHANGE_EFFECT(듀얼리스트,"装備",場所,"表固")
	;CALL STATCHANGE_EFFECT(듀얼리스트,"装備",場所,"不材")
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39553,2)
	IF RESULT == 0
		PRINTFORMW 꼬리는 암캐에 알맞는 상대를 찾아、필드를 뛰어돌아다녔다！
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件성별 = 牝性
		FOR カウンタ,0,5
			;카드가 存在しない場所は見ない
			SIF GETVAR(@"%듀얼리스트%_필드:カウンタ") == -1
				CONTINUE
			;자신은 제외
			SIF カウンタ == 場所
				CONTINUE
			;裏側표시は参照しない
			SIF GETVAR(@"%듀얼리스트%_MZ_表:カウンタ") == 3
				CONTINUE
			IF GETVAR(@"%듀얼리스트%_MZ_所有:カウンタ") == 2
				효과条件ID:雌犬대상 = GETVAR(@"%듀얼리스트%_MZ_ID:カウンタ")
				雌犬대상 ++
			ENDIF
		NEXT
		
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		
		IF RESULT != -1
			選択カード = RESULT
			選択カードID = RESULT:99
			IF 選択カード != 9999
				CALL CARD_SELECT(듀얼리스트,選択カード)
				選択カード = RESULT
				
				TRYCALLFORM CARDNAME_{選択カードID},"略称"
				装備先カード名 = %RESULTS%
				
				PRINTFORMW 꼬리는 암캐에 알맞는 상대를 찾아내니、%装備先カード名%의 엉덩이구멍에 잠입했다！
				PRINTFORMW 자신의 움직임에 맞추라고 말하는듯이 지배적인 움직임이지만、그것에 맞춰 엉덩이를 흔드는 모습은 마치 암캐라고 해도 되겠지…
				CALL CARD_EROTICS(選択カードID)
				IF RESULT != -1
					조교대상 = RESULT
					CALL EROTIC_MOVE("애널解し",조교대상,-1,300)
					装備先カード名 = %NAME:조교대상%
				ENDIF
				
				PRINTFORML 
				PRINTFORMW "훈육"받은 암캐의 꼬리를 흔드는듯이 만족한것인지、꼬리는 엉덩이구멍에서 스르륵 빠져나왔다…
				PRINTFORMW 쩍 벌어진 %装備先カード名%의 엉덩이구멍은 암캐에 걸맞는 구멍으로 만들어 바뀐듯 하다…
				CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"永星",4,上書きモード)
			ENDIF
		ENDIF
	ENDIF
ENDIF



