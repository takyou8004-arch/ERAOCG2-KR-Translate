@CARDNAME_39552,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

;カード制作者：xonng
SELECTCASE 参照先
	CASE "名前"
		RESULTS = 아누스피어─이블 테일
	CASE "略称"
		RESULTS = AnS─악마의 꼬리
	CASE "カテゴリ"
		RESULTS:0 = 아누스피어
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_39552(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 1000
	CASE "튜너"
		RETURN 1
	CASE "分割"
		RETURN 유니온
	CASE "単一存在"
		RETURN 1
ENDSELECT

RETURN 0

