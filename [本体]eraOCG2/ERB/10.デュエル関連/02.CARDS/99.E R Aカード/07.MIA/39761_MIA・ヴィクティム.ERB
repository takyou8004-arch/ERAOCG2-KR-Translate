@CARDNAME_39761,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = MIA 빅팀
	CASE "略称"
		RESULTS = MIA빅팀
	CASE "カテゴリ"
		RESULTS:0 = MIA
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_39761(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 100
	CASE "守備力"
		RETURN 100
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

