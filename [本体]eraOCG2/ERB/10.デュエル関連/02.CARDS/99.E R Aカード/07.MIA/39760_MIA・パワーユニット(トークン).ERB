@CARDNAME_39760,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = MIA 파워유닛
	CASE "略称"
RESULTS = M 파워유닛
	CASE "カテゴリ"
RESULTS:0 = MIA
RESULTS:1 = 생체 파츠
RESULTS:2 = E.R.A
ENDSELECT


@CARD_39760(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "トークン"
		RETURN 1
	CASE "속성"
		RETURN 0
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 0
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
	CASE "소환불가"
		RETURN 1
	CASE "특수불가"
		RETURN 1
ENDSELECT

RETURN 0

@CARD_EXPLANATION_39760(종류)
#DIM DYNAMIC 종류


CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,攻守変動)
PRINTL ①: 이 카드가 제외되어 있는 한, 상대 필드의 기계족/어둠 속성 몬스터의 공격력과 수비력은 100 증가한다.
CALL CARD_ABLE(2,ターン終了時強制)
PRINTL ②: 상대 엔드 페이즈에, 이 카드가 제외되어 있는 경우에 발동할 수 있다.
PRINTL 상대는 라이프를 200 회복한다.
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_39760(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39760)
SIF RESULT == 1
	RETURN 0

IF 종류 == 攻守変動
	SIF ゾーン != "제외"
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時強制
	SIF ゾーン != "제외"
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_39760(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 캐릭터番号
#DIM DYNAMIC 取込カードID
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 攻守変動
	CALL FULLRESET
	효과条件속성 = 어둠속성
	효과条件종족 = 기계족
	CALL STATCHANGE_BASIC(듀얼리스트,"상대필드",場所,100,100,39760)

ELSEIF 종류 == ターン終了時強制
	CALL FULLRESET

	조교대상 = GETVAR(@"%듀얼리스트%_除_変化元ID:場所")
	CALL CARD_EROTICS(조교대상)
	IF RESULT != -1
		조교대상 = RESULT
		PRINTFORML
		PRINTFORML 에너지 추출 장치에 삼켜진 %NAME:조교대상%는 끊임없는 절정에 미쳐가며 에너지를 흡수당하고 있다.
		CALL EROTIC_MOVE("생체파츠",조교대상,-1,200)
	ELSE
		PRINTFORML
		PRINTFORML 에너지 추출 장치에 삼켜진 여성은 끊임없는 절정에 미쳐가며 에너지를 흡수당하고 있다.
	ENDIF
	PRINTFORML
	CALL LIFE_CHANGE(対面者,"増加","효과", 200)
	PRINTFORML
ENDIF

