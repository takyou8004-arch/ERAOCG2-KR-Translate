@CARDCAN_79760(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79760)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,79760)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件속성 = 어둠속성
	효과条件종족 = 기계족
	CALL 효과条件レベル_ADV(4, "以下")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,79760)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲상대제외 = 1
	효과条件성별 = 牝性
	효과条件固ID = 39760
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ENDIF



@CARDEFFECT_79760(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 変換カード
#DIM DYNAMIC 変換カードNO
#DIMS DYNAMIC 対面者
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79760,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件속성 = 어둠속성
		효과条件종족 = 기계족
		CALL 효과条件レベル_ADV(4, "以下")
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79760,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,덱行き)
		FOR カウンタ,0,제외数(対面者)
			CALL FULLRESET
			효과条件성별 = 牝性
			효과条件固ID = 39760
			CALL CARD_FILTER_SET(対面者,"제외",カウンタ,듀얼리스트)
			SIF RESULT != 1
				CONTINUE
			変換カード = GETVAR(@"%対面者%_제외:カウンタ")
			TRYCALLFORM CARDNAME_{変換カード}("名前")
			PRINTFORML 
			PRINTFORML 붙잡힌 %RESULTS%가 검은 에너지 추출장치에 삼켜져 가공당한다……
			CALL CARD_EROTICS(変換カード)
			IF RESULT != -1
				変換カードNO = RESULT
				CALL EROTIC_MOVE("拷問処理",変換カードNO,-1,400)
			ENDIF
			CALL CARD_RECONSTRUCT(対面者,"제외",カウンタ,39760)
			CALL CHANGE_BANISH_ID(対面者,カウンタ,変換カード)

		NEXT
		PRINTFORMW
	ENDIF
ENDIF
