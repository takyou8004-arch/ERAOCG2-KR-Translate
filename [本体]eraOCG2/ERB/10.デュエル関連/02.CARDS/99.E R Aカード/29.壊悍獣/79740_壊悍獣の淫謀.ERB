@CARDNAME_79740,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 괴한수의 음모
	CASE "略称"
		RESULTS = 괴한수의 음모
	CASE "カテゴリ"
		RESULTS:0 = 괴한수
		RESULTS:1 = 괴한에 물든 경계의 날개
		RESULTS:2 = E.R.A
ENDSELECT

@CARD_79740(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250420
ENDSELECT
RETURN 0

