	実行者 = %듀얼리스트%
	参照先 = EX덱
	上級値 = 400
ELSEIF 元参照先 == "제외"
	サーチ範囲 = 紙束最大値
	実行者 = %듀얼리스트%
	参照先 = 제외
	上級値 = 500

ELSEIF 元参照先 == "상대필드"
	サーチ範囲 = 5
	実行者 = %対面者%
	参照先 = 필드
	上級値 = 2000
ELSEIF 元参照先 == "상대마함"
	サーチ範囲 = 5
	実行者 = %対面者%
	参照先 = 마함
	上級値 = 2010
ELSEIF 元参照先 == "상대손패"
	サーチ範囲 = 紙束最大値
	実行者 = %対面者%
	参照先 = 손패
	上級値 = 2100
ELSEIF 元参照先 == "상대묘지"
	サーチ範囲 = 紙束最大値
	実行者 = %対面者%
	参照先 = 묘지
	上級値 = 2200
ELSEIF 元参照先 == "상대덱"
	IF めくり数 != 0
		サーチ範囲 = めくり数
	ELSE
		サーチ範囲 = 紙束最大値
	ENDIF
	実行者 = %対面者%
	参照先 = 덱
	上級値 = 2300
ELSEIF 元参照先 == "상대EX덱"
	サーチ範囲 = 紙束最大値
	実行者 = %対面者%
	参照先 = EX덱
	上級値 = 2400
ELSEIF 元参照先 == "상대제외"
	サーチ範囲 = 紙束最大値
	実行者 = %対面者%
	参照先 = 제외
	上級値 = 2500
ELSE
	サーチ範囲 = 紙束最大値
ENDIF

SIF 上級플래그 == 0
	上級値 = 0

FOR カウンタ,0,サーチ範囲
	SIF GETVAR(@"%実行者%_%参照先%:カウンタ") == -1
		CONTINUE
	IF 효과条件固定 > -1
		SIF 효과条件固定 != カウンタ
			CONTINUE
	ENDIF
	IF 효과条件複数 > 0
		SIF MATCH(효과条件選択済, (カウンタ + 上級値))
			CONTINUE
	ENDIF
	IF 효과条件他所 > 0
		IF 元参照先 == ゾーン
			SIF カウンタ == 場所
				CONTINUE
		ENDIF
	ENDIF
	IF 듀얼리스트 != "編集"
		IF GETVAR(@"%듀얼리스트%_アザミナ呪") >= 1 && ゾーン != "특수"
			IF 元参照先 == "묘지" || 元参照先 == "제외" || 元参照先 == "필드"
				CONTINUE
			ENDIF
		ENDIF
	ENDIF
	VARSET 条件달성
	CALL CARD_S_FILTER_SET(実行者,参照先,カウンタ,듀얼리스트,チェック플래그,カードID)
	SIF RESULT == 1
		条件달성 = 1
	CALL CARD_FILTER_SET(実行者,参照先,カウンタ,듀얼리스트,チェック플래그,カードID)
	SIF RESULT == 1
		条件달성 = 1
	SIF 효과条件상대モン && 元参照先 == "상대필드"
		条件달성 = 1

	SIF 条件달성 != 1
		CONTINUE

	IF 효과条件固名前起動 == 1
		TRYCALLFORM CARDNAME_{GETVAR(@"%実行者%_%参照先%:カウンタ")}, "名前"
		효과条件固名前:カウンタD = %RESULTS%
		カウンタD ++
	ENDIF
	IF 듀얼리스트 == "자신"
		SIF チェック플래그 == 1
			SKIPDISP チェック플래그
		IF (参照先 == "필드" || 参照先 == "마함") && 上級値 >= 2000
			IF 参照先 == "필드"
				IF GETVAR(@"%実行者%_MZ_表:カウンタ") == 3
					PRINTFORML 　%参照先%[{カウンタ + 上級値,4}] == ？？？？
				ELSE
					CALL CARD_SETCOLOR(実行者,参照先,カウンタ)
					CALLFORM CARD_KANPRINT_USE_COUNTER_PLESUREBEAST(参照先,(カウンタ + 上級値),GETVAR(@"%実行者%_%参照先%:カウンタ"),,,GETVAR(@"%実行者%_MZ_悪Ｃ:カウンタ"))

				ENDIF
			ELSEIF 参照先 == "마함"		
				IF GETVAR(@"%実行者%_魔_表:カウンタ") >= 2
					PRINTFORML 　%参照先%[{カウンタ + 上級値,4}] == ？？？？
				ELSE
					CALL CARD_SETCOLOR(実行者,参照先,カウンタ)
					CALLFORM CARD_KANPRINT_USE_COUNTER_PLESUREBEAST(参照先,(カウンタ + 上級値),GETVAR(@"%実行者%_%参照先%:カウンタ"),,,GETVAR(@"%実行者%_魔_悪Ｃ:カウンタ"))
				ENDIF
			ENDIF
			RESETCOLOR
		ELSE
			CALL CARD_SETCOLOR(実行者,参照先,カウンタ)
			IF 参照先 == "필드"
				CALLFORM CARD_KANPRINT_USE_COUNTER_PLESUREBEAST(参照先,(カウンタ + 上級値),GETVAR(@"%実行者%_%参照先%:カウンタ"),,,GETVAR(@"%実行者%_MZ_悪Ｃ:カウンタ"))
			ELSEIF 参照先 == "마함"
				CALLFORM CARD_KANPRINT_USE_COUNTER_PLESUREBEAST(参照先,(カウンタ + 上級値),GETVAR(@"%実行者%_%参照先%:カウンタ"),,,GETVAR(@"%実行者%_魔_悪Ｃ:カウンタ"))
			ELSE
				CALLFORM CARD_KANPRINT_USE_COUNTER_PLESUREBEAST(参照先,(カウンタ + 上級値),GETVAR(@"%実行者%_%参照先%:カウンタ"))
			ENDIF
			RESETCOLOR
		ENDIF

	ELSEIF 듀얼리스트 == "상대"
		;ゾーン특수：上級소환の릴리스、モンスター묘지送りのためのやつ
		IF ゾーン == "특수"
			IF 参照先 == "필드"
				IF GETVAR(@"%実行者%_MZ_攻:カウンタ") < 現在最小攻撃力
					現在最小攻撃力 = RESULT
					選択カード = カウンタ + 上級値
				ENDIF
			ELSE
				CALLFORM CARD_{GETVAR(@"%実行者%_%参照先%:カウンタ")}, "攻撃力"
				IF RESULT < 現在最小攻撃力
					現在最小攻撃力 = RESULT
					選択カード = カウンタ + 上級値
				ENDIF
			ENDIF
		ELSE
			IF 参照先 == "필드"
				IF GETVAR(@"%実行者%_MZ_攻:カウンタ") > 現在最大攻撃力
					現在最大攻撃力 = RESULT
					選択カード = カウンタ + 上級値
				ENDIF
			ELSE
				CALLFORM CARD_{GETVAR(@"%実行者%_%参照先%:カウンタ")}, "攻撃力"
				IF  RESULT > 現在最大攻撃力
					現在最大攻撃力 = RESULT
					選択カード = カウンタ + 上級値
				ENDIF
			ENDIF
		ENDIF
	ENDIF

	;合計数確認系
	条件合致数 ++
	IF 参照先 == "필드"
		条件合致レベル数 += GETVAR(@"%実行者%_MZ_星:カウンタ")
		条件合致攻撃力数 += GETVAR(@"%実行者%_MZ_攻:カウンタ")
		条件合致守備力数 += GETVAR(@"%実行者%_MZ_守:カウンタ")

		条件合致Ｘ素材数 += GETVAR(@"%実行者%_MZ_Ｘ:カウンタ") + 1
		条件合致魔力Ｃ数 += GETVAR(@"%実行者%_MZ_魔Ｃ:カウンタ")
		条件合致個別Ｃ数 += GETVAR(@"%実行者%_MZ_個Ｃ:カウンタ")
		;ザポルグ追記20241029
		条件合致悪性Ｃ数 += GETVAR(@"%実行者%_MZ_悪Ｃ:カウンタ")
	ELSE
		TRYCALLFORM CARD_{GETVAR(@"%実行者%_%参照先%:カウンタ")}, "レベル"
		条件合致レベル数 += RESULT
		TRYCALLFORM CARD_{GETVAR(@"%実行者%_%参照先%:カウンタ")}, "攻撃力"
		条件合致攻撃力数 += RESULT
		TRYCALLFORM CARD_{GETVAR(@"%実行者%_%参照先%:カウンタ")}, "守備力"
		条件合致守備力数 += RESULT

		IF 参照先 == "마함"
			条件合致魔力Ｃ数 += GETVAR(@"%実行者%_魔_魔Ｃ:カウンタ")
			条件合致個別Ｃ数 += GETVAR(@"%実行者%_魔_個Ｃ:カウンタ")
			;ザポルグ追記20241029
			条件合致悪性Ｃ数 += GETVAR(@"%実行者%_魔_悪Ｃ:カウンタ")
		ENDIF
	ENDIF
	SIF めくり数 == 99
		BREAK
NEXT


IF 上級플래그 && カウンタB <= 19
	カウンタB ++
	GOTO INPUTLOOP
ENDIF
$INPUTSKIP
VARSET RESULT
RESULT:1 = 条件合致数
RESULT:2 = 条件合致レベル数
RESULT:3 = 条件合致攻撃力数
RESULT:4 = 条件合致守備力数

RESULT:8 = 条件合致魔力Ｃ数
RESULT:9 = 条件合致個別Ｃ数
RESULT:10 = 条件合致悪性Ｃ数
RESULT:11 = 条件合致Ｘ素材数
SKIPDISP 0

IF 듀얼리스트 == "자신"
	SIF チェック플래그 != 1
		CUSTOMDRAWLINE —
	RETURN 0
ELSEIF 듀얼리스트 == "상대"
	IF 選択カード != -1
		RETURN 選択カード
	ELSE
		RETURN 9999
	ENDIF
ENDIF

;――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
; 악성 카운터使用KANPRINT(ここが변경点)
@CARD_KANPRINT_USE_COUNTER_PLESUREBEAST(参照先,場所,カードID,설정플래그,裏側플래그,悪Ｃ数)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 場所
#DIM DYNAMIC カードID
#DIM DYNAMIC 설정플래그
#DIM DYNAMIC 裏側플래그
#DIMS DYNAMIC 표시名
#DIM DYNAMIC 표시星
#DIM DYNAMIC 표시属
#DIM DYNAMIC 표시種
#DIM DYNAMIC 표시攻
#DIM DYNAMIC 표시守
#DIM DYNAMIC 표시Ｔ
#DIM DYNAMIC 표시CT
#DIM DYNAMIC 표시可否
#DIM DYNAMIC 표시限界

#DIM DYNAMIC 悪Ｃ数


IF オプション_略표시 == 1
	TRYCALLFORM CARDNAME_{カードID}, "略称"
	표시名 = %RESULTS%
ELSE
	TRYCALLFORM CARDNAME_{カードID}, "名前"
	STRLENS RESULTS
	SIF RESULT >= 40
		표시限界 = 1
	TRYCALLFORM CARDNAME_{カードID}, "名前"
	SUBSTRING RESULTS, 0, 38
	표시名 = %RESULTS%
ENDIF

TRYCALLFORM CARD_{カードID}, "レベル"
표시星 = RESULT
TRYCALLFORM CARD_{カードID}, "コスト"
표시CT = RESULT
TRYCALLFORM CARD_{カードID}, "속성"
표시属 = RESULT
TRYCALLFORM CARD_{カードID}, "종족"
표시種 = RESULT
TRYCALLFORM CARD_{カードID}, "攻撃力"
표시攻 = RESULT
TRYCALLFORM CARD_{カードID}, "守備力"
표시守 = RESULT

TRYCALLFORM CARD_{カードID}, "튜너"
표시Ｔ = RESULT


SIF 표시属 >= 1
	표시可否 = 1
IF オプション_略표시 == 1
	SELECTCASE 参照先
		CASE "필드"
			PRINTFORM %"필드",6,RIGHT%
		CASE "마함"
			PRINTFORM %"마함",6,RIGHT%
		CASE "손패"
			PRINTFORM %"손패",6,RIGHT%
		CASE "묘지"
			PRINTFORM %"묘지",6,RIGHT%	
		CASE "덱"
			PRINTFORM %"덱",7,RIGHT%
		CASE "EX덱"
			PRINTFORM %"ＥＸ",6,RIGHT%
		CASE "제외"
			PRINTFORM %"제외",6,RIGHT%	
		CASE "戦闘"
			PRINTFORM %"전투",6,RIGHT%

	ENDSELECT
	IF INRANGE(설정플래그,1,30)
		PRINTFORM ({場所,4}) 
	ELSE
		PRINTFORM [{場所,4}]
	ENDIF
	IF 裏側플래그 >= 1
		PRINTFORM 　%"ＵＮＫＮОＷＮ",16,LEFT%
	ELSE
		PRINTFORM 　%표시名,16,LEFT%
	ENDIF
ELSE
	SELECTCASE 参照先
		CASE "필드"
			PRINTFORM %"필드",6,RIGHT%
		CASE "마함"
			PRINTFORM %"마함",6,RIGHT%
		CASE "손패"
			PRINTFORM %"손패",6,RIGHT%
		CASE "묘지"
			PRINTFORM %"묘지",6,RIGHT%	
		CASE "덱"
			PRINTFORM %"덱",7,RIGHT%
		CASE "EX덱"
			PRINTFORM %"ＥＸ",6,RIGHT%
		CASE "제외"
			PRINTFORM %"제외",6,RIGHT%
		CASE "戦闘"
			PRINTFORM %"전투",6,RIGHT%
	ENDSELECT
	IF INRANGE(설정플래그,1,30)
		PRINTFORM ({場所,4}) 
	ELSE
		PRINTFORM [{場所,4}] 
	ENDIF
	IF 裏側플래그 >= 1
		PRINTFORM 　%"ＵＮＫＮＯＷＮ",40,LEFT%
	ELSE
		PRINTFORM 　%표시名,40,LEFT%
	ENDIF
ENDIF

IF 표시限界 == 1
	PRINT ..
ELSE
	PRINT 　
ENDIF

IF 표시可否 == 1
	IF 裏側플래그 >= 1 && 参照先 == "필드"
		PRINTFORM /（★ ?）/（？）/？？？？？족/　（공:     ?/수:     ?）
	ELSEIF 裏側플래그 == 0 
		SIF 표시星 >= 1
			PRINTFORM /（★{표시星,2}）
		IF 표시属 >= 1
			IF 표시属 == 1
				PRINTFORM /（%"빛",2%）
			ELSEIF 표시属 == 2
				PRINTFORM /（%"어둠",1%）
			ELSEIF 표시属 == 3
				PRINTFORM /（%"땅",2%）
			ELSEIF 표시属 == 4
				PRINTFORM /（%"물",2%）
			ELSEIF 표시属 == 5
				PRINTFORM /（%"화염",1%）
			ELSEIF 표시属 == 6
				PRINTFORM /（%"바람",1%）
			ELSEIF 표시属 == 7
				PRINTFORM /（%"신",2%）
			ENDIF
		ENDIF
		IF 표시種 >= 1
			PRINTFORM /%GET_TALENTNAME(191,표시種),12,RIGHT%
		ENDIF
		PRINTFORM /　（공:{표시攻,6}/수:{표시守,6}）
		IF 표시CT >= 1
			PRINTFORM /　（코스트:{표시CT,6}）		
		ENDIF
	ENDIF
ELSE
	PRINTFORM %"",55%
ENDIF

IF 설정플래그 == 1
	PRINTFORM /（파괴내성）
ELSEIF 설정플래그 == 2
	PRINTFORM /（대상내성）
ELSEIF 설정플래그 == 3
	PRINTFORM /（몬스터 효과내성）
ELSEIF 설정플래그 == 4
	PRINTFORM /（마법효과내성）
ELSEIF 설정플래그 == 5
	PRINTFORM /（함정효과내성）
ELSEIF 설정플래그 == 6
	PRINTFORM /（공격불가）
ELSEIF 설정플래그 == 31
	PRINTFORM /(대신 파괴 장착)
ELSEIF 설정플래그 == 35
	PRINTFORM /(프리 체인)
ELSEIF 설정플래그 == 36
	PRINTFORM /(효과후 유발)
ELSEIF 설정플래그 == 37
	PRINTFORM /(발동유발)


ELSEIF INRANGE(설정플래그,100,300)
	IF 설정플래그 == 101
		PRINTFORM /(소환시효과)
	ELSEIF 설정플래그 == 102
		PRINTFORM /(특수소환시효과)
	ELSEIF 설정플래그 == 103
		PRINTFORM /(EX특수소환시효과)
	ELSEIF 설정플래그 == 110
		PRINTFORM /(필드→묘지효과)
	ELSEIF 설정플래그 == 111
		PRINTFORM /(손패→묘지효과)
	ELSEIF 설정플래그 == 112
		PRINTFORM /(덱→묘지효과)
	ELSEIF 설정플래그 == 113
		PRINTFORM /(파괴→묘지효과)
	ELSEIF 설정플래그 == 114
		PRINTFORM /(전투→묘지효과)
	ELSEIF 설정플래그 == 115 || 설정플래그 == 117
		PRINTFORM /(필드를벗어난효과)
	ELSEIF 설정플래그 == 116
		PRINTFORM /(제외되었을시효과)
	ELSEIF 설정플래그 == 100 + ターン開始時
		PRINTFORM /(턴개시시유발)
	ELSEIF 설정플래그 == 100 + バトル開始時
		PRINTFORM /(배틀개시시유발)
	ELSEIF 설정플래그 == 100 + ターン終了時
		PRINTFORM /(턴종료시유발)
	ELSEIF 설정플래그 == 100 + バトル終了時
		PRINTFORM /(배틀종료시유발)

	ELSEIF 설정플래그 == 100 + 攻撃時誘発
		PRINTFORM /(공격시유발유발)
	ELSEIF 설정플래그 == 100 + 攻撃後誘発
		PRINTFORM /(공격후유발유발)
	ELSEIF 설정플래그 == 100 + 표시변경誘発
		PRINTFORM /(표시변경유발)
	ELSEIF 설정플래그 == 100 + リバース誘発
		PRINTFORM /(리버스유발)

	ELSE
		PRINTFORM /(그외의효과)
	ENDIF
ENDIF

PRINTFORM /악성 카운터 수：{悪Ｃ数}

PRINTL