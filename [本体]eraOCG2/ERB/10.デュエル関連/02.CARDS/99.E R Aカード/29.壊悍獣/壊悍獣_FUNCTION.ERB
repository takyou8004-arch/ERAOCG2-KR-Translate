#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB

#DIM DYNAMIC 最大値
#DIM DYNAMIC 消去カード
#DIM DYNAMIC 一時보존,99
#DIM DYNAMIC 選択カード_ID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


$CHOICE_LOOP
カードID = 処理中_효과ID
IF 듀얼리스트 == "자신"
	CUSTOMDRAWLINE —
	PRINTL 【카드를 선택해 주세요】
	CUSTOMDRAWLINE —
	PRINTL 
ELSEIF 듀얼리스트 == "상대"
	CALL ENEMY_RUTIN_CARD_ADD(参照先,場所,カードID)
ENDIF

$INPUT_LOOP
IF 효과条件複数
	FOR カウンタ,0,複数カウンタ
		효과条件選択済:カウンタ = 選択カード:カウンタ 
	NEXT
ENDIF

CALL CARD_FILTER_BASIC(듀얼리스트,"上級",ゾーン,場所)
条件달성 = RESULT:1

IF 듀얼리스트 == "자신"
	IF 条件달성 != 0 && 효과条件任意 != 1
		SETCOLOR C_GRAY
		PRINTL 　(9999) - 돌아간다(선택할 수 있는 카드가 없습니다)
		RESETCOLOR
	ELSEIF 条件달성 == 0
		選択カード:複数カウンタ = -1
		GOTO INPUT_SKIP
	ELSE
		IF 효과条件複数 == 任意の数 && 複数カウンタ >= 1
			PRINTL 　[8888] - 결정
		ENDIF
		PRINTFORM 　%"[9999] - 돌아간다",41,LEFT%
	ENDIF
	PRINTL
	PRINTFORML　%"[7777] - 필드 / 패 정보 확인",95,RIGHT%
	PRINTL
	BINPUT
	IF RESULT == 7777
		CALL DIPLAY_BOARD(2)			
		GOTO CHOICE_LOOP
	ENDIF
	選択カード:複数カウンタ = RESULT
	IF 選択カード:複数カウンタ == 8888
		PRINTL 이 이상 고를 수 없습니다。
	ENDIF
ELSEIF 듀얼리스트 == "상대"
	選択カード:複数カウンタ = RESULT
	IF 選択カード:複数カウンタ != 9999
		SETCOLOR C_W_RED
		CUSTOMDRAWLINE —
		PRINTL 【선택할 수 있는 카드가 없습니다】
		CUSTOMDRAWLINE —
		RESETCOLOR
		WAIT
	ENDIF
ENDIF

IF 選択カード:複数カウンタ == 9999
	PRINTFORMW 돌아갑니다。
	RETURN 9999
ELSEIF 選択カード:複数カウンタ == 8888
	選択カード:複数カウンタ = -1
	GOTO INPUT_SKIP
ELSEIF 選択カード:複数カウンタ != 9999
	CALL CARD_SELECT(듀얼리스트,選択カード:複数カウンタ)
	選択カード_ID = GETVAR(@"%대상者（CS）%_%参照先（CS）%:{選択カード（CS）}")
	CALL SELECT_UI_SET(듀얼리스트,選択カード_ID,4)
	IF RESULT == 1
		GOTO CHOICE_LOOP
	ENDIF
ENDIF

IF 효과条件複数 != 0
	IF 효과条件複数 > (複数カウンタ + 1)
		複数カウンタ ++
		GOTO INPUT_LOOP
	ELSE
		$INPUT_SKIP
		; FOR カウンタ,0, (複数カウンタ + 1)
		; 	最大値 = -1
		; 	FOR カウンタB,0,(複数カウンタ + 1)
		; 		IF 選択カード:カウンタB >= 最大値
		; 			最大値 = 選択カード:カウンタB
		; 			一時보존:カウンタ = 選択カード:カウンタB
		; 			消去カード = カウンタB
		; 		ENDIF
		; 	NEXT
		; 	ARRAYREMOVE 選択カード, 消去カード, 1
		; NEXT
		; VARSET カウンタ
		; REPEAT (複数カウンタ + 1)
		; 	選択カード:カウンタ = 一時보존:カウンタ
		; 	カウンタ ++
		; REND

		VARSET RESULT
		FOR カウンタ,0,(複数カウンタ + 1)
			RESULT:カウンタ = 選択カード:カウンタ
		NEXT
	ENDIF
ENDIF

CALL CARD_SELECT(듀얼리스트,選択カード)
選択カード_ID = GETVAR(@"%대상者（CS）%_%参照先（CS）%:{選択カード（CS）}")
RESULT:99 = 選択カード_ID

RETURN 選択カード

