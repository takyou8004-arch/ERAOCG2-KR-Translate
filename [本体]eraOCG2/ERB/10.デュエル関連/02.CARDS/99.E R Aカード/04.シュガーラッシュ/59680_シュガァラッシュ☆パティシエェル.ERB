@CARDNAME_59680,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 슈가러쉬☆파티시이엘
	CASE "略称"
		RESULTS = 파티시이엘
	CASE "カテゴリ"
		RESULTS:0 = 슈가러쉬
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_59680(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1200
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

