@CARDNAME_79905,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

;マジック・エキスパンド
SELECTCASE 参照先
	CASE "名前"
		RESULTS = 서큐버스 매직 익스팬드
	CASE "略称"
		RESULTS = 음마도강정
	CASE "カテゴリ"
	    RESULTS:0 = 매지션 걸
		RESULTS:1 = 서큐버스 걸
		RESULTS:2 = E.R.A
ENDSELECT

@CARD_79905(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 速攻마법
	CASE "追加日"
		RETURN 250802
ENDSELECT
RETURN 0

