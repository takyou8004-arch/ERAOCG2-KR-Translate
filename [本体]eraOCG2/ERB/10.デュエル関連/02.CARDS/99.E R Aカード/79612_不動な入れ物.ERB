@CARDCAN_79612(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79612)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,79612)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_79612(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 조교대상
IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79612,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件성별 = 牝性
		CALL CARD_SET_BASIC(듀얼리스트,"상대필드",ゾーン,場所)
		CALL CARD_EROTICS(RESULT:1)
		IF RESULT != -1
			조교대상 = RESULT
			PRINTFORML %CALLNAME:조교대상%는 덜컥 하고 예술적인 표정과 금방이라도 움직일 것 같은 포즈로 피규어가 되어버렸다.
			PRINTFORML 하지만 그 아름답기 그지없는 몸은 움직일 기색 없고, 봉인된 혼은 컬렉션 중 하나가 되어버렸다.
			CALL EROTIC_MOVE("晒しもの",조교대상, -1)
		ENDIF
	ENDIF
ENDIF

