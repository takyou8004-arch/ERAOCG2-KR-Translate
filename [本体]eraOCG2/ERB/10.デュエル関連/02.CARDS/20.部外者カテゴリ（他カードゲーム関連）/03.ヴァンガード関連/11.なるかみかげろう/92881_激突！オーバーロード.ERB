@CARDNAME_92881,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 격돌! 오버로드
	CASE "略称"
RESULTS = 오바 VS 오바
	CASE "カテゴリ"
RESULTS:0 = 카게로우
RESULTS:1 = 섀도우 팰러딘
RESULTS:2 = 오버로드
RESULTS:4 = 뱅가드
ENDSELECT


@CARD_92881(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"

		RETURN 通常함정
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92881(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,발동誘発)
PRINTL ①: 자신 필드에 「오버로드」 몬스터가 앞면 표시로 존재하고,
PRINTL 상대의 마법/함정 몬스터 효과가 발동했을 때 발동할 수 있다.
PRINTL 그 발동을 무효로 하고 파괴한다.
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②: 이 카드가 묘지에 존재하는 상태에서,
PRINTL 「오버로드」 몬스터가 EX 덱에서 자신 필드에 특수 소환했을 경우에 발동할 수 있다.
PRINTL 이 카드를 자신 필드에 세트한다. 이 효과로 세트한 이 카드는, 필드에서 벗어났을 경우에 제외된다.

@CARDCAN_92881(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92881)
SIF RESULT == 1
	RETURN 0
IF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,92881)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 오버로드
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,92881)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과条件カテゴリ = 오버로드
	효과条件종류 = モン全般
	효과条件표시 = 表側표시
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"EX특수소환")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_92881(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92881,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
	ENDIF
	RETURN 0
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92881,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과추가종류 = 500
효과条件名前 = 격돌! 오버로드
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"裏側표시")
	ENDIF
ENDIF

