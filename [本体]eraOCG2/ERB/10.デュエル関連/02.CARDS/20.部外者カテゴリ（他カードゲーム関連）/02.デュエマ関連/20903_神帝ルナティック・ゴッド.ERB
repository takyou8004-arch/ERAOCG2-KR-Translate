@CARDNAME_20903,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 신제 루나틱 갓
	CASE "略称"
		RESULTS = 신제
	CASE "カテゴリ"
		RESULTS:0 = 갓
		RESULTS:1 = 신제
		RESULTS:2 = 오리진
		RESULTS:4 = DUEL MASTERS
ENDSELECT


@CARD_20903(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 1
	CASE "종류"

		RETURN 링크
	CASE "속성"
		RESULT:1 = 물속성
		RETURN 어둠속성
	CASE "종족"
		RETURN 악마족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 3000
	CASE "守備力"
		RETURN 3000
	CASE "특수불가"
		RETURN 1
	CASE "レア度"
		RETURN ULR
ENDSELECT

RETURN 0

@CARDSUMMON_20903(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
;素材条件

CALL RESETCONDITION_MAT
必要링크数 = 4
素材条件カテゴリ = 신제
素材条件固名前起動 = 1

IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,20903) 
	RETURN RESULT
ENDIF


@CARD_EXPLANATION_20903(종류)
#DIM DYNAMIC 종류

PRINTL 카드명이 다른 「신제」モンスター２장 이상
PRINTL 이 카드는 L소환でしか특수 소환할 수 없다。
PRINTL 이 카드명의 ②의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(0,EX소환時強制,특수소환時強制)
PRINTL 이 카드는 X소재를 持つことができ、X소재를 ３つ持った状態で특수소환される。
PRINTL 이 카드가 モンスターゾーンを離れる경우、かわりにX소재를 １つ取り除くことができる。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①：이 카드는 無限攻撃を持つ。
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ②：자신 메인 페이즈에 발동할 수 있다。이하의 효과에서 １개를 골라 적용한다。
PRINTL ●상대는 패를 １장 버린다。
PRINTL 그 후、자신의 묘지에서 「오리진」몬스터 １장을 패에 넣는다。
PRINTL ●자신은 １장 드로우한다。
PRINTL 그 후、상대 필드上の카드 １장을 주인의 패에 戻す。


@CARDCAN_20903(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,20903)
SIF RESULT == 1
	RETURN 0

IF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == EX소환時強制 || 종류 == 특수소환時強制
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 발동済_1(듀얼리스트,20903)
		RETURN 0
	CALL CARD_DRAW_CHECK
	RETURN 1
ELSEIF 종류 == 身代효과
	SIF ゾーン != "필드"
		RETURN 0
	SIF 破壊대상ゾーン != "필드"
		RETURN 0
	SIF 破壊대상場所 != 場所
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_20903(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"追戦",98)
ELSEIF 종류 == EX소환時強制 || 종류 == 특수소환時強制
	CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"Ｘ",3)
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,20903,1)
	IF RESULT == 0
		PRINTL 효과を選択してください
		CALL YN_CHOICE(듀얼리스트,"●ハンデス＆サルベージ。","●ドロー＆バウンス。")
		IF RESULT == 0
			CALL FULLRESET
			효과範囲손패 = 1
			CALL CARD_ADD_GY_ADV(対面者,ゾーン,場所)
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 오리진
			효과条件종류 = モン全般
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ELSEIF RESULT == 1
			CALL CARDDRAW_BASIC(듀얼리스트,1)
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"손패")
		ENDIF
	ENDIF
ELSEIF 종류 == 身代효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,20903,0,X消費1)
	IF RESULT == 0
		RETURN 1
	ENDIF
	RETURN 0
ENDIF
