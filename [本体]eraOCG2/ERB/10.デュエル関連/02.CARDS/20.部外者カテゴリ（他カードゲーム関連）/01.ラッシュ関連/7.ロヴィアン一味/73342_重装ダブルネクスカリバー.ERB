@CARDNAME_73342,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 중장 더블넥스칼리버
	CASE "略称"
		RESULTS = 더블넥스칼리버
	CASE "カテゴリ"
		RESULTS:0 = 없음
ENDSELECT


@CARD_73342(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 0
	CASE "종류"

		RETURN 装備마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73342(종류)
#DIM DYNAMIC 종류

PRINTL 자신의 필드의 사이킥족 몬스터 1체에 장착할 수 있다.
PRINTL 이 카드명의 ③의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,攻守変動)
PRINTL ①：장착 몬스터의 공격력은 600 올린다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ②：장착 몬스터는 1턴에 2번 공격할 수 있고, 그 공격은 관통한다.
CALLFORM CARD_ABLE(3,묘지起動)
PRINTL ③：이 카드가 묘지에 존재할 경우, 자신의 필드의 몬스터 (레벨 5 이상 / 사이킥족) 1장을 대상으로 하여 발동할 수 있다.
PRINTL 이 카드를 장착한다.

@CARDCAN_73342(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 装備者
#DIM DYNAMIC 装備先

CALL 装備판정(듀얼리스트,場所,ゾーン)
装備先 = RESULT	
装備者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73342)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 사이킥족
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 묘지起動
	SIF 발동済_3(듀얼리스트,73342)
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 사이킥족
	효과条件표시 = 表側표시
	CALLFORM 효과条件レベル_ADV(5,"以上")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻守変動 || 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF 装備先 == -1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73342(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者

#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIMS DYNAMIC 装備者
#DIM DYNAMIC 装備先

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL 装備판정(듀얼리스트,場所,ゾーン)
装備先 = RESULT	
装備者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73342)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종족 = 사이킥족
		CALLFORM CARD_EQUIP_ADV(듀얼리스트,場所,ゾーン)
	ENDIF

ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73342,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종족 = 사이킥족
		CALLFORM 효과条件レベル_ADV(5,"以上")
		CALLFORM CARD_EQUIP_ADV(듀얼리스트,場所,ゾーン)
	ENDIF

ELSEIF 종류 == 攻守変動
	CALL STATCHANGE_BASIC(듀얼리스트,"装備",場所,600)

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"装備",場所,"追戦",1)
	CALL STATCHANGE_EFFECT(듀얼리스트,"装備",場所,"貫通")
ENDIF

