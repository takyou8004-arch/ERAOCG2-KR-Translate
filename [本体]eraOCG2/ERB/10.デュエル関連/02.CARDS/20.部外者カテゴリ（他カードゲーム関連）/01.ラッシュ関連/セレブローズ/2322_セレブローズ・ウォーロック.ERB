@CARDNAME_2322,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 셀레브로즈 워록
	CASE "略称"
RESULTS = CR 워록
	CASE "カテゴリ"
RESULTS:0 = 셀레브로즈
ENDSELECT


@CARD_2322(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"

		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 1100
	CASE "守備力"
		RETURN 1700
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_2322(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(10,필드起動)
PRINTL ①: 자신 필드의 레벨 5 이하의 앞면 표시 몬스터를 2장까지 묘지로 보내고 발동할 수 있다.
PRINTL 자신 묘지의 레벨 7 이상 빛 속성 마법사족 몬스터 1장을 특수 소환한다.
PRINTL 이 효과로 "셀레브로즈 매지션"을 특수 소환했을 경우,
PRINTL 추가로 자신 묘지의 "셀레브로즈 위치" 1장을 특수 소환할 수 있다.


@CARDCAN_2322(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,2322)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,,"묘지",1)
	SIF RESULT == 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	CALLFORM 효과条件レベル_ADV(7,"以上")
	효과条件속성 = 빛속성
	효과条件종족 = 마법사족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_2322(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 戦闘カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,2322,10)
	IF RESULT == 0
		CALL FULLRESET
		CALLFORM 효과条件レベル_ADV(5,"以下")
		효과条件표시 = 表側표시
		효과条件複数 = 98
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"필드",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			CALLFORM 효과条件レベル_ADV(7,"以上")
			효과条件속성 = 빛속성
			효과条件종족 = 마법사족
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT:1 == 2320
				CALL FULLRESET
				효과範囲묘지 = 1
				효과条件ID = 2321
				효과条件任意 = 1
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			ENDIF
		ENDIF
	ENDIF
ENDIF
