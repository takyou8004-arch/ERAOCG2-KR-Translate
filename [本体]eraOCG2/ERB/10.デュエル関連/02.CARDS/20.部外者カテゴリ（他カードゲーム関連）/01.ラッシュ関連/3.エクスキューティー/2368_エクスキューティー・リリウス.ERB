@CARDNAME_2368,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 엑스큐티 릴리우스
	CASE "略称"
		RESULTS = 릴리우스
	CASE "カテゴリ"
		RESULTS:0 = 엑스큐티
ENDSELECT


@CARD_2368(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 0
	CASE "종류"

	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 악마족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 3000
	CASE "守備力"
		RETURN 500
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_2368(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 1턴에 1번만 발동할 수 있다.
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①: 자신 필드에 이 카드명 이외의 "엑스큐티" 몬스터가 존재할 경우에 발동할 수 있다.
PRINTL 이 카드를 패에서 특수 소환한다.
PRINTL 이 효과의 발동 후, 턴 종료 시까지 자신은 레벨 7 이상의 몬스터를 특수 소환할 수 없다.
CALLFORM CARD_ABLE(0,常在無効)
PRINTL ②: 이 카드가 필드에 존재하는 한,
PRINTL 레벨 7 이상의 몬스터는 전부 수비 표시가 되며, 효과는 무효화된다.
PRINTL (랭크 링크도 포함된다)

@CARDCAN_2368(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,2368)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,2368)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,2368,"손패")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 엑스큐티
	효과条件固名前 = 엑스큐티 릴리우스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		条件달성 = 1
	SIF 条件달성 != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 常在無効
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_2368(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 常在無効
	CALL FULLRESET
	CALL 효과条件レベル_ADV(7, "以上")
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"表",2)
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"無効")

	CALL FULLRESET
;	효과元カードID = 2368
	CALL 효과条件レベル_ADV(7, "以上")
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"表",2,2368)
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"無効",0,2368)

ELSEIF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,2368,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)
		CALLFORM SS소환可能レベル_ADV(6, "以下")
		CALL CARD_SS_RESTRICTION(듀얼리스트)
	ENDIF
ENDIF
