@CARDNAME_42368,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 엑스큐티 류미엘 프론티어
	CASE "略称"
RESULTS = 류미엘 F
	CASE "カテゴリ"
RESULTS:0 = 엑스큐티
ENDSELECT


@CARD_42368(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"

		RETURN 융합
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2800
	CASE "守備力"
		RETURN 500
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0
@CARDSUMMON_42368(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2

IF EX参照 == 0 || EX参照 == 1
素材条件名前 = 엑스큐티 류미엘
;素材２장目＝モン全般
ELSEIF EX参照 >= 10
素材条件名前 = 엑스큐티 릴리우스
ENDIF


IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,42368) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_42368(종류)
#DIM DYNAMIC 종류
PRINTL 「엑스큐티 류미엘」＋「엑스큐티 릴리우스」
PRINTL 이 카드는 자신 필드의 상기의 카드를 덱으로 되돌리고 융합 소환할 수 있다.
CALLFORM CARD_ABLE(0,EX소환時)
PRINTL ①: 이 카드가 융합 소환했을 경우에 발동할 수 있다.
PRINTL 상대 필드의 몬스터 1장을 덱으로 되돌린다.
PRINTL 그 후, 패 / 묘지에서 「엑스큐티」 몬스터 1장을 특수 소환할 수 있다.
CALLFORM CARD_ABLE(0,ターン開始時)
PRINTL ②: 자신의 드로우 페이즈에 발동할 수 있다.
PRINTL 자신은 손패가 5장이 되도록 드로우한다.
CALLFORM CARD_ABLE(0,破壊→묘지 ,戦闘→묘지)
PRINTL ③: 이 카드가 파괴되었을 경우에 발동할 수 있다.
PRINTL 덱에서 「엑스큐티 류미엘」 또는 「엑스큐티 릴리우스」 1장을 특수 소환한다.


@CARDCAN_42368(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,42368)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX덱起動
	CALL CARD_EX_CHECK(듀얼리스트,42368)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
효과条件名前 = 엑스큐티 류미엘
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
효과条件名前 = 엑스큐티 릴리우스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == EX소환時
	CALL FULLRESET
	효과範囲상대필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン開始時
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	SIF 손패数(듀얼리스트) >= 5
		RETURN 0
	CALL CARD_DRAW_CHECK
	RETURN 1
ELSEIF 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件名前:0 = 엑스큐티 류미엘
	효과条件名前:1 = 엑스큐티 릴리우스
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_42368(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC ドロー枚数
#DIM DYNAMIC 攻撃力상승値

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,42368)
	IF RESULT == 0
		CALL RESETAREA_MAT
		素材範囲필드 = 1
		CALL FULLRESET
		TRYCALLFORM CARDSUMMON_42368(듀얼리스트,場所)
		CALL FUSSUMMON(듀얼리스트,場所,덱行き)
	ENDIF
ELSEIF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,42368)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"덱")
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲묘지 = 1
			효과条件カテゴリ = 엑스큐티
			효과条件任意 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == ターン開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,42368)
	IF RESULT == 0
		ドロー枚数 = 5 - 손패数(듀얼리스트)
		SIF ドロー枚数 >= 1
			CALL CARDDRAW_BASIC(듀얼리스트,ドロー枚数)
	ENDIF
ELSEIF 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,42368)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件名前:0 = 엑스큐티 류미엘
		효과条件名前:1 = 엑스큐티 릴리우스
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF

