CALL CARD_ABLE(2,필드起動,フリー誘発)
PRINTL ③: 자신 / 상대 턴에 상대 필드의 카드 1장을 대상으로 하여 발동할 수 있다.
PRINTL 제외 상태인 "I HERO" 몬스터 2장을 덱으로 되돌리고, 대상 카드를 패로 되돌린다.

@CARDSUMMON_51845(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要素材数 = 2
; 素材条件カテゴリ = I HERO

IF EX参照 == 0 || EX参照 == 1
素材条件名前 = I HERO
	素材条件名前:1 = I HERO 키네틱 파워 오버드라이브 유리코
	; 素材条件カテゴリ:1 = 七尾百合子
	; 素材条件종류 = 융합
ELSEIF EX参照 >= 10
	素材条件カテゴリ = I HERO
	素材条件종류 = 융합
ENDIF

IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,51845) 
	RETURN RESULT
ENDIF

@CARDCAN_51845(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,51845)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,51845)
		RETURN 0
	CALL FULLRESET
	효과範囲제외 = 1
	효과条件カテゴリ = I HERO
	효과条件종류 = モン全般
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과範囲마함 = 1
	효과範囲상대마함 = 1
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF 발동済_2(듀얼리스트,51845)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲제외 = 1
	효과条件カテゴリ = I HERO
	효과条件종류 = モン全般
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_51845(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,51845,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲제외 = 1
		효과条件カテゴリ = I HERO
		효과条件종류 = モン全般
		효과条件行先플래그 = 덱行き
		효과条件複数 = 2
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			; CALL FULLRESET
			; 효과範囲필드 = 1
			; 효과範囲상대필드 = 1
			; 효과範囲마함 = 1
			; 효과範囲상대마함 = 1
			; 효과条件他所 = 1
			; CALL CARD_REMOVAL_ADV(듀얼리스트,"全体除去",場所,"손패")
			CALL FULLRESET
			효과条件他所 = 1
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"両全場全体",場所,"손패")
		ENDIF
	ENDIF
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,51845,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲제외 = 1
		효과条件カテゴリ = I HERO
		효과条件종류 = モン全般
		효과条件行先플래그 = 덱行き
		효과条件複数 = 2
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲상대필드 = 1
			효과範囲상대마함 = 1
			CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"손패")
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"対耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"破耐")
ENDIF