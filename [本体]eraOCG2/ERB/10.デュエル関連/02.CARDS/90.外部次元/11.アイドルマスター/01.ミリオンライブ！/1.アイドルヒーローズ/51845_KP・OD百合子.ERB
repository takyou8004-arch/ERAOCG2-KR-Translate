@CARDNAME_51845,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = I HERO 키네틱 파워 오버드라이브 유리코
	CASE "略称"
		RESULTS = KP OD 유리코
	CASE "カテゴリ"
		RESULTS:0 = HERO
		RESULTS:1 = I HERO
		RESULTS:2 = 밀리언 라이브!
		; RESULTS:2 = THE IDOLM@STER
		; RESULTS:4 = MILLION LIVE!
		; RESULTS:5 = 七尾百合子
		RESULTS:4 = 나나오 유리코
ENDSELECT

@CARD_51845(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 3000
	CASE "守備力"
		RETURN 3000
	CASE "성별"
		RETURN 牝性
	CASE "특수불가"
		RETURN 1
ENDSELECT
RETURN 0

