@CARDNAME_4650,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 매혹의 래빗 딜러 카렌
	CASE "略称"
		RESULTS = 래빗 D 카렌
	CASE "カテゴリ"
		RESULTS:0 = 래빗 걸
		RESULTS:1 = 래빗시티
		RESULTS:2 = 밀리언 라이브!
		; RESULTS:2 = THE IDOLM@STER
		; RESULTS:4 = MILLION LIVE!
		RESULTS:4 = 시노미야 카렌
ENDSELECT

@CARD_4650(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 1
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 2800
	CASE "성별"
		RETURN 牝性
	CASE "分割"
		RETURN 펜듈럼

	CASE "추가日"
		RETURN 250601
ENDSELECT
RETURN 0

@CARD_EXPLANATION_4650(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("P_MAGIC")
PRINTL 이 카드명의 ①의 펜듈럼 효과는 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,발동強制誘発)
PRINTL ①: 자신이 마법 / 함정 카드의 효과를 발동했을 경우에, 자신이 이 카드의 원래 주인이 아니라면 발동한다.
PRINTL 그 발동을 무효로 하고, 발동한 그 카드를 이 카드의 원래 주인의 마법 & 함정 존에 세트한다.
PRINTL 그 후, 이 카드를 파괴한다.
; PRINTL 그 후、이 카드를 元々の주인의 필드에 특수 소환한다。
PRINTL 이 효과로 세트한 카드는 발동할 수 없다.
CALL CARD_ABLE(0,ターン終了時強制)
PRINTL ②: 자신 엔드 페이즈에 발동한다.
PRINTL 이 카드의 컨트롤러에 따라, 이하의 효과를 적용한다.
PRINTL ● 컨트롤러가 원래 주인일 경우: 이 카드를 상대의 마법 & 함정 존에 놓는다.
PRINTL ● 컨트롤러가 원래 주인이 아닐 경우: 이 카드를 원래 주인의 필드에 특수 소환한다.
CALL TEXT_DECORATION("P_MAGIC_2")
PRINTL 이 카드명의 ①②의 몬스터 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(2,ターン開始時)
PRINTL ①: 상대 스탠바이 페이즈에 발동할 수 있다.
PRINTL 패/필드의 이 카드 이외의 "바니 폴리스" 카드 1장을 덱으로 되돌리고,
PRINTL 패의 이 카드를 상대의 마법 & 함정 존에 앞면 표시로 놓는다.
CALL CARD_ABLE(3,특수소환時)
PRINTL ②: 이 카드가 특수 소환되었을 경우에 발동할 수 있다.
PRINTL 덱에서 "바니 폴리스" 마법 / 함정 카드 1장을 패에 추가한다.


@CARDCAN_4650(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

#DIM DYNAMIC 持ち主판정 ;0で자신,1で상대

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4650)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동強制誘発
	SIF 발동済_1(듀얼리스트,4650)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") > 1
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 마함全般
	CALL CARD_CHECK_START_DATA(듀얼리스트)
	SIF RESULT != 1
		RETURN 0
	IF 듀얼리스트 == "자신"
		SIF GETVAR(@"%듀얼리스트%_魔_所有:場所") == 1
			RETURN 0
	ELSE
		SIF GETVAR(@"%듀얼리스트%_魔_所有:場所") == 2
			RETURN 0
	ENDIF
	SIF 마함数(対面者) >= 5
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時強制
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") > 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	IF 듀얼리스트 == "자신"
		SIF GETVAR(@"%듀얼리스트%_魔_所有:場所") == 2
			持ち主판정 = 1
	ELSE
		SIF GETVAR(@"%듀얼리스트%_魔_所有:場所") == 1
			持ち主판정 = 1
	ENDIF
	IF 持ち主판정 == 1
		CALL CARD_SS_CHECK(듀얼리스트,4650,"마함")
		SIF RESULT == 1
			RETURN 0
	ELSEIF 持ち主판정 == 0
		SIF 마함数(対面者) >= 5
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	RETURN 1
ELSEIF 종류 == ターン開始時
	SIF 발동済_2(듀얼리스트,4650)
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	SIF ゾーン != "손패"
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과条件他所 = 1
효과条件カテゴリ = 바니 폴리스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	SIF 마함数(対面者) >= 5
		RETURN 0
	RETURN 1
ELSEIF 종류 == 특수소환時
	SIF 발동済_3(듀얼리스트,4650)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 바니 폴리스
	효과条件종류 = 마함全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_4650(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID
#DIMS DYNAMIC 選択カードゾーン
#DIM DYNAMIC 選択カード場所
#DIM DYNAMIC 持ち主판정 ;0で자신,1で상대

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4650)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ELSEIF 종류 == 발동強制誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4650,1)
	IF RESULT == 0
		選択カードゾーン = 상대%効後_ゾーン:処理中番号% 
		選択カード場所 = 効後_設場所:(処理中番号) 
		; CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
		効後_발동無:(処理中番号) = 無効行き
		CALL CARD_ADD(対面者,選択カードゾーン,"마함",選択カード場所,"使用불가")
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,破壊行き)
		; IF モンスター数(対面者) < 5
		; 	選択カード場所 = 現필드(対面者)
		; 	CALL CARD_SP_SUMMON(対面者,"상대마함",場所)
		; 	IF 듀얼리스트 == "자신"
		; 		SETVAR @"%対面者%_MZ_所有:選択カード場所", 2
		; 	ELSE
		; 		SETVAR @"%対面者%_MZ_所有:選択カード場所", 1
		; 	ENDIF
		; ENDIF
	ENDIF
ELSEIF 종류 == ターン終了時強制
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4650)
	IF RESULT == 0
		IF 듀얼리스트 == "자신"
			SIF GETVAR(@"%듀얼리스트%_魔_所有:場所") == 2
				持ち主판정 = 1
		ELSE
			SIF GETVAR(@"%듀얼리스트%_魔_所有:場所") == 1
				持ち主판정 = 1
		ENDIF
		IF 持ち主판정 == 1
			選択カード場所 = 現필드(対面者)
			CALL CARD_SP_SUMMON(対面者,"상대마함",場所)
			IF 듀얼리스트 == "자신"
				SETVAR @"%対面者%_MZ_所有:選択カード場所", 2
			ELSE
				SETVAR @"%対面者%_MZ_所有:選択カード場所", 1
			ENDIF
		ELSE
			CALL CARD_ADD(対面者,"상대마함","마함",場所)
		ENDIF
	ENDIF
ELSEIF 종류 == ターン開始時
	SIF GETVAR(@"%듀얼리스트%_손패:場所") != 4650
		RETURN
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4650,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲필드 = 1
		효과条件他所 = 1
		효과条件カテゴリ = 바니 폴리스
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL CARD_ADD(対面者,"상대손패","마함",場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4650,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 바니 폴리스
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF