#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 바니 폴리스의 출입 조사
	CASE "略称"
		RESULTS = 바니 P 출입조사
	CASE "カテゴリ"
		RESULTS:0 = 바니 폴리스
		RESULTS:1 = 래빗시티
		RESULTS:2 = 밀리언 라이브!
ENDSELECT

@CARD_78712(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 速攻마법
	CASE "추가日"
		RETURN 250601
ENDSELECT
RETURN 0

