@CARDNAME_78710,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 바니 폴리스 인력 배치
	CASE "略称"
		RESULTS = 바니 P 인력 배치
	CASE "カテゴリ"
		RESULTS:0 = 래빗시티
		RESULTS:1 = 바니 폴리스
		RESULTS:2 = 밀리언 라이브!
ENDSELECT

@CARD_78710(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250601
ENDSELECT
RETURN 0

