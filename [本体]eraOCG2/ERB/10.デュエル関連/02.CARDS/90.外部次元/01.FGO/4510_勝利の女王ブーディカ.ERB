VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 승리의 여왕 부디카
	CASE "略称"
		RESULTS = 부디카
	CASE "カテゴリ"
		RESULTS:0 = 영령
		RESULTS:1 = 라이더
		RESULTS:2 = FATE
ENDSELECT


