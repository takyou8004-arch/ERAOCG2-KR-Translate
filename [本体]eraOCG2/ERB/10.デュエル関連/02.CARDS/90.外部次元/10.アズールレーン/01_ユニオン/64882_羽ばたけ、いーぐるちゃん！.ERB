CALL CARD_ABLE(0,묘지起動)
PRINTL ②: 자신 메인 페이즈에 발동할 수 있다.
PRINTL 묘지의 이 카드와 "유니온 함재기" 토큰 3장을 제외하고, 자신 필드에 "유니온 함재기" 토큰 1장을 특수 소환한다.
PRINTL 이 효과의 발동 후, 자신은 턴 종료시까지 "유니온 (AL)" 몬스터밖에 특수 소환할 수 없다.

@CARDCAN_64882(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ数

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,64882)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,64882)
		RETURN 0
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	CALL CARD_SS_CHECK(듀얼리스트,164881)
	SIF RESULT == 1
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,64882)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,164881)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件名前 = 유니온 함재기
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 3
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_64882(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 戦闘カード
#DIM DYNAMIC 変身カードID
#DIM DYNAMIC 修正値
#DIM DYNAMIC 元괴한플랜트위치
#DIMS DYNAMIC ヒロインネーム
#DIMS DYNAMIC 元ヒロインネーム
#DIMS DYNAMIC 無効대상ID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,64882,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_GENERATE(듀얼리스트,164881)
		CALL CARD_SS_RESTRICTION(듀얼리스트,"유니온(AL)")
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,64882,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件名前 = 유니온 함재기
		효과条件複数 = 3
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_GENERATE(듀얼리스트,164881)
		ENDIF
		CALL CARD_SS_RESTRICTION(듀얼리스트,"유니온(AL)")
	ENDIF
ENDIF