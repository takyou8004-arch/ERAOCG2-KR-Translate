@CARDNAME_64882,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 날개짓해, 이-글쨩!
	CASE "略称"
		RESULTS = 이-글쨩
	CASE "カテゴリ"
		RESULTS:0 = 벽람항로
		RESULTS:1 = 유니온(AL)
ENDSELECT

@CARD_64882(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 速攻마법
	CASE "生成カード"
		RETURN 164881
	CASE "추가日"
		RETURN 250503
ENDSELECT
RETURN 0

