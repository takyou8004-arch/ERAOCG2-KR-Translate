@CARDNAME_4906,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 항공모함 요크타운
	CASE "略称"
		RESULTS = 요크타운
	CASE "カテゴリ"
		RESULTS:0 = 벽람항로
		RESULTS:1 = 유니온(AL)
ENDSELECT

@CARD_4906(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 牝性
	CASE "生成カード"
		RETURN 164881
	CASE "추가日"
		RETURN 250503
ENDSELECT
RETURN 0

@CARD_EXPLANATION_4906(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 1턴에 1번, 둘 중 1개밖에 사용할 수 없다.
CALL CARD_ABLE(1,손패起動)
PRINTL ①: 자신 메인 페이즈에, 묘지에 이 카드명의 카드가 존재하지 않는 경우에 발동할 수 있다.
PRINTL 패에서 이 카드와 카드 1장을 묘지로 보내고, 덱에서 레벨 4 이하의 "유니온(AL)" 몬스터 1장을 특수 소환한다.
CALL CARD_ABLE(2,묘지起動)
PRINTL ②: 자신 메인 페이즈에 이 카드가 묘지에 존재할 경우에 발동할 수 있다.
PRINTL 손패를 1장 버리고, 묘지의 "유니온(AL)" L 몬스터 1장을 EX덱으로 되돌린다.
PRINTL 그 후, 자신 필드에 "유니온 함재기" 토큰을 특수 소환한다.
PRINTL 이 효과의 발동 후, 자신은 턴 종료시까지 "유니온(AL)" 몬스터밖에 특수 소환할 수 없다.

@CARDCAN_4906(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4906)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,4906)
		RETURN 0
	SIF 발동済_2(듀얼리스트,4906)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件名前 = 항공모함 요크타운
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	SIF 손패数(듀얼리스트) < 2
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 유니온(AL)
	CALL 효과条件レベル_ADV(4, "以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_1(듀얼리스트,4906)
		RETURN 0
	SIF 발동済_2(듀얼리스트,4906)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 유니온(AL)
	효과条件종류 = 링크
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	SIF 손패数(듀얼리스트) == 0
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,164881)
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_4906(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4906,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = 유니온(AL)
			CALL 효과条件レベル_ADV(4, "以下")
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4906,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 유니온(AL)
			효과条件종류 = 링크
			효과条件行先플래그 = 덱行き
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				CALL CARD_GENERATE(듀얼리스트,164881)
			ENDIF
		ENDIF
		CALL CARD_SS_RESTRICTION(듀얼리스트,"유니온(AL)")
	ENDIF
ENDIF