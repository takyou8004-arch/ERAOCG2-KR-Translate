@CARDNAME_4907,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 항공모함 엔터프라이즈
	CASE "略称"
RESULTS = 엔터프라이즈
	CASE "カテゴリ"
RESULTS:0 = 아주르 레인
RESULTS:1 = 유니온(AL)
ENDSELECT

@CARD_4907(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 1500
	CASE "특수制限"
		RETURN 1
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250503
ENDSELECT
RETURN 0

@CARD_EXPLANATION_4907(종류)
#DIM DYNAMIC 종류

PRINTL 토큰 이외의 "아주르 레인" 몬스터 2체
PRINTL 자신은 "항공모함 엔터프라이즈"를 1턴에 1번만 특수 소환할 수 없다.
PRINTL 이 카드명의 ①의 효과는 1턴에 1번만 사용할 수 없다.
CALL CARD_ABLE(1,EX소환時)
PRINTL ①: 이 카드가 L 소환에 성공했을 경우에 발동할 수 있다.
PRINTL 손패를 1장 버리고, 덱에서 "아주르 레인" 필드 마법 1장을 자신 필드에 앞면 표시로 놓는다.
PRINTL 그 후, 이 효과로 버린 카드가 "유니온(AL)" 카드였을 경우, 자신 필드에 "유니온 함재기" 토큰을 특수 소환한다.

@CARDCAN_4907(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4907)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,4907)
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5
		RETURN 0
	SIF 손패数(듀얼리스트) == 0
		RETURN 0
	SIF 現마함(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 아주르레인
	효과条件종류 = フィールド
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDSUMMON_4907(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要링크数 = 2
素材条件종류 = トークン以外

IF EX参照 == 0 || EX参照 == 1
	素材条件カテゴリ = 아주르레인
ENDIF

IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,4907) 
	RETURN RESULT
ENDIF


@CARDEFFECT_4907(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4907,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		選択カードID = RESULT:1
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = 아주르레인
			효과条件종류 = フィールド
			효과条件任意 = 1
			CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시")
			IF RESULT == 1
				CALLFORM CARDNAME_{RESULT:1}("カテゴリ")
				IF MATCH(RESULTS, "유니온(AL)")
					CALL CARD_SS_CHECK(듀얼리스트,164881)
					IF RESULT != 1
						CALL FULLRESET
						CALL CARD_GENERATE(듀얼리스트,164881)
					ENDIF
				ENDIF				
			ENDIF
		ENDIF
	ENDIF
ENDIF