@CARDNAME_4973,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 전함 퀸 엘리자베스
	CASE "略称"
		RESULTS = Q. 엘리자베스
	CASE "カテゴリ"
		RESULTS:0 = 벽람항로
		RESULTS:1 = 로열(AL)
ENDSELECT

@CARD_4973(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 2000
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 牝性
	CASE "単一存在"
		RETURN 1
ENDSELECT
RETURN 0

@CARD_EXPLANATION_4973(종류)
#DIM DYNAMIC 종류

PRINTL "로열(AL)" 튜너 ＋ 튜너 이외의 "로열(AL)" 몬스터 1장 이상
PRINTL 이 카드명의 ③의 효과는 1턴에 1번밖에 사용할 수 없다.
PRINTL ①: "전함 퀸 엘리자베스"는 자신 필드에 1장밖에 앞면 표시로 존재할 수 없다.
CALL CARD_ABLE(0,攻守変動)
PRINTL ②: 자신 필드의 "로열(AL)" 몬스터 모든 공격력, 수비력은 500 업한다.
CALL CARD_ABLE(1,EX소환時)
PRINTL ③: 이 카드의 S소환에 성공했을 경우에 발동할 수 있다.
PRINTL 덱에서 "로열(AL)" 튜너를 2장까지 특수 소환한다. (최소 1장, 동명 카드는 1장까지)
PRINTL 그 후, 덱에서 튜너 이외의 "로열(AL)" 몬스터를 2장까지 특수 소환할 수 있다. (동명 카드는 1장까지)
PRINTL 이 효과의 발동 후, 자신은 턴 종료시까지 "로열(AL)" 몬스터밖에 특수 소환할 수 없다.

@CARDSUMMON_4973(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 10
素材条件カテゴリ = 로열(AL)

;1体目の条件
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF

IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,4973) 
	RETURN RESULT
ENDIF

@CARDCAN_4973(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4973)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,4973)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 로열(AL)
	효과条件튜너 = 1
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_4973(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ
#DIM DYNAMIC 選択효과

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4973,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 로열(AL)
		효과条件튜너 = 1
		효과条件複数 = 98
		효과条件固名前選択 = 1
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT:1 != 0
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = 로열(AL)
			효과条件튜너 = -1
			효과条件複数 = 98
			효과条件任意 = 1
			효과条件固名前選択 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
		CALL CARD_SS_RESTRICTION(듀얼리스트,"로열(AL)")
	ENDIF
ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과条件カテゴリ = 로열(AL)
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,500,500)
ENDIF