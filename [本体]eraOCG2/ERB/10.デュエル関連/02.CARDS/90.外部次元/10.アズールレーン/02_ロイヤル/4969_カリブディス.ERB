@CARDNAME_4969,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 경순양함 카리브디스
	CASE "略称"
		RESULTS = 카리브디스
	CASE "カテゴリ"
		RESULTS:0 = 벽람항로
		RESULTS:1 = 로열(AL)
ENDSELECT

@CARD_4969(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 牝性
	CASE "튜너"
		RETURN 1
ENDSELECT
RETURN 0

@CARD_EXPLANATION_4969(종류)
#DIM DYNAMIC 종류

PRINTL "로열(AL)" 튜너 ＋ 튜너 이외의 몬스터 1장 이상
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,EX소환時,특수소환時)
PRINTL ①: 이 카드가 특수 소환되었을 경우에, 자신 필드의 몬스터 1장을 대상으로 하고 발동할 수 있다.
PRINTL 덱에서 "로열(AL)" 카드 1장을 묘지로 보내고, 대상 카드의 공격력 / 수비력을 500 올린다.
PRINTL 그 후, 이 카드의 레벨을 1개 낮춘다.
CALL CARD_ABLE(2,발동誘発)
PRINTL ②: 상대가 몬스터의 효과를 발동했을 경우에, 자신 필드에 "전함 퀸 엘리자베스"가 존재한다면 발동할 수 있다.
PRINTL 묘지의 이 카드와 튜너 이외의 몬스터 1장을 덱으로 되돌리고, 그 효과를 무효로 한다.

@CARDSUMMON_4969(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 5

;1体目の条件
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
	素材条件カテゴリ = 로열(AL)
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF

IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,4969) 
	RETURN RESULT
ENDIF

@CARDCAN_4969(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4969)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,4969)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 로열(AL)
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF 발동済_2(듀얼리스트,4969)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
효과条件名前 = 전함 퀸 엘리자베스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件튜너 = -1
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_4969(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ
#DIM DYNAMIC 選択효과

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == EX소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4969,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = 로열(AL)
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"永攻",500)
				CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"永守",500)
				CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"永星",-1)
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4969,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,덱行き)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件튜너 = -1
			효과条件종류 = モン全般
			효과条件行先플래그 = 덱行き
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効")
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	RETURN 0
ENDIF