@CARDNAME_4961,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 구축함 재너스
	CASE "略称"
		RESULTS = 재너스
	CASE "カテゴリ"
		RESULTS:0 = 벽람항로
		RESULTS:1 = 로열(AL)
ENDSELECT

@CARD_4961(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 600
	CASE "守備力"
		RETURN 800
	CASE "성별"
		RETURN 牝性
ENDSELECT
RETURN 0

@CARD_EXPLANATION_4961(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번만 사용할 수 있다.
CALL CARD_ABLE(1,손패起動)
PRINTL ①: 자신 메인 페이즈에 발동할 수 있다.
PRINTL 패의 이 카드를 버리고, EX덱의 튜너 이외의 "로열(AL)" S 몬스터 1장을 상대에게 보여준다.
PRINTL 덱에서 레벨이 "공개한 S 몬스터의 레벨-1"인 "로열(AL)" 몬스터 1장을 특수 소환한다.
CALL CARD_ABLE(2,묘지起動)
PRINTL ②: 자신 필드에 "로열(AL)" 몬스터가 존재할 경우에 발동할 수 있다.
PRINTL 묘지의 이 카드를 특수 소환한다.
PRINTL 이 효과의 발동 후, 자신은 턴 종료시까지 S 몬스터밖에 EX덱에서 특수 소환할 수 없다.

@CARDCAN_4961(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4961)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,4961)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件종류 = 싱크로
	효과条件カテゴリ = 로열(AL)
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = モン全般
	효과条件カテゴリ = 벽람항로
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,4961)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,4961,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 로열(AL)
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_4961(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4961,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲EX덱 = 1
		효과条件종류 = 싱크로
		효과条件カテゴリ = 로열(AL)
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		選択カードID = RESULT:99
		IF 選択カード != 9999
			CALL FULLRESET
			효과範囲덱 = 1
			CALLFORM CARD_{選択カードID}("レベル")
			효과条件レベル = RESULT - 1
			효과条件종류 = モン全般
			효과条件カテゴリ = 로열(AL)
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4961,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
	ENDIF
	CALL FULLRESET
	EX소환可能종류 = 싱크로
	CALL CARD_EX_RESTRICTION(듀얼리스트)
ENDIF