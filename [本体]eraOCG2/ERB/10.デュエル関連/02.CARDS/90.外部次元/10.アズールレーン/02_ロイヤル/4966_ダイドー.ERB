@CARDNAME_4966,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 경순양함 다이도
	CASE "略称"
		RESULTS = 다이도
	CASE "カテゴリ"
		RESULTS:0 = 벽람항로
		RESULTS:1 = 로열(AL)
ENDSELECT

@CARD_4966(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2000
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 牝性
	CASE "튜너"
		RETURN 1
ENDSELECT
RETURN 0

@CARD_EXPLANATION_4966(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,손패起動,묘지起動)
PRINTL ①: 자신 메인 페이즈에 자신 필드에 "전함 퀸 엘리자베스"가 존재하지 않는 경우에 발동할 수 있다.
PRINTL 손패 묘지의 이 카드를 특수 소환한다.
PRINTL 이 효과의 발동 후, 자신은 턴 종료시까지 "로열(AL)" 몬스터밖에 특수 소환할 수 없다.
CALL CARD_ABLE(2,S素材時효과)
PRINTL ②: 이 카드가 "전함 퀸 엘리자베스"의 S 소재로서 묘지로 보내졌을 경우에,
PRINTL 자신 필드의 "전함 퀸 엘리자베스" 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 카드에 마법 내성을 부여한다.
CALL CARD_ABLE(3,身代효과)
PRINTL ③: 자신 필드의 "전함 퀸 엘리자베스"가 전투 / 상대의 효과로 필드를 벗어날 경우,
PRINTL 대신 묘지의 이 카드를 제외한다.

@CARDCAN_4966(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4966)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動 || 종류 == 묘지起動
	SIF 발동済_1(듀얼리스트,4966)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,4966,ゾーン)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
효과条件名前 = 전함 퀸 엘리자베스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == S素材時효과
	SIF 발동済_2(듀얼리스트,4966)
		RETURN 0
	CALL FULLRESET
효과条件名前 = 전함 퀸 엘리자베스
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"EX특수소환")
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
효과条件名前 = 전함 퀸 엘리자베스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 身代효과
	SIF ゾーン != "묘지"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	TRYCALLFORM CARDNAME_{破壊대상ID}("名前")
	SIF RESULTS != "전함　クイ-ン エリザベス"
		RETURN 0
	SIF !GROUPMATCH(破壊종류,"효과","戦闘")
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_4966(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ
#DIM DYNAMIC 選択효과

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 손패起動 || 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4966,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
		CALL CARD_SS_RESTRICTION(듀얼리스트,"로열(AL)")
	ENDIF
ELSEIF 종류 == S素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4966,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
효과条件名前 = 전함 퀸 엘리자베스
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"魔耐",2)
			; CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"함정耐",2)
		ENDIF
	ENDIF
ELSEIF 종류 == 身代효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4966)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		RETURN 1
	ENDIF
	RETURN 0
ENDIF