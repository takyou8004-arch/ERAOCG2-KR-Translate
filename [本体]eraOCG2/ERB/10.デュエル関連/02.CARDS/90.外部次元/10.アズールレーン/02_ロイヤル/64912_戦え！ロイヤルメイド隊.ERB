@CARD_EXPLANATION_64912(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번만 사용할 수 없다.
CALL CARD_ABLE(1,손패起動,마함起動,フリー誘発)
PRINTL ①: 이 카드를 발동한 턴의 플레이어에 의해, 이하의 효과를 적용한다.
PRINTL ● 자신: 덱 묘지에서 "로열(AL)" 튜너 1체를 특수 소환한다.
PRINTL ● 상대: 묘지에서 "로열(AL)" 튜너 1체를 특수 소환한다.
CALL CARD_ABLE(2,묘지起動)
PRINTL ②: 자신 메인 페이즈에 자신 필드의 "로열(AL)" 몬스터 1체를 대상으로 발동할 수 있다.
PRINTL 묘지의 이 카드를 제외하고, 그 카드를 손패로 되돌리고, 묘지의 "로열(AL)" 튜너 1체를 특수 소환한다.

@CARDCAN_64912(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ数

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,64912)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	SIF 발동済_1(듀얼리스트,64912)
		RETURN 0
	IF ゾーン == "손패"
		SIF ターンプレイヤー != 듀얼리스트
			RETURN 0
	ELSEIF ゾーン == "마함"
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	IF ターンプレイヤー == 듀얼리스트
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 로열(AL)
		효과条件튜너 = 1
		효과条件소환可 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ELSE
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 로열(AL)
		효과条件튜너 = 1
		효과条件소환可 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ENDIF
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,64912)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 로열(AL)
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 로열(AL)
	효과条件튜너 = 1
	효과条件소환可 = 2
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_64912(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 戦闘カード
#DIM DYNAMIC 変身カードID
#DIM DYNAMIC 修正値
#DIM DYNAMIC 元괴수플랜트위치
#DIMS DYNAMIC ヒロインネーム
#DIMS DYNAMIC 元ヒロインネーム
#DIMS DYNAMIC 無効대상ID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,64912,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		IF ターンプレイヤー == 듀얼리스트
			CALL FULLRESET
			효과範囲덱 = 1
			효과範囲묘지 = 1
			효과条件カテゴリ = 로열(AL)
			효과条件튜너 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ELSE
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 로열(AL)
			효과条件튜너 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,64912,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ = 로열(AL)
		CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"손패")
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 로열(AL)
			효과条件튜너 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF