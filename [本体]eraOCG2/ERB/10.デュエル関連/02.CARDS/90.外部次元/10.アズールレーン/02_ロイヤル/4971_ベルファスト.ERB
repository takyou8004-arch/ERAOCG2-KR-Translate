@CARDNAME_4971,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 경순양함 벨파스트
	CASE "略称"
RESULTS = 벨파스트
	CASE "カテゴリ"
RESULTS:0 = 아주르 레인
RESULTS:1 = 로열(AL)
ENDSELECT

@CARD_4971(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2000
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 牝性
	CASE "튜너"
		RETURN 1
ENDSELECT
RETURN 0

@CARD_EXPLANATION_4971(종류)
#DIM DYNAMIC 종류

PRINTL "로열(AL)" 튜너 ＋ 튜너 이외의 몬스터 1장 이상
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,EX소환時,특수소환時)
PRINTL ①: 이 카드가 특수 소환되었을 경우에 발동할 수 있다.
PRINTL 덱에서 "로열(AL)" 마법/함정 카드 1장을 세트한다.
PRINTL 자신 필드에 "전함 퀸 엘리자베스"가 존재할 경우, 이 효과로 세트한 속공 마법/함정 카드는 그 턴에 발동할 수 있다.
CALL CARD_ABLE(2,효과後誘発)
PRINTL ②: "로열(AL)" 튜너가 패/덱으로 되돌아갔을 경우, 필드의 카드 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 카드를 패로 되돌린다.
PRINTL 그 후, 자신 필드의 몬스터 1장을 고르고, 턴 종료시까지 대상 내성을 부여할 수 있다.

@CARDSUMMON_4971(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 7

;1体目の条件
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
	素材条件カテゴリ = 로열(AL)
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF

IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,4971) 
	RETURN RESULT
ENDIF

@CARDCAN_4971(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4971)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,4971)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = 마함全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,4971)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 로열(AL)
	효과条件튜너 = 1
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"손패덱")
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_4971(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ
#DIM DYNAMIC 選択효과

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == EX소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4971,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
효과条件名前 = 전함 퀸 엘리자베스
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과条件カテゴリ = 로열(AL)
			효과条件종류 = 마함全般
			CALL CARD_SET_BASIC(듀얼리스트,"덱",ゾーン,場所,"使化裏側표시")
		ELSE
			CALL FULLRESET
			효과条件カテゴリ = 로열(AL)
			효과条件종류 = 마함全般
			CALL CARD_SET_BASIC(듀얼리스트,"덱",ゾーン,場所,"裏側표시")
		ENDIF
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4971,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과範囲상대필드 = 1
		효과範囲상대마함 = 1
		CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"손패")
		IF RESULT == 1
			CALL FULLRESET
			효과条件任意 = 1
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
			IF RESULT != 9999
				CALL STATCHANGE_EFFECT(듀얼리스트,"自身",RESULT,"対耐",1)
			ENDIF
		ENDIF
	ENDIF
ENDIF