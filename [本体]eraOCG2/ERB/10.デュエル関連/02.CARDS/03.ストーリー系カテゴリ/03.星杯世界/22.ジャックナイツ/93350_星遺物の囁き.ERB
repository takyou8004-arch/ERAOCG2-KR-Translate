@CARDNAME_93350,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 성유물의 속삭임
	CASE "略称"
RESULTS = 성유물의 속삭임
	CASE "カテゴリ"
RESULTS:0 = 성유물
ENDSELECT


@CARD_93350(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 지속함정
ENDSELECT

RETURN 0
@CARD_EXPLANATION_93350(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,마함起動)
PRINTL ①: 이 카드의 발동 시에, 필드의 레벨 5 이상의 몬스터 1체를 대상으로 할 수 있다.
PRINTL 그 경우, 그 몬스터의 공격력 / 수비력은 1000 올린다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ②: 자신 필드에 "잭나이츠" 몬스터는 마법 내성을 얻는다.
CALLFORM CARD_ABLE(0,발동誘発)
PRINTL ③: 자신 필드에 "잭나이츠" 몬스터가 5체 존재할 경우,
PRINTL 상대가 마법 카드의 효과를 발동했을 때에 발동할 수 있다.
PRINTL 그 발동을 무효로 한다.

@CARDCAN_93350(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIM DYNAMIC EX条件달성


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,93350)
SIF RESULT == 1
	RETURN 0


IF 종류 == 마함起動
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3	
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
    SIF ゾーン != "마함"
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 마법全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 잭나이츠
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT <= 4
		RETURN 0

	RETURN 1
ENDIF

@CARDEFFECT_93350(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC EX参照
#DIM DYNAMIC 묘지送カード,10
#DIM DYNAMIC 손패묘지送カード,10
#DIM DYNAMIC 素材数
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択済カード
#DIM DYNAMIC EX条件달성
;융합소환



IF 종류 == 마함起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93350)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
		CALL FULLRESET
		효과条件任意 = 1
		CALL 효과条件レベル_ADV(5,"以上")
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"永攻",1000)
			CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"永守",1000)
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과条件カテゴリ = 잭나이츠
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"魔耐")

ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,93350)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効")
		RETURN 1
	ENDIF
	RETURN 0
ENDIF