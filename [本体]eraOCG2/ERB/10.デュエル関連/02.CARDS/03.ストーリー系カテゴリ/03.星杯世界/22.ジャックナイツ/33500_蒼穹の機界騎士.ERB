@CARDNAME_33500,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 창궁의 잭나이츠
	CASE "略称"
		RESULTS = 창궁의 기계기사
	CASE "カテゴリ"
		RESULTS:0 = 잭나이츠
ENDSELECT


@CARD_33500(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 2000
	CASE "守備力"
		RETURN 2500
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

