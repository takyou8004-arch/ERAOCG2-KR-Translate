@CARDNAME_1934,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 하얀 숲의 요마 디아벨
	CASE "略称"
		RESULTS = 디아벨
	CASE "カテゴリ"
		RESULTS:0 = 하얀 숲
		RESULTS:1 = 디아벨
ENDSELECT


@CARD_1934(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 미확인족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 牝性
ENDSELECT
RETURN 0

@CARD_EXPLANATION_1934(종류)
#DIM DYNAMIC 종류

PRINTL 튜너＋튜너 이외의 몬스터 1장 이상
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
;PRINTL ①：이 카드가 Ｓ몬스터의 튜너를 소재로 하고 Ｓ소환된 경우、
CALLFORM CARD_ABLE(1,EX소환時)
PRINTL ①: 이 카드가 싱크로 소환했을 때, 자신 묘지에 싱크로 몬스터의 튜너가 존재할 경우에 발동할 수 있다.
PRINTL 자신 묘지에서 마법 / 함정 카드 1장을 패에 넣는다.
CALLFORM CARD_ABLE(2,발동誘発)
PRINTL ②: 상대가 마법 / 함정 / 몬스터의 효과를 발동했을 때에 발동할 수 있다.
PRINTL 자신의 패 / 필드에서 마법 / 함정 카드 1장을 묘지로 보내고,
PRINTL 자신의 엑스트라 덱 / 묘지 / 제외되어 있는 레벨 7 이하의 싱크로 몬스터의 튜너 1장을 특수 소환한다.


@CARDSUMMON_1934(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 8
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
;	CALLFORM 素材条件レベル_ADV(7, "以下")
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF
IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,1934) 
	RETURN RESULT
ENDIF


@CARDCAN_1934(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1934)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,1934)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件튜너 = 1
	효과条件종류 = 싱크로
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = 마함全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF 발동済_2(듀얼리스트,1934)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲마함 = 1
	효과範囲필드 = 1
	효과条件종류 = 마함全般
	효과条件표시 = 0
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과範囲묘지 = 1
	효과範囲제외 = 1
	CALL 효과条件レベル_ADV(7, "以下")
	효과条件종류 = 싱크로
	효과条件튜너 = 1
	효과条件소환可 = 1
	효과条件特不無視 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_1934(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == EX소환時
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1934,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = 마함全般
		CALL CARD_ADD_BASIC(듀얼리스트,"묘지",ゾーン,場所)
	ENDIF
ELSEIF 종류 == 발동誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1934,2)
	IF RESULT == 0
		CALL FULLRESET
		;패、場の마함を묘지に
		효과範囲손패 = 1
		효과範囲마함 = 1
		효과範囲필드 = 1
		효과条件종류 = 마함全般
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			;EX덱、묘지、제외からレベル７以下、싱크로튜너を특수소환
			CALL FULLRESET
			효과範囲EX덱 = 1
			효과範囲묘지 = 1
			효과範囲제외 = 1
			CALL 효과条件レベル_ADV(7, "以下")
			효과条件종류 = 싱크로
			효과条件튜너 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
	RETURN 0
ENDIF
