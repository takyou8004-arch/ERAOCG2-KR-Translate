@CARDNAME_52113,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 바이론 오메가
	CASE "略称"
		RESULTS = 바이론 오메가
	CASE "カテゴリ"
		RESULTS:0 = 바이론
ENDSELECT


@CARD_52113(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 3200
	CASE "守備力"
		RETURN 1900
ENDSELECT

RETURN 0

@CARDSUMMON_52113(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 10
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件カテゴリ = 바이론
ENDIF

IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,52113) 
	RETURN RESULT
ENDIF


@CARD_EXPLANATION_52113(종류)
#DIM DYNAMIC 종류

PRINTL 튜너 1체 + 튜너 이외의 "바이론" S 몬스터 1체
CALL CARD_ABLE(1,EX덱起動)
PRINTL 이 카드는 1턴에 1번, 자신 필드 마법 & 함정 존의
PRINTL "바이론" 몬스터 2장을 소재로서 S 소환할 수도 있다.
CALL CARD_ABLE(0,EX소환時)
PRINTL ①: 이 카드가 S 소환에 성공했을 경우에 발동할 수 있다.
PRINTL 상대 필드의 몬스터 1장을 장착 카드로서 이 카드에 장착한다.
CALL CARD_ABLE(10,필드起動)
PRINTL ②: 자신 메인 페이즈에 1번 발동할 수 있다.
PRINTL 자신 묘지의 "바이론" 몬스터 1장을 장착 카드로서 이 카드에 장착한다.
CALLFORM CARD_ABLE(0,발동誘発)
PRINTL ③: 상대 몬스터의 효과가 발동했을 때에 발동할 수 있다.
PRINTL 자신의 마법 & 함정 존에서 몬스터 또는 장착 마법 카드 1장을 묘지로 보내고,
PRINTL 그 발동을 무효로 하고 파괴한다.


@CARDCAN_52113(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 条件レベル

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,52113)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX덱起動
	SIF 발동済_1(듀얼리스트,52113)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,52113,ゾーン,1)
	SIF RESULT == 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 바이론
	효과条件튜너 = 1
	효과条件レベル = 3
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 바이론
	효과条件종류 = 싱크로

	효과条件レベル = 7
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == EX소환時
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = モン全般
	효과条件カテゴリ = 바이론
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件종류:0 = モン全般
	효과条件종류:1 = 装備마법
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과 || 종류 == リアクション효과
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_表:場所") == 3
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_52113(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード, 10
#DIM DYNAMIC 選択カード_ID, 10
#DIM DYNAMIC 選択レベル
#DIM DYNAMIC 合計レベル
#DIM DYNAMIC 必要レベル
#DIM DYNAMIC 素材数
#DIM DYNAMIC 조교대상
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52113,1)
	IF RESULT == 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 바이론
	효과条件튜너 = 1
	효과条件レベル = 3
	CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
	選択カード:0 = RESULT
	IF 選択カード:0 != 9999
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件カテゴリ = 바이론
		효과条件종류 = 싱크로
		효과条件튜너 = -1
		효과条件レベル = 7
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード:1 = RESULT
		IF 選択カード:1 != 9999
			FOR カウンタ, 0, 2
				CALL CARD_SELECT(듀얼리스트,選択カード:カウンタ)
				選択カード = RESULT
				選択カード_ID = GETVAR(@"%대상者（CS）%_%参照先（CS）%:選択カード（CS）")
				CALL CARD_DISPOSE(대상者（CS）,参照先（CS）,選択カード（CS）,묘지行き)
				CALLFORM CARD_STANBY(選択カード_ID:カウンタ,듀얼리스트,S素材時효과,"묘지")
			NEXT
			CALL FULLRESET
			CALL CARD_EX_SUMMON(듀얼리스트, "EX덱", 場所)
		ENDIF
	ENDIF
	ENDIF
ELSEIF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52113)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대필드 = 1
		CALL CARD_EQUIP_MONSTER_ADV(듀얼리스트,ゾーン,場所,,52113)
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52113,10)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종류 = モン全般
		효과条件カテゴリ = 바이론
		CALL CARD_EQUIP_MONSTER_ADV(듀얼리스트,ゾーン,場所,,52113)
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52113)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류:0 = モン全般
		효과条件종류:1 = 装備마법
		효과条件표시 = 表側표시
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"마함",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",0,"묘지")
			RETURN 1
		ENDIF
	ENDIF
	RETURN 0
;사이버 다크のコピペ
ELSEIF 종류 == リアクション효과
	FOR カウンタ,0,5
		SIF GETVAR(@"%듀얼리스트%_마함:カウンタ") == -1
			CONTINUE
		SIF GETVAR(@"%듀얼리스트%_魔_装:カウンタ") != 場所
			CONTINUE
		SIF GETVAR(@"%듀얼리스트%_魔_リ数:カウンタ") >= 10
			CONTINUE
			
		CALL CARD_EROTICS_FIELD(듀얼리스트,"마함",カウンタ)
		조교대상 = RESULT
		TRYCALLFORM CARD_{GETVAR(@"%듀얼리스트%_마함:カウンタ")},"성별"
		IF 조교대상 != -1 && RESULT == 牝性
			PRINTFORML 
			PRINTFORMW 붙잡혀버린 %CALLNAME:조교대상%는 파츠취급되어 힘을 빨리고있다。
			CALL EROTIC_MOVE("구속晒し",조교대상,-1,150)
			CALL SET_FIELD_INFO(듀얼리스트,"마함",カウンタ,"リ数",1,0,1)
		ENDIF
	NEXT
ENDIF
