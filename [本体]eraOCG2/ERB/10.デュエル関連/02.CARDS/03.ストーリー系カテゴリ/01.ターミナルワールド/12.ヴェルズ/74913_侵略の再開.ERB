@CARD_74913(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_74913(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,마함起動,손패起動)
PRINTL ①：자신의 묘지의 수비력 0의 어둠 속성 몬스터 2장을 대상으로 하여 발동할 수 있다.
PRINTL 그 어둠 속성 몬스터를 손패에 넣는다.
PRINTL 자신의 「인벨즈」 몬스터가 5장 이상 제외되어 있는 경우,
PRINTL 제외 상태의 수비력 0의 어둠 속성 몬스터도 대상으로 할 수 있다.

@CARDCAN_74913(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,74913)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	CALL FULLRESET
	효과範囲제외 = 1
	효과条件カテゴリ = 인벨즈
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT > 4
		条件달성 = 1

	CALL FULLRESET
	효과範囲묘지 = 1
	SIF 条件달성 != 0
		효과範囲제외 = 1
	효과条件속성 = 어둠속성
	효과条件守備力 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_74913(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성

IF 종류 == 마함起動 || 종류 == 손패起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,74913)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)

		CALL FULLRESET
		효과範囲제외 = 1
		효과条件カテゴリ = 인벨즈
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT > 4
			条件달성 = 1

		CALL FULLRESET
		효과範囲묘지 = 1
		SIF 条件달성 != 0
			효과範囲제외 = 1
		효과条件속성 = 어둠속성
		효과条件守備力 = 1
		효과条件複数 = 2
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
