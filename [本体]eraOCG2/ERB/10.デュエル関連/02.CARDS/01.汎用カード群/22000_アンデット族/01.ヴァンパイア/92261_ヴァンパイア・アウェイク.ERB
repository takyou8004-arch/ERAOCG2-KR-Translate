@CARDNAME_92261,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 뱀파이어 어웨이크
	CASE "略称"
RESULTS = 뱀파이어 어웨이크
	CASE "カテゴリ"
RESULTS:0 = 뱀파이어
ENDSELECT


@CARD_92261(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常함정
ENDSELECT

