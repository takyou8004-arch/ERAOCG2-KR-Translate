VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 언데드 월드
	CASE "略称"
RESULTS = 언월
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_72200(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN フィールド
ENDSELECT

