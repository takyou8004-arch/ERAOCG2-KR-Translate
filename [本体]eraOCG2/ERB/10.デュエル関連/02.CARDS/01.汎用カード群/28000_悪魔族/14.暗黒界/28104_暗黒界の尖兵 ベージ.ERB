@CARDNAME_28104,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 암흑계의 첨병 베이지
	CASE "略称"
RESULTS = 베이지
	CASE "カテゴリ"
RESULTS:0 = 암흑계
ENDSELECT


@CARD_28104(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 악마족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1600
	CASE "守備力"
		RETURN 1300
ENDSELECT

RETURN 0

@CARD_EXPLANATION_28104(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,손패→묘지)
PRINTL ①：이 카드가 효과로 손패에서 묘지로 버려졌을 경우에 발동할 수 있다.
PRINTL 이 카드를 특수 소환한다.


@CARDCAN_28104(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,28104)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패→묘지
	CALL CARD_SS_CHECK(듀얼리스트,28104,"묘지")
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_28104(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード


IF 종류 == 손패→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,28104)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
효과条件名前 = 암흑계의 첨병 베이지
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	
	ENDIF
ENDIF

