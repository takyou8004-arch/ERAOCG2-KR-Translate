@CARDNAME_72892,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 특허권의 계약서류
	CASE "略称"
RESULTS = 특허권의 계약서
	CASE "カテゴリ"
RESULTS:0 = 계약서
ENDSELECT


@CARD_72892(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_72892(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(0,마함起動)
PRINTL ①: 자신 필드의 "DDD" 몬스터와 같은 종류(융합 S X 링크)의 몬스터를 상대가 특수소환했을 경우에 발동할 수 있다.
PRINTL 상대에게 1000 데미지를 준다.
PRINTL 이 턴, 이 카드가 필드에 존재하는 동안, 상대는 그 특수소환한 몬스터와 같은 종류의 몬스터를 특수 소환할 수 없다.
CALLFORM CARD_ABLE(2,필드→묘지,손패→묘지,덱→묘지,破壊→묘지,戦闘→묘지)
PRINTL ②: 이 카드가 묘지로 보내졌을 경우에 발동할 수 있다.
PRINTL 자신의 묘지의 몬스터 및 자신의 EX덱의 앞면 표시의 P 몬스터 중에서, "DD" 몬스터 1장을 골라 패에 넣는다.


@CARDCAN_72892(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIM DYNAMIC FUS存在
#DIM DYNAMIC SYN存在
#DIM DYNAMIC XYZ存在
#DIM DYNAMIC LIN存在
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,72892)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 소환誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 最終소환決者 == 듀얼리스트
		RETURN 0
	SIF 最終소환타입 == "通常"
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 융합
	효과条件カテゴリ = DDD
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		FUS存在 = 1
	CALL FULLRESET
	효과条件종류 = 싱크로
	효과条件カテゴリ = DDD
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		SYN存在 = 1
	CALL FULLRESET
	효과条件종류 = 엑시즈
	효과条件カテゴリ = DDD
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		XYZ存在 = 1
	CALL FULLRESET
	효과条件종류 = 링크
	효과条件カテゴリ = DDD
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		LIN存在 = 1

	CALLFORM CARD_{最終소환ID}("종류") 
	IF RESULT == 융합 && GETBIT(GETVAR(@"%듀얼리스트%_魔_T1汎数:場所"),0) == 1 && FUS存在
		RETURN 0
	ELSEIF RESULT == 싱크로 && GETBIT(GETVAR(@"%듀얼리스트%_魔_T1汎数:場所"),1) == 1 && SYN存在
		RETURN 0
	ELSEIF RESULT == 엑시즈 && GETBIT(GETVAR(@"%듀얼리스트%_魔_T1汎数:場所"),2) == 1 && XYZ存在
		RETURN 0
	ELSEIF RESULT == 링크 && GETBIT(GETVAR(@"%듀얼리스트%_魔_T1汎数:場所"),3) == 1 && LIN存在
		RETURN 0
	ELSE
		RETURN 0
	ENDIF
	RETURN 1

ELSEIF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	SIF 발동済_2(듀얼리스트,72892)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲EX덱 = 1
	효과条件カテゴリ = DD
	효과条件종류 = EX以外全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_72892(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 속성数
#DIM DYNAMIC 종류セットビット

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72892)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ELSEIF 종류 == 소환誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72892)
	IF RESULT == 0
		CALLFORM CARD_{最終소환ID}("종류") 
		IF RESULT == 융합
			SETBIT 종류セットビット, 0
		ELSEIF RESULT == 싱크로
			SETBIT 종류セットビット, 1
		ELSEIF RESULT == 엑시즈
			SETBIT 종류セットビット, 2
		ELSEIF RESULT == 링크
			SETBIT 종류セットビット, 3
		ELSE
			RETURN 0
		ENDIF
		CALL LIFE_CHANGE(듀얼리스트,"減少","효과",1000,1)	
		CALL SET_FIELD_INFO(듀얼리스트,ゾーン,場所,"T1汎数",종류セットビット)
	ENDIF
ELSEIF 종류 == 지속효과
	SIF GETBIT(GETVAR(@"%듀얼리스트%_魔_T1汎数:場所"),0) == 1
		CALL SET_TMP_SS_RST(듀얼리스트,"종류",융합)
	SIF GETBIT(GETVAR(@"%듀얼리스트%_魔_T1汎数:場所"),1) == 1
		CALL SET_TMP_SS_RST(듀얼리스트,"종류",싱크로)
	SIF GETBIT(GETVAR(@"%듀얼리스트%_魔_T1汎数:場所"),2) == 1
		CALL SET_TMP_SS_RST(듀얼리스트,"종류",엑시즈)
	SIF GETBIT(GETVAR(@"%듀얼리스트%_魔_T1汎数:場所"),3) == 1
		CALL SET_TMP_SS_RST(듀얼리스트,"종류",링크)
ELSEIF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72892,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲EX덱 = 1
		효과条件カテゴリ = DD
		효과条件종류 = EX以外全般
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF