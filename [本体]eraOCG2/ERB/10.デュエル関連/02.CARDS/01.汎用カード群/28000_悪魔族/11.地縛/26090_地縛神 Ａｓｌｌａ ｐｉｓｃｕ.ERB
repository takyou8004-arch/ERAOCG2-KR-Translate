@CARDNAME_26090,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 지박신 Ａｓｌｌａ ｐｉｓｃｕ
	CASE "略称"
		RESULTS = Aslla piscu
	CASE "カテゴリ"
		RESULTS:0 = 지박신
		RESULTS:1 = 지박
ENDSELECT


@CARD_26090(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 2500
	CASE "성별"
		RETURN 牝性
	CASE "単一存在"
		RETURN 1
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26090(종류)
#DIM DYNAMIC 종류

PRINTL 「지박신 Ａｓｌｌａ ｐｉｓｃｕ」は자신 필드에 １장 しか앞면 표시로 存在できない。
PRINTL ①：フィールド마법が앞면 표시로 존재하지 않을 경우、이 카드는 묘지へ送られる。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ②：이 카드는 상대의 공격 대상にならず、상대에게 직접 공격할 수 있다。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ③：자신의 패 / 필드 덱 / 묘지 / 제외상태に존재하는 「지박신 Ａｓｌｌａ ｐｉｓｃｕ」が이 카드だけの경우、
PRINTL 이 카드는 마법 / 함정내성을 얻는다。
CALLFORM CARD_ABLE(0,戦破壊→묘지)
PRINTL ④：앞면 표시인 이 카드가 효과でモンスターゾーンから벗어난 경우에 발동할 수 있다。
PRINTL 상대 필드의 앞면 표시 몬스터를 全て破壊し、파괴한 数×８００데미지를 상대에게 준다。

@CARDCAN_26090(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,26090)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과範囲상대마함 = 1
	효과条件종류 = フィールド
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 戦破壊→묘지
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_表:場所") == 裏側표시
		RETURN 0
	SIF 묘지送り종류 == "戦闘破壊"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_26090(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC ダメージ量
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 효과後強制誘発
	CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)

ELSEIF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26090)
	IF RESULT == 0
		CALL FULLRESET
		효과条件표시 = 表側표시
		효과추가종류 = 3
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드全体",場所,"묘지",800)
	ENDIF

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"攻避")
	CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"直攻")

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과範囲묘지 = 1
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과範囲제외 = 1
	효과条件名前 = 지박신 Ａｓｌｌａ ｐｉｓｃｕ
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1 
		CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"魔耐")
		CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"함정耐")
	ENDIF
ENDIF
