@CARDNAME_30090,参照先
;ココで지정 카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 지박신 Ｕｒｕ
	CASE "略称"
		RESULTS = Uru
	CASE "カテゴリ"
		RESULTS:0 = 지박신
		RESULTS:1 = 지박
ENDSELECT


@CARD_30090(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 곤충족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 3000
	CASE "守備力"
		RETURN 3000
	CASE "성별"
		RETURN 雄性
	CASE "単一存在"
		RETURN 1
ENDSELECT

RETURN 0

@CARD_EXPLANATION_30090(종류)
#DIM DYNAMIC 종류

PRINTL "지박신 Ｕｒｕ"는 자신 필드에 1장밖에 앞면 표시로 존재할 수 없다.
PRINTL ①：필드 마법이 앞면 표시로 존재하지 않을 경우, 이 카드는 묘지로 보내진다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ②：이 카드는 상대의 공격 대상이 되지 않으며, 상대에게 직접 공격할 수 있다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ③：자신의 패 / 필드 / 덱 / 묘지 / 제외 상태에 존재하는 "지박신 Ｕｒｕ"가 이 카드뿐인 경우,
PRINTL 이 카드는 마법 / 함정내성을 얻는다。
CALLFORM CARD_ABLE(10,필드起動)
PRINTL ④：１턴에 １번、이 카드 이외의 자신 필드의 몬스터 １장을 릴리스하고 발동할 수 있다。
PRINTL 상대 필드의 몬스터 １장을 고르고、엔드 페이즈까지 컨트롤을 얻는다。

@CARDCAN_30090(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,30090)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과範囲상대마함 = 1
	효과条件종류 = フィールド
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_30090(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC ダメージ量
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 효과後強制誘発
	CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)

ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,30090,10)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件他所 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과추가종류 = 5
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"強奪")
		ENDIF
	ENDIF

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"攻避")
	CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"直攻")

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과範囲묘지 = 1
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과範囲제외 = 1
	효과条件名前 = 지박신 Ｕｒｕ
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1 
		CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"魔耐")
		CALL STATCHANGE_EFFECT(듀얼리스트,"자신",場所,"함정耐")
	ENDIF
ENDIF
