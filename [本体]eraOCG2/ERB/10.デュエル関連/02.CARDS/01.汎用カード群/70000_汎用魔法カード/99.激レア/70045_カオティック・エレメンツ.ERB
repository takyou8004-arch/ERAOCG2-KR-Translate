@CARDNAME_70045,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 카오틱 엘레멘츠
	CASE "略称"
		RESULTS = 카오틱 엘레멘츠
	CASE "カテゴリ"
		RESULTS:0 = 없음
ENDSELECT


@CARD_70045(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250416
ENDSELECT

RETURN 0

@CARD_EXPLANATION_70045(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,마함起動,손패起動)
PRINTL ①: 자신의 덱 / 묘지에서 레벨 5 이상인 원소정령족의 빛 / 어둠 속성 몬스터 1장을 패에 넣는다.
PRINTL 그 후, 자신의 묘지에 원소정령족 몬스터가 3장 이상 존재할 경우, 상대 필드의 카드 1장을 파괴할 수 있다.
CALLFORM CARD_ABLE(2,묘지起動)
PRINTL ②: 상대 필드에 원소정령족 몬스터가 존재할 경우, 묘지의 이 카드를 제외하고 발동할 수 있다.
PRINTL 상대 필드의 몬스터 1장의 컨트롤을 엔드 페이즈까지 얻는다.

@CARDCAN_70045(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,70045)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,70045)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲묘지 = 1
	효과条件종족 = 원소정령족
	효과条件속성:0 = 빛속성
	효과条件속성:1 = 어둠속성
	CALL 효과条件レベル_ADV(5,"以上")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,70045)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件종족 = 원소정령족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_70045(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,70045,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件종족 = 원소정령족
		효과条件속성:0 = 빛속성
		효과条件속성:1 = 어둠속성
		CALL 효과条件レベル_ADV(5,"以上")
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件종족 = 원소정령족
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT >= 3
				CALL FULLRESET
				효과範囲상대필드 = 1
				효과範囲상대마함 = 1
				CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"묘지")
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,70045,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과추가종류 = 返却行き
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"強奪")
	ENDIF
ENDIF
