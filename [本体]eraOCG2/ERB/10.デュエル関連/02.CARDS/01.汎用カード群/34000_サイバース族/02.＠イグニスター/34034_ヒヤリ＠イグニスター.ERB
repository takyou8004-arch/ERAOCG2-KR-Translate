@CARDNAME_34034,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 히야리＠이그니스터
	CASE "略称"
RESULTS = 히야리＠
	CASE "カテゴリ"
RESULTS:0 = ＠이그니스터
ENDSELECT


@CARD_34034(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 300
	CASE "守備力"
		RETURN 400
ENDSELECT
RETURN 0


@CARD_EXPLANATION_34034(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,손패起動)
PRINTL ①: 자신 필드에 "＠이그니스터" 몬스터가 존재할 경우에 발동할 수 있다.
PRINTL 이 카드를 패에서 특수 소환한다.
CALL CARD_ABLE(2,필드起動)
PRINTL ②: 자신 메인 페이즈에 발동할 수 있다.
PRINTL 이 카드 이외의 자신 필드의 사이버스족 몬스터 1장을 릴리스하고,
PRINTL 덱에서 레벨 5 이상의 사이버스족 몬스터 1장을 패에 추가하고, 이 카드의 레벨을 턴 종료시까지 4로 한다.
PRINTL 이 효과로 의식 몬스터를 패에 추가했을 경우, 또한 덱에서 의식 마법 1장을 패에 추가할 수 있다.


@CARDCAN_34034(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,34034)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,34034)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,34034,"손패")
	SIF RESULT == 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = ＠이그니스터
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,34034)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 사이버스족
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = モン全般
	효과条件종족 = 사이버스족
	CALLFORM 효과条件レベル_ADV(5,"以上")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_34034(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID


IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,34034,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,"손패",場所)
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,34034,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종족 = 사이버스족
		효과条件他所 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"一星",4,1)
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종류 = モン全般
			효과条件종족 = 사이버스족
			CALLFORM 효과条件レベル_ADV(5,"以上")
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
			選択カード_ID = RESULT:1
			IF 選択カード_ID > 0
				TRYCALLFORM CARD_{選択カード_ID},"종류"
				IF RESULT == 의식
					CALL FULLRESET
					효과範囲덱 = 1
					효과条件종류 = 마법全般
					효과条件カテゴリ = 의식 마법
					CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF

