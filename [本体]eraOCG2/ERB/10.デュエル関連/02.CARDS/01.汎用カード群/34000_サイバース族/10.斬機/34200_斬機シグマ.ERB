@CARDNAME_34200,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 참기 시그마
	CASE "略称"
RESULTS = 참기 시그마
	CASE "カテゴリ"
RESULTS:0 = 참기
ENDSELECT


@CARD_34200(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 1500
	CASE "튜너"
		RETURN 2
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_34200(종류)
#DIM DYNAMIC 종류


PRINTL 이 카드명의 ①의 방법에 의한 특수 소환은 1턴에 1번밖에 할 수 없다.
CALLFORM CARD_ABLE(1,손패起動,묘지起動)
PRINTL ①: 이 카드가 손패 / 묘지에 존재하고, 자신 필드에 링크 몬스터가 존재하지 않을 경우에 발동할 수 있다.
PRINTL 이 카드를 특수 소환한다.
PRINTL 이 효과로 특수 소환한 이 카드는, 필드에서 벗어났을 경우에 제외된다.
PRINTL 이 효과의 발동 후, 턴 종료시까지 자신은 사이버스족 몬스터밖에 EX 덱에서 특수 소환할 수 없다.
PRINTL ②: 자신 필드의 이 카드를 싱크로 소재로 할 경우,
PRINTL 이 카드를 튜너 이외의 몬스터로서 취급할 수 있다.

@CARDCAN_34200(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,34200)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動 || 종류 == 묘지起動 
	SIF 발동済_1(듀얼리스트,34200)
		RETURN 0

	CALL CARD_SS_CHECK(듀얼리스트,34200,ゾーン)
	SIF RESULT == 1
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 링크
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	
	RETURN 1
ENDIF

@CARDEFFECT_34200(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM 손패選択
#DIM 묘지送カード
#DIM 묘지送カード_ID
#DIM 소환カード
#DIM 소환カード_ID


IF 종류 == 손패起動 || 종류 == 묘지起動 
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,34200,1)
	IF RESULT == 0
		CALL FULLRESET
		효과추가종류 = 2
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)
		EX소환可能종족 = 사이버스족
		CALL CARD_EX_RESTRICTION(듀얼리스트)
	ENDIF
ENDIF