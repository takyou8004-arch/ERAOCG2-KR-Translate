@CARDNAME_51650,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 플뢰르 드 바론
	CASE "略称"
RESULTS = 바론
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_51650(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 싱크로
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 3000
	CASE "守備力"
		RETURN 2400
	CASE "성별"
		RETURN 牝性

ENDSELECT

RETURN 0

@CARD_EXPLANATION_51650(종류)
#DIM DYNAMIC 종류


PRINTL 튜너 ＋ 튜너 이외의 몬스터 1장 이상
PRINTL 이 카드명의 ①③의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ①: 상대 필드의 카드 1장을 대상으로 하고 발동할 수 있다.
PRINTL 그 카드를 파괴한다.
CALLFORM CARD_ABLE(11,발동誘発)
PRINTL ②: 이 카드가 필드에 앞면 표시로 존재하는 한 1번만, 마법 / 함정 / 몬스터의 효과를 상대가 발동했을 때 발동할 수 있다.
PRINTL 그 발동을 무효로 하고 파괴한다.
CALLFORM CARD_ABLE(3,ターン開始時)
PRINTL ③: 자신 / 상대 턴 개시 시에, 자신 묘지의 레벨 9 이하의 몬스터 1장을 대상으로 하고 발동할 수 있다.
PRINTL 이 카드를 주인의 엑스트라 덱으로 되돌리고, 대상 몬스터를 특수 소환한다.


@CARDSUMMON_51650(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 10
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF
IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,51650) 
	RETURN RESULT
ENDIF


@CARDCAN_51650(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,51650)
SIF RESULT == 1
	RETURN 0
IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,51650)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン開始時
	SIF 발동済_3(듀얼리스트,51650)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	CALLFORM 효과条件レベル_ADV(9, "以下")
	효과条件소환可 = 2
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_51650(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,51650,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"묘지")
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,51650,11)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
		RETURN 1
	ENDIF
	RETURN 0
ELSEIF 종류 == ターン開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,51650,3)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,"필드",場所,2)
		CALL FULLRESET
		효과範囲묘지 = 1
		CALL 효과条件レベル_ADV(9, "以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF

