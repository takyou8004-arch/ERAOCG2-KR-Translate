@CARDNAME_92340,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 환영기사단 셰이드 브리간딘
	CASE "略称"
RESULTS = 브리간딘
	CASE "カテゴリ"
RESULTS:0 = 팬텀
ENDSELECT


@CARD_92340(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 300
	CASE "分割"
		RETURN 효과なし
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92340(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 1턴에 1번밖에 사용할 수 없다.
PRINTL 자신 묘지에 함정 카드가 존재하지 않는 경우, 이 카드는 세트한 턴에도 발동할 수 있다.
CALLFORM CARD_ABLE(0,마함起動,フリー誘発)
PRINTL ①: 이 카드는 발동 후, 몬스터 존에 수비 표시로 특수 소환한다.


@CARDCAN_92340(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,92340)
SIF RESULT == 1
	RETURN 0


IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_1(듀얼리스트,92340)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,92340,"마함")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = 함정全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3 && RESULT == 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_92340(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード

IF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,92340,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,"마함",場所,"守備표시")
	ENDIF
ENDIF

