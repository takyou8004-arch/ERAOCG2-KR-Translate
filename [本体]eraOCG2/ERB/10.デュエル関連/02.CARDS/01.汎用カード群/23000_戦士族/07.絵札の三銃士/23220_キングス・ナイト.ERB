@CARD_EXPLANATION_23220(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,소환時,특수소환時)
PRINTL ①: 자신 필드에 '퀸즈 나이트'가 존재하고, 이 카드가 소환/특수 소환에 성공했을 때 발동할 수 있다.
PRINTL 덱에서 '잭스 나이트' 1장을 특수 소환한다.
PRINTL 

@CARDCAN_23220(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,23220)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_SS_CHECK(듀얼리스트,23221,"덱")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
효과条件名前 = 퀸즈 나이트
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_23220(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,23220)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
효과条件名前 = 잭스 나이트
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF

