@CARDNAME_25232,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 요선수 겸이태도
	CASE "略称"
		RESULTS = 겸이태도
	CASE "カテゴリ"
		RESULTS:0 = 요선수
ENDSELECT

@CARD_25232(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 200
	CASE "성별"
		RETURN 雄性
	CASE "추가日"
		RETURN 260000
ENDSELECT
RETURN 0

@CARD_EXPLANATION_25232(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,소환時)
PRINTL ①：이 카드가 소환된 경우에 발동할 수 있다。
PRINTL 패에서 "요선수 겸이태도" 이외의 "요선수" 몬스터 １장을 소환한다。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①：자신 필드에 "요선수" 몬스터가 ３장 이상 존재하는 한、자신의 "요선수" 몬스터는 직접 공격할 수 있다。
CALLFORM CARD_ABLE(0,ターン終了時強制)
PRINTL ③：이 카드를 소환한 턴의 엔드 페이즈에 발동한다。
PRINTL 이 카드를 원래 주인의 패로 되돌린다。

@CARDCAN_25232(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,25232)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = 요선수
	효과条件固名前 = 요선수 겸이태도
	효과条件종류 = モン全般
	효과条件소환可 = 1
	IF モンスター数(듀얼리스트) >= 5
		CALLFORM 효과条件レベル_ADV(5,"以上")
	ELSEIF モンスター数(듀얼리스트) < 1
		CALLFORM 효과条件レベル_ADV(4,"以下")
	ELSEIF モンスター数(듀얼리스트) < 2
		CALLFORM 효과条件レベル_ADV(6,"以下")
	ENDIF
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = 요선수
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 3
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時強制
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_T1召種:場所") != 召種通常
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_25232(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,25232,0)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件カテゴリ = 요선수
		효과条件固名前 = 요선수 겸이태도
		효과条件종류 = モン全般
		IF モンスター数(듀얼리스트) >= 5
			CALLFORM 효과条件レベル_ADV(5,"以上")
		ELSEIF モンスター数(듀얼리스트) < 1
			CALLFORM 효과条件レベル_ADV(4,"以下")
		ELSEIF モンスター数(듀얼리스트) < 2
			CALLFORM 효과条件レベル_ADV(6,"以下")
		ENDIF
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		選択カードID = RESULT:99
		IF 選択カードID != 9999
			CALL CARD_SELECT(듀얼리스트,選択カード)
			選択カード = RESULT
			CALL CARD_SUMMON_SHORI(듀얼리스트,選択カード,"攻撃표시",3)
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"直攻")
ELSEIF 종류 == ターン終了時強制
	CALL CARD_DISPOSE(듀얼리스트,"필드",場所,3)
ENDIF