#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 트윈 테일 캣 레이디
	CASE "略称"
RESULTS = T T C 레이디
	CASE "カテゴリ"
RESULTS:0 = 캣
ENDSELECT


@CARD_52520(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 1000
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

