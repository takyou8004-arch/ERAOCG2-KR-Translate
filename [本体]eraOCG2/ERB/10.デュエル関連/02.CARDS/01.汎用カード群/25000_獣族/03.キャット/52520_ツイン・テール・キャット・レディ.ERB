#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 3
素材条件レベル = 1

IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,52520) 
	RETURN RESULT
ENDIF



