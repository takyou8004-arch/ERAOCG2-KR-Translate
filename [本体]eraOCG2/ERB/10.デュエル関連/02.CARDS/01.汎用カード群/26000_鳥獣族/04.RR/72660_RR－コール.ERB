@CARDNAME_72660,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = RR－콜
	CASE "略称"
RESULTS = RR－콜
	CASE "カテゴリ"
RESULTS:0 = 레이드 랩터즈
ENDSELECT


@CARD_72660(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_72660(종류)
#DIM DYNAMIC 종류
PRINTL 이 카드명의 카드는 1턴에 1장밖에 발동할 수 없으며,
PRINTL 이 카드를 발동하는 턴, 자신은 'RR' 몬스터밖에 특수 소환할 수 없다.
CALLFORM CARD_ABLE(1,마함起動 ,손패起動)
PRINTL ①: 자신 필드의 'RR' 몬스터 1장을 대상으로 하고 발동할 수 있다.
PRINTL 그 동명 몬스터 1장을 패 / 덱에서 수비 표시로 특수 소환한다.

@CARDCAN_72660(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,72660)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,72660)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 레이드 랩터즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과条件固カテゴリ = 레이드 랩터즈
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"SS이력")
	SIF RESULT != 0
		RETURN 0

	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과条件カテゴリ = 레이드 랩터즈
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_72660(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID
#DIMS DYNAMIC 選択カード名前
IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72660,1)
	IF RESULT == 0
		;通常마법汎用処理
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件カテゴリ = 레이드 랩터즈
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"필드",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			選択カードID = RESULT:99
			TRYCALLFORM CARDNAME_{選択カードID}, "名前"
			選択カード名前 = %RESULTS%
			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲덱 = 1
효과条件名前 = %選択カード名前%
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"守備표시")
			CALL CARD_SS_RESTRICTION(듀얼리스트,"레이드 랩터즈")
		ENDIF
	ENDIF
ENDIF
