@CARDNAME_26202,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 펭귄 나이트메어
	CASE "略称"
RESULTS = 피지 나이트메어
	CASE "カテゴリ"
RESULTS:0 = 펭귄
ENDSELECT


@CARD_26202(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 900
	CASE "守備力"
		RETURN 1800
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26202(종류)
#DIM DYNAMIC 종류

CALLFORM CARD_ABLE(0,소환時 ,특수소환時)
PRINTL ①: 이 카드가 소환 / 특수 소환했을 경우, 상대 필드의 카드 1장을 대상으로 하고 발동한다.
PRINTL 그 상대 필드의 카드를 주인의 패로 되돌린다.
CALLFORM CARD_ABLE(0,攻守変動)
PRINTL ②: 이 카드가 몬스터 존에 존재하는 한, 자신 필드의 물 속성 몬스터의 공격력은 300 올린다.


@CARDCAN_26202(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,26202)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時
	;상대場に몬스터가 いないなら침묵
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_表:場所") == 3
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26202(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 除去種
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26202)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
효과条件名前 = 부정펭귄
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT == 1
			除去種 = 제외
		ELSE
			除去種 = 손패
		ENDIF
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,除去種)
	ENDIF
ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과条件속성 = 물속성
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,300)
ENDIF

