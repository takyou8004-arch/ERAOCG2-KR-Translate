@CARDNAME_41250,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 청렬의 수령사 에리어
	CASE "略称"
RESULTS = 청렬 에리어
	CASE "カテゴリ"
RESULTS:0 = 령사
ENDSELECT


@CARD_41250(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 1850
	CASE "守備力"
		RETURN 1500
	CASE "성별"
		RETURN 牝性
ENDSELECT
RETURN 0

@CARD_EXPLANATION_41250(종류)
#DIM DYNAMIC 종류

PRINTL 물속성 몬스터를 포함하는 몬스터 2체
PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ①: 상대의 묘지의 물속성 몬스터 1체를 대상으로 발동할 수 있다.
PRINTL 그 몬스터를 자신 필드에 특수 소환한다.
CALLFORM CARD_ABLE(2,破壊→묘지,戦闘→묘지)
PRINTL ②: 이 카드가 전투 또는 효과로 파괴되었을 경우에 발동할 수 있다.
PRINTL 덱에서 수비력 1500 이하의 물속성 몬스터 1체를 손패에 추가한다.


@CARDSUMMON_41250(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要링크数 = 2

IF EX参照 == 0 || EX参照 == 1
	素材条件속성 = 물속성
ELSE
	PRINT
ENDIF


IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,41250) 
	RETURN RESULT
ENDIF

@CARDCAN_41250(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,41250)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,41250)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲상대묘지 = 1
	효과条件속성 = 물속성
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0		
	RETURN 1
ELSEIF 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	SIF 발동済_2(듀얼리스트,41250)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件속성 = 물속성
	효과条件守備力 = 1500
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_41250(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正





IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,41250,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대묘지 = 1
		효과条件속성 = 물속성
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,41250,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件속성 = 물속성
		효과条件守備力 = 1500
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ENDIF


