@CARDNAME_73201,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 생바인 사당
	CASE "略称"
RESULTS = 성만의 사당
	CASE "カテゴリ"
RESULTS:0 = 생아발론
RESULTS:1 = 생바인
ENDSELECT


@CARD_73201(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 지속마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73201(종류)
#DIM DYNAMIC 종류

PRINTL 자신 필드에 「생아발론」 L 몬스터가 존재할 경우,
PRINTL 손패 1장을 묘지로 보내고 이 카드를 발동할 수 있다.
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①: 이 카드가 마법 & 함정 존에 존재하는 한, 자신은 식물족 몬스터밖에 EX 덱에서 특수 소환할 수 없다.
CALLFORM CARD_ABLE(10,마함起動)
PRINTL ②: 1턴에 1번, 발동할 수 있다. 자신 묘지에서 레벨 4 이하의 식물족 일반 몬스터 1장을 고르고 특수 소환한다.
CALLFORM CARD_ABLE(0,ターン終了時)
PRINTL ③: 자신/상대의 엔드 페이즈에, 마법 & 함정 존의 앞면 표시의 이 카드를 묘지로 보내고,
PRINTL 자신 묘지에서 지속 함정 카드 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 카드를 자신 마법 & 함정 존에 세트한다.


@CARDCAN_73201(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73201)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종족 = 식물족
	효과条件종류 = 通常モン
	CALL 효과条件レベル_ADV(4, "以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
		SIF 손패数(듀얼리스트) == 0
			RETURN 0
	ELSE
		SIF 손패数(듀얼리스트) <= 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 링크
	효과条件カテゴリ = 생아발론
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時
	SIF ゾーン != "마함"
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종류 = 지속함정
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73201(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 選択カード_ID
#DIMS DYNAMIC 選択カード名前

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73201,10)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종족 = 식물족
		효과条件속성 = 땅속성
		효과条件종류 = 通常モン
		CALL 효과条件レベル_ADV(4, "以下")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73201)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_TMP_SS_RST(듀얼리스트,"종족",식물족)
ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73201)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종류 = 지속함정
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"裏側표시")
	ENDIF
ENDIF
