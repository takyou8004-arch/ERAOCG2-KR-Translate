@CARDNAME_32404,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 프레데터 플랜츠 비브리스프
	CASE "略称"
RESULTS = 포식 비브리스프
	CASE "カテゴリ"
RESULTS:0 = 프레데터 플랜츠
ENDSELECT


@CARD_32404(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 식물족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 1900
ENDSELECT

RETURN 0

@CARD_EXPLANATION_32404(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,필드→묘지 ,손패→묘지 ,덱→묘지 ,破壊→묘지 ,戦闘→묘지)
PRINTL ①: 이 카드가 묘지로 보내졌을 경우에 발동할 수 있다.
PRINTL 덱에서 「프레데터 플랜츠 비브리스프」 이외의 「프레데터 플랜츠」 몬스터 1장을 손패에 더한다.
CALLFORM CARD_ABLE(2,묘지起動)
PRINTL ②: 필드의 몬스터에 악성 카운터가 놓여 있을 경우에 발동한다.
PRINTL 이 카드를 묘지에서 특수 소환한다.
PRINTL 이 효과로 특수 소환한 이 카드는, 필드에서 벗어났을 경우에 제외된다.
PRINTL 

@CARDCAN_32404(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,32404)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	SIF 발동済_1(듀얼리스트,32404)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 프레데터 플랜츠
	효과条件종류 = モン全般
	효과条件固名前 = 프레데터 플랜츠 비브리스프
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,32404)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,32404,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件悪Ｃ所持 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_32404(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,32404,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 프레데터 플랜츠
		효과条件종류 = モン全般
		효과条件固名前 = 프레데터 플랜츠 비브리스프
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,32404,2)
	IF RESULT == 0
		CALL FULLRESET
		효과추가종류 = 効追除処
		CALL CARD_SP_SUMMON(듀얼리스트, "묘지", 場所)
	ENDIF
ENDIF



