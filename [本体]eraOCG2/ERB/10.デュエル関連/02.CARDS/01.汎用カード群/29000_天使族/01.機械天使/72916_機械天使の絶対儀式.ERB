@CARDNAME_72916,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 기계천사의 절대의식
	CASE "略称"
		RESULTS = 기계천사절대의식
	CASE "カテゴリ"
		RESULTS:0 = 사이버 엔젤
		RESULTS:1 = 의식 마법
ENDSELECT


@CARD_72916(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"

		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_72916(종류)
#DIM DYNAMIC 종류

PRINTL "사이버 엔젤" 의식 몬스터의 강림에 필요.
CALL CARD_ABLE(0,마함起動 ,손패起動)
PRINTL ①: 레벨의 합계가 의식 소환하는 몬스터와 같아지도록,
PRINTL 자신의 패 / 필드의 몬스터를 릴리스,
PRINTL 또는 릴리스 대신에 자신의 묘지에서 천사족 또는 전사족 몬스터를 덱으로 되돌리고,
PRINTL 패에서 "사이버 엔젤" 의식 몬스터 1장을 의식 소환한다.


@CARDCAN_72916(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성,5

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,72916)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	CALL RESETAREA_MAT
	素材範囲필드 = 1
	素材範囲손패 = 1
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件종류 = 의식
	효과条件カテゴリ = 사이버 엔젤
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		条件달성 = 1

	CALL FULLRESET
	효과範囲손패 = 1
	효과条件종류 = 의식
	효과条件カテゴリ = 사이버 엔젤
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		条件달성:1 ++
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종족:0 = 천사족
	효과条件종족:1 = 전사족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		条件달성:1 ++
	SIF 条件달성:1 == 2
		条件달성 = 1

	SIF 条件달성 == 0
		RETURN 0
	RETURN 1

ENDIF

@CARDEFFECT_72916(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC EX参照
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 素材数
#DIM DYNAMIC 選択済カード
#DIM DYNAMIC レベル合計



IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72916)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)		
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件종류 = 의식
		효과条件カテゴリ = 사이버 엔젤
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999

			選択カード -= 100
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件종족:0 = 천사족
			효과条件종족:1 = 전사족
			효과条件종류 = モン全般
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT == 1
				PRINTL 묘지의 특정 카드를 제외하시겠습니까?
				CALL YN_CHOICE(듀얼리스트)
			ELSE
				RESULT = 1
			ENDIF
			IF RESULT == 0
				CALL RESETAREA_MAT
				TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_손패:{選択カード}")}(듀얼리스트,選択カード)
				必要レベル数 = 1
				素材範囲묘지 = 1
				효과条件종족:0 = 천사족
				효과条件종족:1 = 전사족
				효과条件종류 = モン全般
				TRYCALLFORM RITSUMMON(듀얼리스트, 選択カード,"손패",덱行き)
			ELSE
				CALL RESETAREA_MAT
				TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_손패:{選択カード}")}(듀얼리스트,選択カード)
				素材範囲필드 = 1
				素材範囲손패 = 1
				TRYCALLFORM RITSUMMON(듀얼리스트, 選択カード,"손패")
			ENDIF
		ENDIF
	ENDIF
ENDIF


