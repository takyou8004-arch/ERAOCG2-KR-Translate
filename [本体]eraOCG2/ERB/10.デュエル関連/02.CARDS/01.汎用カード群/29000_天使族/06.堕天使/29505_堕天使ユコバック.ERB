@CARDNAME_29505,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 타락천사 유코백
	CASE "略称"
RESULTS = 유코백
	CASE "カテゴリ"
RESULTS:0 = 타락천사
ENDSELECT


@CARD_29505(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 700
	CASE "守備力"
		RETURN 1000
ENDSELECT

RETURN 0

@CARD_EXPLANATION_29505(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드 명의 효과는 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,소환時,특수소환時)
PRINTL ①: 이 카드가 소환/특수 소환에 성공했을 경우에 발동할 수 있다.
PRINTL 덱에서 '타락천사' 카드 1장을 묘지로 보낸다.
PRINTL 

@CARDCAN_29505(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,29505)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,29505)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 타락천사
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_29505(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所


IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29505,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 타락천사
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF
ENDIF
