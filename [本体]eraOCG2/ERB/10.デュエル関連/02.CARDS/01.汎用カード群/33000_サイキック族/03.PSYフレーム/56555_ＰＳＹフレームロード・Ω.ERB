ENDSELECT


@CARD_56555(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 사이킥족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 2800
	CASE "守備力"
		RETURN 2200
	CASE "성별"
		RETURN 雄性
	CASE "レア度"
		RETURN ULR
ENDSELECT

RETURN 0

@CARD_EXPLANATION_56555(종류)
#DIM DYNAMIC 종류

PRINTL 튜너 ＋ 튜너 이외의 몬스터 1장 이상
CALLFORM CARD_ABLE(10,필드起動,フリー誘発)
PRINTL ①: 1턴에 1번, 자신 상대의 메인 페이즈에 발동할 수 있다.
PRINTL 이 카드를 제외하고, 상대의 손패를 무작위로 1장 고르고 제외한다.
PRINTL 다음 자신 스탠바이 페이즈에, 자신은 제외 상태인 "PSY프레임로드 오메가"을 특수 소환하고,
PRINTL 상대는 자신의 제외 상태의 카드 1장을 고르고 패에 더한다.
CALLFORM CARD_ABLE(0,ターン開始時)
PRINTL ②: 상대 스탠바이 페이즈에,
PRINTL 자신 상대의 제외 상태의 카드 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 카드를 묘지로 되돌린다.
CALLFORM CARD_ABLE(0,묘지起動)
PRINTL ③: 이 카드가 묘지에 존재할 경우,
PRINTL 이 카드 이외의 자신 상대의 묘지의 카드 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 카드와 이 카드를 덱으로 되돌린다.


@CARDSUMMON_56555(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 8
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF

IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,56555) 
	RETURN RESULT
ENDIF


@CARDCAN_56555(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,56555)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	SIF 손패数(対面者) < 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == ターン開始時
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲제외 = 1
	효과範囲상대제외 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 묘지起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲상대묘지 = 1
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == ターン開始時予約
	RETURN 1

ELSEIF 종류 == ターン終了時予約
	RETURN 1
ENDIF

@CARDEFFECT_56555(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,56555,10)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,제외行き)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"손패破壊",場所,"제외")
		CALL CARD_RESERV(듀얼리스트,56555,スタン予約)
	ENDIF

ELSEIF 종류 == ターン開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,56555)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲제외 = 1
		효과範囲상대제외 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,56555)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲상대묘지 = 1
		효과条件他所 = 1
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
효과条件名前 = PSY프레임로드 오메가
			효과条件行先플래그 = 덱行き
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == ターン開始時予約
	IF ターンプレイヤー == 듀얼리스트
		CALL FULLRESET
		효과範囲제외 = 1
효과条件名前 = PSY프레임로드 오메가
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		
		CALL FULLRESET
		효과範囲제외 = 1
		CALL CARD_ADD_ADV(対面者,ゾーン,場所)
	ELSE
		CALL CARD_RESERV(듀얼리스트,56555)
	ENDIF

ELSEIF 종류 == ターン終了時予約
	CALL CARD_RESERV(듀얼리스트,56555,スタン予約)
ENDIF



