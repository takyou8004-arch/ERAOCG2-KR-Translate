		RETURN 사이킥족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 1000
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250502
ENDSELECT

RETURN 0

@CARD_EXPLANATION_2313(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ①: 자신 메인 페이즈에 발동할 수 있다.
PRINTL 자신의 덱/묘지에서 「스타 리스타트」 또는 「시작의 별」 1장을 손패에 더한다.
PRINTL 그 후, 덱에서 레벨 5 이하의 일반 몬스터 1장을 묘지로 보낼 수 있다.
CALLFORM CARD_ABLE(2,F素材時효과)
PRINTL ②: 이 카드가 융합 소재가 되었을 경우에 발동할 수 있다.
PRINTL 자신은 1장 드로우한다.


@CARDCAN_2313(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,2313)
SIF RESULT == 1
	RETURN 0


IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,2313)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲묘지 = 1
	효과条件名前:0 = 스타 리스타트
	효과条件名前:1 = 시작의 별
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == F素材時효과
	SIF 발동済_2(듀얼리스트,2313)
		RETURN 0
	CALL CARD_DRAW_CHECK
	RETURN 1
ENDIF

@CARDEFFECT_2313(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,2313,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件名前:0 = 스타 리스타트
		효과条件名前:1 = 시작의 별
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종류 = 通常モン
			CALLFORM 효과条件レベル_ADV(5,"以下")
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == F素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,2313,2)
	IF RESULT == 0
		CALL CARDDRAW_BASIC(듀얼리스트,1)
	ENDIF
ENDIF


