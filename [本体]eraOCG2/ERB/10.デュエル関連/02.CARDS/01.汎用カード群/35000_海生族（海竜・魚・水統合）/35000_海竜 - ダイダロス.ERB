@CARDCAN_35000(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,35000)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVARS(@"%듀얼리스트%_フィールド名") != "바다"
		RETURN 0
	IF 듀얼리스트 != "자신"
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과範囲상대마함 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ENDIF

	CALL FULLRESET
	효과範囲마함 = 1
	효과条件종류 = フィールド
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	
	RETURN 1
ENDIF

@CARDEFFECT_35000(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所



IF 종류 == 필드起動 
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,35000)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲마함 = 1
		효과条件종류 = フィールド
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과条件他所 = 1
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"両全場全体",場所,"묘지")
		ENDIF
	ENDIF
ENDIF

