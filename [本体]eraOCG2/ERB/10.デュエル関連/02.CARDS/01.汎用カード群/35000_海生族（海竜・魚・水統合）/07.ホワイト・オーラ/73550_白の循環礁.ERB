@CARDNAME_73550,参照先
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 화이트 서클 리프
	CASE "略称"
RESULTS = 백의 순환초
	CASE "カテゴリ"
RESULTS:0 = 화이트 아우라
ENDSELECT


@CARD_73550(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73550(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 1턴에 한 번밖에 사용할 수 없다.
CALL CARD_ABLE(1,마함起動 ,손패起動)
PRINTL ①: 자신 필드의 바다 생물족 몬스터 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 몬스터를 파괴하고, 그 동명 몬스터 1장을 덱에서 패에 넣는다.
PRINTL 이 카드의 효과 처리 시에 자신 필드에 바다 생물족 싱크로 몬스터가 존재했을 경우,
PRINTL 패에 넣지 않고 특수 소환할 수도 있다.
CALL CARD_ABLE(2,묘지起動)
PRINTL ②: 묘지의 이 카드를 제외하고, 자신 묘지의 바다 생물족 몬스터 2장을 대상으로 하여 발동할 수 있다.
PRINTL 그 2장 중 1장을 덱의 맨 아래로 되돌리고, 나머지 1장을 특수 소환한다.


@CARDCAN_73550(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73550)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,73550)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 바다생물족
	효과条件종류:0 = 通常モン
	효과条件종류:1 = 효과モン
	효과条件종류:2 = 의식
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종족 = 바다생물족
	효과条件종류:0 = 通常モン
	효과条件종류:1 = 효과モン
	효과条件종류:2 = 의식
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,73550)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件종족 = 바다생물족
	효과条件소환可 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73550(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 選択カード_ID

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73550,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종족 = 바다생물족
		효과条件종류 = 싱크로
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件달성 = 1
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종족 = 바다생물족
		효과条件종류:0 = 通常モン
		효과条件종류:1 = 효과モン
		효과条件종류:2 = 의식
		효과条件行先플래그 = 破壊行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			選択カード_ID = RESULT:1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件ID = 選択カード_ID
			IF 条件달성 == 1
				CALL CARD_SS_CHECK(듀얼리스트,選択カード_ID,"덱")
				IF RESULT == 1
					RESULT = 1
				ELSE
					SIF 듀얼리스트 == "자신"
PRINTL 몬스터를 특수 소환하시겠습니까?
					CALL YN_CHOICE(듀얼리스트)
				ENDIF
			ELSE
				RESULT = 1
			ENDIF

			IF RESULT == 0
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			ELSEIF RESULT == 1
				CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73550,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종족 = 바다생물족
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件종족 = 바다생물족
			효과条件行先플래그 = 덱行き
			효과条件종류 = モン全般
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF
