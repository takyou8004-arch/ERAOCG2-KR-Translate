ENDSELECT


@CARD_57605(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 바다생물족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 3300
	CASE "守備力"
		RETURN 3000
	CASE "生成カード"
		RETURN 157605
ENDSELECT

RETURN 0

