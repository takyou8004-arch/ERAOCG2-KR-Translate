@CARDNAME_57545,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 심해희 프리마돈나
	CASE "略称"
RESULTS = 프리마돈나
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_57545(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 싱크로
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 바다생물족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 2700
	CASE "튜너"
		RETURN 1
ENDSELECT

RETURN 0

@CARD_EXPLANATION_57545(종류)
#DIM DYNAMIC 종류


PRINTL 튜너 + 튜너 이외의 몬스터 1장 이상
PRINTL 이 카드명의 ①③의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ①: 제외되어 있는 상대의 카드 1장을 대상으로 발동할 수 있다.
PRINTL 덱에서 레벨 4 이하의 물 속성 몬스터 1장을 패에 넣거나 특수 소환하고, 대상의 카드를 상대의 패에 넣는다.
CALLFORM CARD_ABLE(0,S素材時효과)
PRINTL ②: 이 카드를 싱크로 소재로 했을 경우, 자신 필드의 싱크로 몬스터 1장을 대상으로 발동할 수 있다.
PRINTL 그 몬스터는 대상 내성을 얻는다.
CALLFORM CARD_ABLE(3,필드→묘지 ,손패→묘지 ,덱→묘지 ,破壊→묘지 ,戦闘→묘지)
PRINTL ③: 이 카드가 묘지로 보내졌을 경우, 제외되어 있는 자신 또는 상대의 카드 1장을 대상으로 발동할 수 있다.
PRINTL 그 카드를 주인의 덱으로 되돌린다.


@CARDSUMMON_57545(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 6
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF
IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,57545) 
	RETURN RESULT
ENDIF


@CARDCAN_57545(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,57545)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,57545)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲상대제외 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件속성 = 물속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == S素材時효과
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 싱크로
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	SIF 발동済_3(듀얼리스트,57545)
		RETURN 0
	CALL FULLRESET
	효과範囲제외 = 1
	효과範囲상대제외 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ENDIF

@CARDEFFECT_57545(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57545,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대제외 = 1
		효과条件行先플래그 = 손패行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL YN_CHOICE(듀얼리스트,"특수 소환한다","패에 넣는다")
			IF RESULT == 0
				CALL FULLRESET
				효과範囲덱 = 1
				효과条件속성 = 물속성
				CALLFORM 효과条件レベル_ADV(4, "以下")
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			ELSEIF RESULT == 1
				CALL FULLRESET
				효과範囲덱 = 1
				효과条件속성 = 물속성
				CALLFORM 효과条件レベル_ADV(4, "以下")
				CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
			ENDIF

		ENDIF
	ENDIF

ELSEIF 종류 == S素材時효과
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57545)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件종류 = 싱크로
		CALL CARD_GRANT_ADV(듀얼리스트,ゾーン,場所,"対耐",2)
	ENDIF
ELSEIF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57545,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲제외 = 1
		효과範囲상대제외 = 1
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF



