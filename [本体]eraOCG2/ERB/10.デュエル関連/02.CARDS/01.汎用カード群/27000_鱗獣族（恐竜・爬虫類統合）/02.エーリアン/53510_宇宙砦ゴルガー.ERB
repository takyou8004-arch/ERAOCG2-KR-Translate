@CARDNAME_53510,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
RESULTS = 우주 요새 고르가
	CASE "略称"
RESULTS = 하늘 요새
	CASE "カテゴリ"
RESULTS:0 = 없음
ENDSELECT


@CARD_53510(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 비늘야수족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 2600
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 雄性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_53510(종류)
#DIM DYNAMIC 종류

PRINTL 「에일리언 모나이트」＋튜너 이외의 「에일리언」 몬스터 1체 이상
PRINTL 이 카드명의 ②의 효과는 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(10,필드起動)
PRINTL ①: 1턴에 1번, 필드의 앞면 표시 마법 / 함정 카드를 임의의 수만큼 대상으로 하여 발동할 수 있다.
PRINTL 그 앞면 표시 카드를 주인의 패로 되돌린다.
PRINTL 그 후, 패로 되돌아간 수만큼 필드의 앞면 표시 몬스터에 악성 카운터를 놓는다.
CALL CARD_ABLE(1,필드起動,フリー誘発)
PRINTL ②: 서로의 턴에, 필드의 악성 카운터를 2개 제거하고,
PRINTL 상대 필드의 카드 1장을 대상으로 하여 발동할 수 있다.
PRINTL 그 상대의 카드를 파괴한다.


@CARDSUMMON_53510(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 5
IF EX参照 == 0 || EX参照 == 1
素材条件名前 = 에일리언 모나이트
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
	素材条件カテゴリ = 에이리언
ENDIF

IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,53510) 
	RETURN RESULT
ENDIF


@CARDCAN_53510(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 발동불가,2

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,53510)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	IF 종류 == フリー誘発
		SIF 발동済_1(듀얼리스트,53510)
			RETURN 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과範囲상대필드 = 1
		효과範囲상대마함 = 1
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT:10 < 2
			RETURN 0
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과範囲상대마함 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ENDIF
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		발동불가:0 ++
	SIF ターンプレイヤー != 듀얼리스트
		발동불가:0 ++
	SIF メインフェイズ != 1
		발동불가:0 ++
	CALL FULLRESET
	효과範囲마함 = 1
	효과範囲상대마함 = 1
	효과条件표시 = 10
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		발동불가:0 ++
	
	SIF 발동済_1(듀얼리스트,53510)
		발동불가:1 ++
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT:10 < 2
		발동불가:1 ++
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		발동불가:1 ++
	
	SIF 발동불가:0 > 0 && 발동불가:1 > 0
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_53510(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성,2
#DIM DYNAMIC 選択효과
#DIM DYNAMIC パワー充填

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動 || 종류 == フリー誘発
	IF 종류 == フリー誘発
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,53510,1)
		SIF RESULT == 0
			選択효과 = 2

	ELSEIF 종류 == 필드起動
		SIF ターンプレイヤー == 듀얼리스트
			条件달성:0 ++
		SIF メインフェイズ == 1
			条件달성:0 ++
		CALL FULLRESET
		효과範囲마함 = 1
		효과範囲상대마함 = 1
		효과条件표시 = 10
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件달성:0 ++
		
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과範囲상대필드 = 1
		효과範囲상대마함 = 1
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT:10 >= 2
			条件달성:1 ++
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과範囲상대마함 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT == 1
			条件달성:1 ++
		
		
		SIF 효과발동済(듀얼리스트,場所,ゾーン)
			条件달성:0 = 0
		SIF 발동済_1(듀얼리스트,53510)
			条件달성:1 = 0
		PRINTFORML {条件달성:0}
		PRINTFORMW {条件달성:1}
		IF 条件달성:0 == 3 && 条件달성:1 == 2
			IF 듀얼리스트 == "자신"
				CALL YN_CHOICE(듀얼리스트,"①：필드의 表側の마법함정を戻し、その数分悪性카운터を置く","②：필드의 悪性카운터を２つ取り除き、상대의 카드 １장을 파괴한다")
			ELSE
				RESULT = 1
			ENDIF
		ELSEIF 条件달성:0 == 3
			RESULT = 0
		ELSEIF 条件달성:1 == 2
			RESULT = 1
		ENDIF

		IF RESULT == 0
			CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,53510,10)
			SIF RESULT == 0
				選択효과 = 1
		ELSEIF RESULT == 1
			CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,53510,1)
			SIF RESULT == 0
				選択효과 = 2
		ENDIF
	ENDIF

	IF 選択효과 == 1
		REPEAT 10
			CALL FULLRESET
			효과条件표시 = 表側표시
			효과条件任意 = 1
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"両마함単体",場所,"손패")
			SIF RESULT != 1
				BREAK
			パワー充填 ++
		REND
		IF パワー充填 > 0
			PRINTL
			SETCOLOR C_L_BLUE
			PRINTFORMW 에이리언サイボウ　{パワー充填}ハツ　ジュンビカンリョウ
			RESETCOLOR
			REPEAT パワー充填
				CALL FULLRESET
				효과範囲필드 = 1
				효과範囲상대필드 = 1
				효과条件표시 = 表側표시
				CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
				IF RESULT != 9999
					CALL CARD_SELECT(듀얼리스트,RESULT)
					選択カード = RESULT
					CALL SET_FIELD_INFO(대상者（CS）,"필드",選択カード,"悪Ｃ",1) 
				ENDIF
			REND
		ENDIF

	ELSEIF 選択효과 == 2
		REPEAT 2
			CALL FULLRESET
			효과範囲필드 = 1
			효과範囲마함 = 1
			효과範囲상대필드 = 1
			효과範囲상대마함 = 1
			효과条件悪Ｃ所持 = 999
			CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT != 9999
				CALL CARD_SELECT(듀얼리스트,RESULT)
				CALL SET_FIELD_INFO(대상者（CS）,参照先（CS）,選択カード（CS）,"悪Ｃ",-1)
				パワー充填 ++
			ENDIF
		REND
		IF パワー充填 == 2
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"全場単体",場所,"묘지")
		ENDIF
	ENDIF
ENDIF



