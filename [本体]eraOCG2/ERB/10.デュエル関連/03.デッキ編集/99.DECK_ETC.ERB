	FOR LCOUNT, 135, 150
		SIF DECK_INT:(LCOUNT - 135) == -1
			CONTINUE
		DECK_STR:LCOUNT = ITEM:%TOSTR(DECK_INT:(LCOUNT - 135))% += 1
	NEXT

