		TRYCALLFORM CARD_{カウンタ}, "속성"
		FOR カウンタB,1,7
			SIF RESULT == カウンタB
				EX속성数:カウンタB ++
		NEXT
	NEXT



	FOR カウンタB,1,7
		SETCOLOR (0x000000 + カウンタB*1000000 + カウンタB*100000 + カウンタB*10000 + カウンタB*1000 + カウンタB*100 + カウンタB*10 + カウンタB*1) 
