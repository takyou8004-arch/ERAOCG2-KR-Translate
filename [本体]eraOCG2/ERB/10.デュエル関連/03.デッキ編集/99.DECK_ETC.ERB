		SPLIT LOADTEXT(RESULTS:読込덱), UNICODE(0xA), DECK_STR
		FOR LCOUNT, 0, 60
			SETVAR @"編集_덱:{LCOUNT}", TOINT(DECK_STR:LCOUNT)
		NEXT

