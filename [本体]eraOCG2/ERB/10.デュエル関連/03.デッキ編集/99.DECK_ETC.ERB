		PRINTFORML [{LCOUNT}] - %SUBSTRING(RESULTS:LCOUNT, 5)%
	NEXT
	PRINTL
	PRINTL [1000] - 돌아간다
	BINPUT

	SIF RESULT == 1000
		RESTART
	読込덱 = RESULT
	PRINTL
